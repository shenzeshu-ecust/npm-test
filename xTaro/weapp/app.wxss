.page {
  display: flex;
  flex-direction: column;
  background-color:#efefef;
  height: 100%;
}

.link_txt {
  color: #099FDE;
}
/* iconfont */
@font-face {
  font-family: 'ctrip_icon';
  src: url(data:application/octet-stream;base64,%3D) format('truetype');
}
icon {
  display: inline-block;
  vertical-align: middle;
}
icon:before {
  font-size: 36rpx;
}
/* 小红点 */
.ico-point_red {
  width: 8px;
  height: 8px;
  overflow: hidden;
  border-radius: 50%;
  background-color: #F43530;
}
.ico-point_text {
  float: left;
  min-width: 18px;
  height: 18px;
  white-space: nowrap;
  color: #fff;
  background-color: #F43530;
  font-size: 13px;
  text-align: center;
  line-height: 1.5;
  border-radius: 9px;
  box-sizing: border-box;
  padding:0 6px;
}
/* 头像 icon */
.ico-header:before {
  font-family: 'ctrip_icon';
  content: "\EC01";
}
