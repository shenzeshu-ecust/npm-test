/* 文字样式 */
page {
  font-size: 32rpx;
  color: #333;
  background-color: #EFEFEF;
  font-family: -apple-system-font,Helvetica Neue,Helvetica,sans-serif;
}

/* 颜色 */
.color-interact {
  color: #099FDE !important;
}
.color-money {
  color: #FF7D13 !important;
}
.color-warn {
  color: #F43530 !important;
}
.color-gray_333 {
  color: #333 !important;
}
.color-gray_666 {
  color: #666 !important;
}
.color-gray_999 {
  color: #999 !important;
}
.color-gray_bbb {
