/**app.wxss**/
.view-check {
  pointer-events: none;
  width: 0;
  height: 1px;
  position: fixed;
  animation: __check_anim 200ms forwards;
}

@keyframes __check_anim {
  from   {
      width: 0;
  }
  to {
      width: 20px;
  }
}

/* 原先放在 /cwx/component/backhome/backhome.wxss ，由于 backhome 目前无人使用，已删除，直接将backhome的样式内容拷贝到 app.wxss 内了 */
.ico-logo-w{
  display: inline-block;
  background: url(//pic.c-ctrip.com/platform/h5/public/logo-ctrip-w.png) no-repeat center center;
  width: 134rpx;
  height: 49rpx;
  background-size: 134rpx 49rpx;
  vertical-align: middle;
}
.icon-arrow-r{
  display: inline-block;
  width: 14rpx;
  height: 14rpx;
  border:4rpx solid #fff;
  border-color:transparent #fff #fff transparent;
  -webkit-transform: rotate(-45deg) translateY(-2rpx);
