/*tab置顶*/
.tkt_tab_list {
    position: relative;
}
.tkt_tab {
    width: 100%;
    position: absolute;
    top: 0;
    z-index: 2;
    background-color: #FE483F;
}
.tkt_tab.fixed {
    position: -webkit-sticky;
    position: sticky;
}
.tab_holder {
    padding-top: 85px;
}
.fixed + .tab_holder {
    display: none;
}
.tab_content {
    padding-bottom: 160rpx;
    padding-bottom: calc(160rpx + env(safe-area-inset-bottom));
    padding-bottom: calc(160rpx + constant(safe-area-inset-bottom));
}
