.arrow_right {
    margin-left: 2px;
    position: relative;
    display: inline-block;
}
.arrow_right:before,
.arrow_right:after {
    content: "";
    display: block;
    position: absolute;
    top: 1px;
    width: 0;
    height: 0;
    border: 4px solid;
}
.arrow_right:before {
    left: 1px;
    border-color: transparent transparent transparent #0086F6;
}
.arrow_right:after {
    left: 0;
    border-color: transparent transparent transparent #fff;
}
/*arrow*/
.ttd-icon-arrow {
    display: inline-block;
    width: 6px;
    height: 6px;
    border-width: 2px 2px 0 0;
    border-style: solid;
    border-color: #099fde;
}
.ttd-icon-arrow-r {
	transform: rotate(45deg);
}

.ttd-icon-arrow-up {
    transform: rotate(-45deg);
}

.ttd-icon-arrow-down {
    transform: rotate(135deg);
}
.border_before {
    position: relative;
}
.border_before:before {
    content: '';
    display: inline-block;
    width: 200%;
    height: 200%;
    transform-origin: 0 0;
    box-sizing: border-box;
    position: absolute;
    left: 100%;
    top: 100%;
    transform: translate(-50%,-50%) scale(.5,.5);
    pointer-events: none;
}
.ttd-card{
    border-radius: 5px;
    margin-right: 6px;
    margin-bottom: 10px;
    margin-left: 6px;
    background-color: #fff;
    overflow: hidden;
}
/* flex layout */
.ttd-flex{
    display: -webkit-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
}
 .ttd-flex-item,.ttd-col-auto {
     -webkit-box-flex: 1;
     -webkit-flex: 1;
     flex: 1;
     -ms-flex: 1;
}
/*列表插件*/
/*两种列表：列表单位边框距离左边0px ,添加class ttd-list;边框距离左边15px，父层添加class ttd-list-gap;*/
.ttd-list-gap{
    padding-left: 15px;
}
.ttd-list-item {
    display: flex;
    padding-top: 12px;
    padding-right: 15px;
    padding-bottom: 12px;
    word-break: break-all;
}
.ttd-list .ttd-list-item {
    padding-left: 15px;
}
.ttd-list-gap .ttd-list-item {
    padding-left: 0;
}
.ttd-list-text {
    width: 88px;
    line-height: 16px;
    font-size: 14px;
    /*默认垂直居中*/
    display: flex;
    margin:auto;
}
.ttd-list-box{
    flex: 1;
    font-size: 14px;
    overflow: hidden;
    /*默认居中*/
    margin: auto;
}
.ttd-icon-list {
    font-size: 13px;
    padding-left: 5px;
    /*默认垂直居中*/
    display: flex;
    margin:auto;
}
/*列表头部标题*/
.ttd-tit-special {
    padding-left: 15px;
    background-color: #eff0f2;
}
.ttd-tit-special .ttd-list-box {
    font-size: 17px;
    color: #000;
}
/*列表插件结束*/
