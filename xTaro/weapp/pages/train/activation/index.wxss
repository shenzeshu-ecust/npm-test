.share-box .pyqs{background:url(https://images3.c-ctrip.com/train/hd/20200305-ztrip-miandan/pop/pyq.png) no-repeat;background-size:100% 100%;}
.share-box .wx::after{ border: none; }
.share-box .pyqs::after{ border: none; }

.share-image {position: fixed;top: 0;left: 0;z-index: 1000;display: flex;justify-content: center;  align-items: center;width: 100vw;height: 100vh;}


