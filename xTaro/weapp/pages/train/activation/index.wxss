.share-box .title text{position:relative;}
.share-box .title text::after,.share-box .title text::before{position:absolute;width:24rpx;height:4rpx;background:url(https://images3.c-ctrip.com/train/hd/20200305-ztrip-miandan/pop/Line.png) no-repeat;background-size:100% 100%;content:"";top:25rpx;}
.share-box .title text::after{left:-38rpx;}
.share-box .title text::before{right:-38rpx;}
.share-box .share-btn{display:flex;justify-content:center;padding-top:20rpx;}
.share-box .wx,.share-box .pyqs{width:128rpx;height:158rpx;margin:0 30rpx;}
