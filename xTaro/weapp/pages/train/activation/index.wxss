.qrcode-container {
  background: url('https://images3.c-ctrip.com/train/2022/app/8.56/zengzhang/zhuli/kap.png') no-repeat;
  /* height: 363rpx; */
  height: 750rpx;
  width: 100%;
  background-size: contain;
  display: flex;
  flex-direction: column;
  align-items: center;
  position: relative;
}

.qrcode-container .qrcode {
  width: 343rpx;
  height: 343rpx;
  border-radius: 8px;
  margin-top: 50rpx;
}
.qrcode-container .title {
  margin-top: 54rpx;
color: #fff9e8;
font-size: 34rpx;
font-family: PingFangSC-Semibold;
font-weight: 600;
background: linear-gradient(-90deg, rgb(237, 211, 181) 0%, rgb(227, 176, 127) 100%);
border-radius: 21px;
padding: 10rpx 48rpx;
/* box-shadow: 0px 2px 8px 0px rgba(122, 73, 17, 0.34); */
}
.qrcode-container .subtitle {
color: #ffffff;
font-size: 26rpx;
margin-top: 8rpx;
font-family: PingFangSC-Medium;
font-weight: 500;
}
.qrcode-container .tips {
  margin-top: 48rpx;
color: #fff9e8;
font-size: 30rpx;
font-family: PingFangSC-Semibold;
font-weight: 600;
}
.qrcode-container .hand-illustration {
  position: absolute;
  width: 188rpx;
  right: 83rpx;
  bottom: 150rpx;
  animation: handAni 0.96s ease-in-out infinite;
}

@keyframes handAni {
  0% {
    transform: translate3d(0, 0, 0);
  }
  50% {
    transform: translate3d(-0.3rem, -0.3rem, 0);
  }
  100% {
    transform: translate3d(0, 0, 0);
  }
}

.card-box.without-side-margin {
  margin-left: 0;
