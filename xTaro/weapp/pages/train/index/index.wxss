.calendar {
    border-radius: 24rpx;
    margin: 16rpx;
    background: #fff;
    box-sizing: border-box;
    position: relative;
}

.top-tab-bar {
    height: 80rpx;
    width: calc(100% - 30rpx);
    display: flex;
    justify-content: space-around;
    position: absolute;
    top: 270rpx;
    left: 16rpx;
    border-radius: 24rpx 24rpx 0 0;
    background: #ffffff;
    /* box-shadow: 0px 6px 16px 0px rgba(0, 0, 0, 0.04); */
    background: rgba(255, 255, 255, .7);
    z-index: 6;
}

.top-tab-bar .bar-item {
    flex: 1;
    text-align: center;
    font-size: 34rpx;
    line-height: 80rpx;
    color: #111111;
    font-family: PingFangSC-Regular;
    font-weight: normal; 
    letter-spacing: -0.82px;
    z-index: 1;
}
.top-tab-bar .bg {
    width: 50%;
    height: 88rpx;
    background: #fff;
    position: absolute;
    top: -8rpx;
    left: 0;
    
    transition: all 0.3s linear;
    border-radius: 16rpx 16rpx 1rpx 0rpx ;
    transform: translateX(0rpx);
}
.top-tab-bar .bg::after {
    content: "";
    width: 25rpx;
    height: 88rpx;
    position: absolute;
    top: 0rpx;
    right: -25rpx;
    background: linear-gradient(70deg, #fff 50%, transparent 0%);
    transition: all 0.3s linear;
}
.top-tab-bar .bg-right::after {
    right: 0;
    left: -25rpx;
    background: linear-gradient(-70deg, #fff 50%, transparent 0%);
}
.top-tab-bar .bg-right {
    transform: translateX(100%);
}

.top-tab-bar .active {
    color: #006FF6;
}

.index-box.oversea-box {
    transform: translateX(100%);
    transition: all .3s linear;
    display: none;
}
.index-box.trans-left {
    transform: translateX(0);
    display: block;
}


.main-swiper {
    width: 100%;
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    overflow-x: hidden;
    /* margin: -70rpx 0rpx; */
}

.main-swiper.main-swiper-left{
    height: 100%;
    overflow: hidden;
}

.main-swiper .main-swiper-item {
    width: 100%;
    flex-shrink: 0;
    left: 0;
    transition: all .3s linear;
    transform: translate3d(0,0,0);
    overflow-x: hidden;
    overflow-y: scroll;
    padding-bottom: calc(env(safe-area-inset-bottom) + 150rpx);
    box-sizing: border-box;
}
.main-swiper .main-swiper-item:nth-of-type(2) {
    height: 100%;
}
.main-swiper-item.swiper-left {
    /* transform 导致子元素fixed失效 */
    transform: translate3d(-100%, 0, 0);
}

