page{background: #eef1f6;color: #333;width: 100%; height:100%}

.cont{flex: 1;}
button::after{border:0}

.index-top{height: 348rpx;position: relative}
.index-top::after{position: absolute;content: "";height: 120rpx;left: 0;right: 0;bottom: -1px;z-index: 5;background: -webkit-linear-gradient(top,rgba(242,242,242,0),#f2f2f2 80%);}
.top-bar{position: fixed;z-index: 10;height: 88rpx;padding-top:40rpx;left: 0;top:0;right: 0;pointer-events: none;}
.top-bar.iphone,.index-top.iphone{padding-top: 88rpx!important;}
.top-bar::before{position: absolute;content: "";background: -webkit-linear-gradient(top,rgba(0,0,0,.25),rgba(0,0,0,0));top:0;left: 0;right:0;bottom: 0;}
.top-bar .icon-back{width: 44rpx;height: 44rpx;position: relative;z-index: 2;pointer-events: auto;background: url(https://images3.c-ctrip.com/train/2023-3/xiaochengxu/7.19/shouye/img-back.png) no-repeat;background-size: 100% 100%;margin-left: 24rpx;transform: translateY(50%);}
.top-bar .ifont-home{width: 56rpx;height: 56rpx;position: absolute;left: 16rpx;bottom: 16rpx;border-radius: 46rpx;background: rgba(0,0,0,.25);font-size: 30rpx;line-height: 56rpx;text-align: center;color: #fff;pointer-events: auto;}
.swiper-box{position: absolute;left: 0;bottom: 0;right: 0;width:100%;height: 348rpx;z-index: 2;background: url(https://images3.c-ctrip.com/train/2021/xiaochengxu/index_banner_bg/375-174@2x.png)no-repeat; background-size: contain;}
.swiper-box .mask{position: absolute;top: 0;left: 0;width: 100%;height: 100%;background: linear-gradient(180deg, rgba(0,0,0,0.4), rgba(0,0,0,0));pointer-events: none;z-index: 1;}
.slide-image{width: 100%;height: 348rpx;}
.slide-dot{position: absolute;bottom: 80rpx;z-index: 10;text-align: center;left: 40rpx;right: 40rpx;}
.slide-dot .item{width: 10rpx;height: 10rpx;display: inline-block;border-radius: 10rpx;background: rgba(255,255,255,.44);margin-right: 8rpx;}
.slide-dot .item:last-child{margin-right: 0;}
.slide-dot .item.cur{width: 28rpx;background: #fff;}
.tips-collect{line-height: 60rpx;position: absolute;z-index: 5;background: rgba(0,0,0,.7);color: #fff;font-size: 24rpx;font-weight: 600;padding: 0 28rpx;border-radius: 60px;right: 20rpx;}
.tips-collect::before{position: absolute;content: "";height:0px;width:0px;line-height:0px;border-style:dashed dashed solid dashed;border-width:9px;border-color: transparent transparent rgba(0,0,0,.7) transparent;top:-18px;right: 100rpx;}
.tips-collect .ifont-menu{font-size: 34rpx;margin: -2px 12rpx 0;}

.index-box{position: relative;z-index: 10;margin: -1rpx 16rpx 0;border-radius:0 0 24rpx 24rpx;background: #fff;}
.index-box.noAbord{margin:-76rpx 16rpx 0; border-radius:24rpx}
.notice {padding: 24rpx 28rpx 24rpx 32rpx;background: #FFF8F2;color: #FF6602;display:flex;align-items: center;border-radius: 10px 10px 0 0;}
.notice-international {
  background: #fffaf5 !important;
  color: #FF7700 !important;
}
.notice .tag{line-height: 34rpx;font-size: 22rpx;color: #fff;font-weight: 600;background: -webkit-linear-gradient(top left,#FF9130,#FE7700);padding: 0 8rpx;border-radius: 2px;margin-right: 16rpx;}
.notice .cont{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;font-size: 26rpx;}
.notice .ifont-arr{width: 24rpx;height: 24rpx;font-size: 18rpx;text-align: center;line-height: 24rpx;margin-left: 4rpx;}

.sel-box{padding: 0 32rpx 28rpx;}
.sel-item{height: 136rpx;position: relative;display: flex;align-items: center;}
.sel-item.first{justify-content: space-between;}
.sel-item .gap{width: 104rpx; height: 1rpx;}
.sel-item .from,.sel-item .to,.sel-item .date{height: 100%;flex: 1;transition: all 0.7s ease-out;line-height: 136rpx;}
.sel-item .date {display: flex;align-items: center;}
.sel-item .from,.sel-item .to{border-bottom: 1rpx solid #f4f4f4;}
.sel-item .to{text-align: right;}
.sel-item .tgrey{font-size: 26rpx;color: #bbb;line-height: 36rpx;}
.sel-item .strong{font-size: 44rpx;font-weight: 600;color: #111;}
.sel-item .from.exchange {transform: translate3d(calc(100% + 104rpx), 0, 0);text-align: right;}
.sel-item .to.exchange {transform: translate3d(calc(-100% - 104rpx), 0, 0);text-align: left;}
.sel-item .icon-change {width: 72rpx;height: 72rpx;position: absolute;left: 50%;top: 50%;margin: -42rpx 0 0 -42rpx;background: url(https://images3.c-ctrip.com/train/2023-3/xiaochengxu/7.19/shouye/img-qh.png) no-repeat;background-size: cover;z-index: 2;}
.sel-item .icon-change:active{animation: 1s ease-out rotate forwards;}
.p-t-16 {padding-top: 32rpx;}

@keyframes rotate{
    0% {transform: rotate(0deg)}
    50% { transform: rotate(180deg)}
}

