.intelligent-order-detail {
    /* height: 100vh; */
    /* background: linear-gradient(90deg, rgb(0, 170, 255) 0%, rgb(0, 111, 255) 100%); */
    background: -webkit-linear-gradient(top, #0086f6 20%, #eef1f6 80%);
}

.intelligent-order-detail .order-linear {
    background: none;
    margin-bottom: 0;
}

.intelligent-order-detail .order-ticbox {
    background-color: #fff;
    margin: 0 16rpx 22rpx;
    position: relative;
    border-radius: 6px;
}

.intelligent-order-detail .order-ticbox-hd {
    background: #fff;

    border-radius: 6px;
}

.intelligent-order-detail .order-ticbox-hd .top {
    border-bottom: none;
    display: block;
    background: linear-gradient(
        -180deg,
        rgb(241, 247, 253) 0%,
        rgb(255, 255, 255) 100%
    );
    border-radius: 6px;
}

.intelligent-order-detail .top-detail {
    color: #0086f6;
    font-family: PingFangSC-Regular;
    font-size: 30rpx;
    font-weight: normal;
    position: absolute;
    right: 40rpx;
    top: 26rpx;
}

.intelligent-order-detail .top-detail .decoration {
    border-radius: 0px 0px;
    border-left: 0.5px solid #979797;
    height: 20rpx;
    /* width: 2rpx; */
    margin: 0 16rpx;
    display: inline-block;
}

.intelligent-order-detail .decoration-cord {
    background: linear-gradient(
        -180deg,
        rgb(243, 248, 255) 0%,
        rgb(227, 233, 242) 100%
    );
    border-radius: 4px 4px 4px 4px;
    height: 44rpx;
    width: 12rpx;
    position: absolute;
    bottom: 0;
    transform: translate(0, 75%);
    left: 40rpx;
    z-index: 1;
}

.intelligent-order-detail .decoration-cord:last-child {
    left: unset;
    right: 40rpx;
}

.intelligent-order-detail .show-more-btn {
    margin: 20rpx auto;
    color: #999999;
    width: 100%;
    text-align: center;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    font-weight: normal;
    display: inline-flex;
    justify-content: center;
    align-items: center;
}

.intelligent-order-detail .show-more-btn .ifont-arr {
    transform: rotate(-90deg);
    height: 18rpx;
    margin-left: 2rpx;
}

.intelligent-order-detail .ifont-arr.unfold {
    transform: rotate(90deg);
}

.intelligent-order-detail .order-ticbox-bd {
    background-color: #f8fafd;
    border-radius: 4px;
    margin: 0 16rpx 16rpx;
}

.intelligent-order-detail .order-ticbox-bd .btn-grey {
    background: #ffffff;
    border-radius: 4px;
    border: 0.5px solid #edeff0;
    height: 60rpx;
    width: 120rpx;
    color: #0086f6;
    font-family: PingFangSC-Medium;
    font-size: 13px;
    font-weight: 500;
    /* height: 17px;
line-height: 17px;
width: 26px; */
}

.intelligent-order-detail .second-trip-tip {
    background: #2698f7;
    border-radius: 3px;
    height: 30rpx;
    line-height: 30rpx;
    /* width: 58px; */
    color: #ffffff;
    font-family: PingFangSC-Semibold;
    font-size: 12px;
    font-weight: 600;
    padding: 4rpx 10rpx;
    margin-left: 10rpx;
}

.intelligent-order-detail .cross-route-container {
    /* background: rgba(0, 61, 202, 0.2); */
    border-radius: 8px;
    height: 132rpx;
    /* width: 359px; */
    margin: 0 auto;
    position: relative;
    /* margin-bottom: 10px; */
    padding: 10rpx 40rpx 26rpx;
    box-sizing: border-box;
}

.intelligent-order-detail .router-top {
    height: 44rpx;
    color: #89a0c2;
    font-family: PingFangSC-Semibold;
    font-size: 22rpx;
    font-weight: 600;
    line-height: 22rpx;
    display: flex;
    align-items: center;
}

.intelligent-order-detail .decoration-stick {
    background: #bbc9dc;
    height: 1px;
    width: 100%;
    /* transform: translate(0, -50%); */
    width: 100%;
}

.intelligent-order-detail .decoration-stick.left-stick {
    height: 3px;
    width: 100%;
}

.intelligent-order-detail .decoration-dot {
    border-radius: 50%;
    background: #bbc9dc;
    height: 12rpx;
    width: 12rpx;
}

.intelligent-order-detail .decoration-dot:first-of-type {
    position: absolute;
    left: 40rpx;
}

.intelligent-order-detail .decoration-dot:nth-of-type(2) {
    position: absolute;
    right: 40rpx;
}

.intelligent-order-detail .router-top text {
    display: inline-block;
    text-align: center;
    background-color: #fff;
    border: 1px solid #89a0c2;
    border-radius: 10px;
    height: 24rpx;
    line-height: 24rpx;
    padding: 8rpx 12rpx;

    position: absolute;
}

.intelligent-order-detail .router-top text.mid-tip {
    /* background-color: #2078f4; */
    border: 1px solid #89a0c2;
    color: #89a0c2;

    left: 50%;
    transform: translate(-50%, 0);
}

.intelligent-order-detail .router-top text.from-tip {

  left: 25%;
  transform: translateX(-50%);
}

.intelligent-order-detail .router-top text.focus {
    background: #99aeca;
    color: #fff;
    border-color: #99aeca;
}

.intelligent-order-detail .router-top text.to-tip {

  right: 25%;
  transform: translateX(50%);
}

.intelligent-order-detail .router-bottom {
    top: 4rpx;
    position: relative;

    color: #333333;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    font-weight: normal;
    line-height: 34rpx;
}

.intelligent-order-detail .router-bottom .from-station {
    float: left;
}

.intelligent-order-detail .router-bottom .mid-station {
    position: absolute;
    left: 50%;
    transform: translate(-50%, 0);
}

.intelligent-order-detail .router-bottom .to-station {
    float: right;
}

.intelligent-order-detail .order-ticbox-bd .item {
    padding: 24rpx 0 0;
}
.intelligent-order-detail .red-top {
    background: linear-gradient(
        -180deg,
        rgb(255, 232, 232) 0%,
        rgb(255, 255, 255) 100%
    ) !important;
}

.intelligent-order-detail .failure-tips {
    margin-top: 14rpx;
    color: #999999;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    font-weight: normal;
    line-height: 12px;
    display: flex;
    align-items: center;
}

.intelligent-order-detail .failure-tips .ifont-detail {
    color: #f85e53;
    font-size: 11px;
    padding-right: 2px;
}

.intelligent-order-detail .failure-tips .ifont-detail::before {
    display: block;
    transform: rotate(180deg);
}


