}

.countdown-container .cont .tit {
    color: #333333;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    font-weight: normal;
    /* line-height: 1.25; */
    display: inline-block;
}

/* .countdown-container .cont .circle {
    width: 2px;
    height: 2px;
    background: #eef1f6;
    display: inline-block;
} */

.countdown-container .cont .tit .time {
    color: #333333;
}

.countdown-container .icon-pop-tishi {
    width: 64rpx;
    height: 64rpx;
    background: url("https://images3.c-ctrip.com/train/app/827/qiangpiao/icon_tishi.png")
        no-repeat;
    background-size: cover;
    margin-right: 40rpx;
    flex-shrink: 0;
}

.white-panel {
    box-sizing: border-box;
    margin: 0 16rpx 16rpx;
    border-radius: 16rpx;
    background: #fff;
    overflow: hidden;
}

/* 行程轴 */
.trip-axis {
    position: relative;
    padding: 24rpx;
    background: linear-gradient(-180deg, rgb(241, 247, 253) 4.73%, rgb(255, 255, 255) 100%);
}
.trip-axis .combi-tit {
    display: flex;
    flex-direction: row;
    position: absolute;
    top:0;
    left: 0;
}
.order-linear .trip-axis .combi-txt {
  line-height: normal;
}
.trip-axis .combi-tit .img{
    width: 94rpx;
    height: 36rpx;
}
.trip-axis .combi-tit .txt{
    color: #192964;
    font-size: 24rpx;
    background-color: #DFEDFF;
}
.trip-axis .combi-tit .img-after {
    width: 20rpx;
    height: 36rpx;
}
.trip-axis .tit{
    font-size: 30rpx;
    color: #333;
    line-height: 34rpx;
    font-weight: 600;
}
.trip-axis .tit .blue {
    color: #0086f6;
}
.trip-axis .axis {
    position: relative;
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin: 24rpx 0 12rpx;
}
.trip-axis .axis .real {
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: space-around;
}
.trip-axis .axis .axis-line {
    position: absolute;
    top: 50%;
    left: 0;
    transform: translateY(-50%);
    width: 100%;
    height: 4rpx;
    background: #AABBD3;
}
.axis-line::before,.axis-line::after {
    position: absolute;
    content: '';
    width: 12rpx;
    height: 12rpx;
    background: #AABBD3;
    border-radius: 50%;
    top: 50%;
    transform: translateY(-50%);
}
.axis-line::before {
    left: 0;
}
.axis-line::after {
    right: 0;
}
.trip-axis .axis .train {
    box-sizing: border-box;
    padding: 6rpx;
    border: 2rpx solid #ccd6e5;
    background: #fff;
    border-radius: 20rpx;
    color: #111;
    font-size: 24rpx;
    line-height: 30rpx;
    position: relative;
    z-index: 1;
}
.trip-axis .axis .train .ifont-arr {
    margin-left: 2rpx;
    font-size: 16rpx;
}
.trip-axis .axis .mid {
    background: #E2E9F2;
    border-radius: 20rpx;
    padding: 6rpx 16rpx;
    font-size: 24rpx;
    font-weight: 600;
    color: #111;
    line-height: 30rpx;
    position: relative;
    z-index: 1;
}
.trip-axis .axis .combi-train {
    display: flex;
    height: 40rpx;
    padding: 0 16rpx;
    justify-content: center;
    align-items: center;
}
.trip-axis .axis .cross {
    width: 172rpx;
    text-align: center;
    font-size: 24rpx;
    color: #999;
    position: relative;
    background: #fff;
}
.trip-axis .axis .cross text{
    position: relative;
    padding: 0 4rpx;
    background: #fff;
}
.trip-axis .axis .axis-dotted {
    width: 100%;
    height: 0;
    position: absolute;
    top: 50%;
    left: 0;
    width: 100%;
    height: 0px;
    border-top: 4rpx dotted #AABBD3;
    transform: translateY(-50%);
}
.axis-dotted::before,
.axis-dotted::after {
    content: "";
    position: absolute;
    width: 12rpx;
    height: 12rpx;
    border-radius: 50%;
    top: 0;
    transform: translateY(-8rpx);
}
.axis-dotted::before {
    left: -6rpx;
    background: #CCD6E5;
}
.axis-dotted::after {
    right: -6rpx;
    background: #AABBD3;
}
.axis-dotted.after::before {
    background: #AABBD3;
}
.axis-dotted.after::after {
    background: #CCD6E5;
}

.trip-axis .stations {
    display: flex;
    align-items: center;
    justify-content: space-between;
    color: #333;
    line-height: 34rpx;
    font-size: 26rpx;
}
.trip-axis .stations .txt.cross {
    color: #999999;
}

/* 车次卡片 */
.ticket-card {
    padding: 32rpx 24rpx 16rpx;
    position: relative;
}
.ticket-card.fail::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 120rpx;
    background: linear-gradient(-180deg, rgb(255, 232, 232) 4.73%, rgb(255, 255, 255) 100%);
}
.ticket-card .tag {
    position: absolute;
    top: 0;
    left: 0;
    padding: 4rpx 8rpx;
    background: #FF7700;
    border-radius: 16rpx 0 12rpx 0;
    font-size: 22rpx;
    color: #fff;
    line-height: 24rpx;
    letter-spacing: .14rpx;
}
.ticket-card-hd {
    position: relative;
    margin-bottom: 24rpx;
}
.ticket-card-hd .title {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 12rpx;
}
.ticket-card-hd .title .tit{
    font-size: 38rpx;
    font-weight: 600;
    line-height: 42rpx;
    color: #333333;
}
.ticket-card-hd .title .green{
    font-size: 30rpx;
    font-weight: 500;
    line-height: 34rpx;
    color: #00B87A;
}
.ticket-card-hd .no,.ticket-card-hd .err{
    font-size: 24rpx;
    color: #999;
    line-height: 1;
    display: flex;
    align-items: center;
}
.ticket-card-hd .err .ifont-detail {
    color: #f85e53;
    font-size: 22rpx;
    padding-right: 4rpx;
}

.ticket-card-bd {
    display: flex;
    align-items: center;
    justify-content: space-between;
    position: relative;
    text-align: center;
    margin-bottom: 32rpx;
}
.ticket-card-bd .info {
    color: #333;
}
.ticket-card-bd .info .date {
    font-size: 24rpx;
    line-height: 30rpx;
    margin-bottom: 4rpx;
}
.ticket-card-bd .info .time{
    font-size: 48rpx;
    font-weight: 600;
    line-height: 56rpx;
    margin-bottom: 4rpx;
}
.ticket-card-bd .info .area {
    font-size: 28rpx;
    font-weight: 500;
    line-height: 36rpx;
}
.ticket-card-bd .from {
    text-align: left;
}
.ticket-card-bd .to {
    text-align: right;
}
.ticket-card-bd .mid .checi {
    font-size: 24rpx;
    color: #999;
    line-height: 1;
    margin-bottom: 6rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.ticket-card-bd .mid .checi .change {
    color: #0086f6;
}
.ticket-card-bd .mid .transit {
    border-radius: 24rpx;
    border: 1px solid #cccccc;
    height: 38rpx;
    width: 140rpx;
    margin: 10rpx 0 26rpx;
    color: #5678a8;
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    line-height: 36rpx;
    display: inline-flex;
    justify-content: center;
    align-items: center;
    position: relative;
}
.ticket-card-bd .mid .transit::before,
.ticket-card-bd .mid .transit::after {
    content: '';
    background: #cccccc;
    position: absolute;
    top: 50%;
    width: 38rpx;
    height: 4rpx;
}
.ticket-card-bd .mid .transit::before {
    left: 0;
    transform: translateX(-100%);
    background: linear-gradient(-90deg, #ccc 0%, #fff 100%);
}
.ticket-card-bd .mid .transit::after {
    right: 0;
    transform: translateX(100%);
    background: linear-gradient(90deg, #ccc 0%, #fff 100%);
}
.ticket-card-bd.disabled .info,
.ticket-card-bd.disabled .checi,
.ticket-card-bd.disabled .transit {
    color: #ccc !important;
    border-color: #ccc !important;
}

.ticket-card-fd .pasg-box {
    box-sizing: border-box;
    padding: 24rpx;
    border-radius: 8rpx;
    background: #F8FAFD;
    margin-bottom: 16rpx;
}
.ticket-card-fd .pasg-box:last-child {
    margin-bottom: 0;
}
.ticket-card-fd .between {
    display: flex;
    align-items: center;
    justify-content: space-between;
    white-space: nowrap;

}
.ticket-card-fd .pasg-box-hd {
    color: #333;
    font-weight: 500;
    margin-bottom: 8rpx;
}
.ticket-card-fd .pasg-box-hd .name {
    font-size: 32rpx;
}
.ticket-card-fd .pasg-box-hd .carriage {
    font-size: 28rpx;
}
.ticket-card-fd .pasg-box-bd {
    color: #999999;
    font-size: 24rpx;
    line-height: 30rpx;
    margin-bottom: 22rpx;
}
.ticket-card-fd .pasg-box-fd .status {
    font-size: 26rpx;
