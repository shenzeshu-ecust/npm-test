    background: url("https://images3.c-ctrip.com/train/app/827/qiangpiao/bg_tishi.png")
        no-repeat;
    background-size: contain;
    /* width: 100%; */
    position: absolute;
    width: 100%;
    top: 0;
    left: 0;
    height: 100%;
