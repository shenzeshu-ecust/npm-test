.ttgob-white-box {
    width: 100%;
    border-radius: 20rpx;
    background-color: #fff;
    margin-bottom: 16rpx;
    padding: 32rpx;
    box-sizing: border-box;
    background: no-repeat url(https://images3.c-ctrip.com/train/2021/app/V8.41.6/xiaochengxu/zhuliucheng/img-bg.png) top;
    background-size: contain;
    background-color: #fff;
}

.ttgob-outter-title {
    color: rgb(51, 51, 51);
    font-size: 34rpx;
    font-weight: 600;
    text-align: center;
    margin-bottom: 50rpx;
}

.ttgob-inner-banner {
    display: flex;
    align-items: center;
    position: relative;
}
.ttgob-inner-banner.disutop{
  padding-top:32rpx;
  border-top:1rpx solid rgb(238, 238, 238);
}

.ttgob-inner-banner .left-img {
    width: 68rpx;
    height: 68rpx;
    margin-right: 20rpx;
}

.left-img.kuaisu {
    background: url(https://images3.c-ctrip.com/train/2021/app/V8.41.6/xiaochengxu/zhuliucheng/ic-jisu.png);
    background-size: cover;
}

.left-img.disu {
    background: url(https://images3.c-ctrip.com/train/2021/app/V8.41.6/xiaochengxu/zhuliucheng/ic-disu.png);
    background-size: cover;
}

.ttgob-inner-banner .mid-txt {
    flex-shrink: 0;
}

.mid-txt .main-title {
    color: rgb(51, 51, 51);
    font-size: 34rpx;
    font-weight: 600;
    line-height: 42rpx;
    display:flex;
    align-items:center;
}

.mid-txt .main-title .tag{
  color: rgb(255,119,1);
  font-size: 24rpx;
  margin-left:8rpx;
}
.mid-txt .sub-title {
    color: rgb(153, 153, 153);
    font-size: 24rpx;
    line-height: 30rpx;
    margin-top: 8rpx;
}

.ttgob-inner-banner .ttgob-right-btn {
    position: absolute;
    right: 0;
    top: calc(50% - 31rpx);
    border-radius: 8rpx;
    line-height: 62rpx;
    width: 152rpx;
    flex-shrink: 0;
    text-align: center;
    font-size: 26rpx;
    font-weight: 500;
}

.ttgob-right-btn.org {
    background: linear-gradient(90deg, rgb(255, 165, 10) 0%, rgb(255, 119, 0) 100%);
    color: #fff
}

.ttgob-right-btn.normal {
    border: 1rpx solid rgb(153, 153, 153);
    box-sizing: border-box;
    color: rgb(51, 51, 51);
    background-color: #fff;
    margin-top: 15rpx;
}

.ttgob-extra-des {
    color: rgb(51, 51, 51);
    font-size: 24rpx;
    height: 30rpx;
    letter-spacing: 0px;
    line-height: 30rpx;
    display: flex;
    padding: 24rpx 0 36rpx 56rpx;
    white-space: nowrap;
}

.ttgob-extra-des .ifont-gou {
    font-size: 20rpx;
    padding-left: 32rpx;
    padding-right: 12rpx;
    color: rgb(0, 134, 246);
}

.ttgod-backmask {
    transition: all 300ms ease-in-out;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1112;
    background-color: rgba(0, 0, 0, 0.7);
}

.ttgod-botm-box {
    border-radius: 24rpx 24rpx 0 0;
    transform: translate3d(0, 0, 0);
    position: fixed;
    left: 0;
    top: auto;
    right: 0;
    bottom: 0;
    z-index: 1113;
    transition: all ease-in-out 300ms;
    padding-bottom: env(safe-area-inset-bottom);
    background-color: #fff;
}
.ttgod-card {
  background: rgb(238,241,246);
  padding: 0 24rpx 20rpx 24rpx;
  box-sizing: border-box;
  width: 100%;
  border-radius: 24rpx 24rpx 0 0;
}
.ttgod-hd{
  display: flex;
}
.ttgod-hd .ttgod-left-img{
  width: 160rpx;
  height: 160rpx;
  background: url(https://images3.c-ctrip.com/train/2021/app/V8.41.6/xiaochengxu/zhuliucheng/img-piao.png);
  background-size: cover;
  padding-left:8rpx;
  padding-right: 20rpx;
}
.ttgod-hd .ttgod-right-txt{
  padding-top: 44rpx;
  padding-bottom: 28rpx;
}
.ttgod-main-title{
  color: rgb(51, 51, 51);
  font-size: 40rpx;
  font-weight: 600;
  line-height: 48rpx;
  padding-bottom: 10rpx;
}
.ttgod-sub-title{
  color: rgb(102, 102, 102);
  font-size: 26rpx;
  line-height: 34rpx;
}
.ttgod-bd{
  background-color: #fff;
  padding: 32rpx 32rpx 0 32rpx;
  box-sizing: border-box;
  margin-top: 12rpx;
  border-radius:24rpx;
}
.ttgod-order-box{
  padding-bottom: 32rpx;
  border-bottom: 1rpx solid rgb(238, 238, 238);
}
.ttgod-order-box .depart-time{
  color: rgb(153, 153, 153);
  font-size: 24rpx;
  line-height: 30rpx;
}
.ttgod-order-box .depart-time{
  color: rgb(153, 153, 153);
  font-size: 24rpx;
  line-height: 30rpx;
}
.ttgod-order-box .train-info{
  color: rgb(51, 51, 51);
  font-size: 40rpx;
  font-weight: 600;
  line-height: 76rpx;
  display: flex;
  justify-content: space-between;
  position: relative;
}

/** 中间的箭头 */
.ttgod-order-box .train-info .icon-go{
  width: 140rpx;
  height: 9rpx;
  position: absolute;
  content: "";
  background: url(https://pic.c-ctrip.com/train/wechat/icon-list-0620.png) no-repeat;
  background-size: cover;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.ttgod-order-box .train-info .no{
  color: rgb(170, 170, 170);
  font-size: 24rpx;
  font-weight: normal;
  line-height: 30rpx;
  position:absolute;
  left: 50%;
  top: 0;
  transform: translate(-50%,0);

}
.ttgod-order-box .ticket-info{
  color: rgb(153, 153, 153);
  font-size: 24rpx;
  line-height: 30rpx;
  display: flex;
  position: relative;
  justify-content: sapce-between;
}

.ttgod-order-box .ticket-info .pas{
  width: 288rpx;
  overflow:hidden; 
  text-overflow:ellipsis; 
  white-space:nowrap;
}
.ttgod-order-box .ticket-info .seat{
  position: absolute;
  right: 0;
}





.ttgod-des{
  padding-top:16rpx;
  padding-bottom: 32rpx;
}
.ttgod-des-li{
  padding-top: 16rpx;
  color: rgb(51, 51, 51);
  font-size: 24rpx;
  line-height: 32rpx;
}
.ttgod-des-li .ifont-gou{
  font-size: 18rpx;
  color:rgb(0,134,246);
  padding-right:8rpx;
}
.ttgod-btns{
  display: flex;
  justify-content: space-between;
  padding: 24rpx 32rpx;

}
.ttgod-btns .ttgod-btn{
  border-radius: 12rpx;
  height: 88rpx;
  width: 330rpx;
  text-align: center;
  box-sizing: border-box;
  line-height: 88rpx;
}
.ttgod-btn.normal{
  border: 1rpx solid rgb(153, 153, 153);
  color:rgb(51,51,51);

}
.ttgod-btn.org{
  color:#fff;
  background: linear-gradient(90deg, rgb(255, 165, 10) 0%, rgb(255, 119, 0) 100%);
}
.ttgod-btn.org .txt{
  padding-top:12rpx;
  line-height: 40rpx;
  font-weight: 600;
}
.ttgod-btn.org .price{
  font-size: 22rpx;
  line-height: 24rpx;
}
