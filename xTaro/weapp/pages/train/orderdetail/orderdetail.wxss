  flex-direction: column;
  flex: 1;
}
.free-child-view .free-child-left .free-child-up {
  display: flex;
  flex-direction: row;
  color: #333333;
  font-size: 32rpx;
  font-weight: 500;
  line-height: 40rpx;
  align-items: center;
}
.free-child-view .free-child-left .free-child-up .free-child-tag {
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0 8rpx;
  margin-left: 12rpx;
  height: 32rpx;
  background: #ffffff;
  border: 1rpx solid #bfeedd;
  border-radius: 4rpx;
  color: #00ae73;
  font-size: 22rpx;
  line-height: 24rpx;
}
.free-child-view .free-child-left .free-child-down {
  margin-top: 10rpx;
  color: #999999;
  font-size: 24rpx;
  line-height: 30rpx;
}
.free-child-view .ifont-del {
  color: #cccccc;
  font-size: 36rpx;
  width: 60rpx;
  height: 60rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  justify-content: center;
}

.order-ticbox-bd .item .child-declare-view .disabled {
  color: #ddd;
  border-color: #edeff0;
  background-color: #fff;
}
.free-child-view-pay {
  padding: 30rpx 0;
  margin: 0 32rpx;
  border-bottom: 1rpx solid #ececec;
}
.free-child-view .free-child-box-pay {
  margin-top: 0;
}

.countdown-container.point-card {
  margin: 0 0 20rpx 0;
}
/* 工具箱入口 */
.func-tools-box {
  padding: 28rpx 16rpx 24rpx;
  border-radius: 16rpx;
  background: #fff;
  display: grid;
  grid-template-columns: repeat(5, 1fr);
  align-items: center;
  text-align: center;
  margin: 16rpx;
}
.func-tools-box .item image {
  width: 52rpx;
  height: 52rpx;
  margin-bottom: 10rpx;
}
.func-tools-box .item .tit {
  font-size: 26rpx;
  font-weight: normal;
  line-height: 34rpx;
  color: #111;
}

/* 新人特权出票成功弹窗 */
.new-guest-privilege-pop {
  width: 600rpx;
  padding: 266rpx 40rpx 48rpx;
  border-radius: 32rpx;
  background: #fff;
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  z-index: 3002;
  box-sizing: border-box;
}
.new-guest-privilege-pop .top-img {
  position: absolute;
  top: -96rpx;
  left: 0;
  width: 630rpx;
  height: 456rpx;
  object-fit: contain;
}
.new-guest-privilege-pop .txt {
  font-size: 26rpx;
  color: #111;
  line-height: 40rpx;
  letter-spacing: -0.76rpx;
  position: relative;
  z-index: 1;
}
.new-guest-privilege-pop .strip {
  margin: 20rpx 0;
  padding: 0 32rpx 0 24rpx;
  border-radius: 20rpx;
  background: #f3f7fd;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  color: #111;
  font-weight: 600;
  letter-spacing: -0.88rpx;
}
.new-guest-privilege-pop .strip image {
  width: 80rpx;
  height: 100rpx;
}
.new-guest-privilege-pop .strip .desc {
  margin-right: auto;
  margin-left: 16rpx;
}
.new-guest-privilege-pop .strip .tgreen {
  color: #08a66f;
}
.new-guest-privilege-pop .btn {
  margin-top: 48rpx;
  border-radius: 12rpx;
  background: #006ff6;
  padding: 24rpx 0;
  font-size: 34rpx;
  line-height: 44rpx;
  color: #fff;
  text-align: center;
}
