@keyframes scale {
  from {
    transform-origin: top right;
    transform: scale(0);
  }
  to {
    transform-origin: top right;
    transform: scale(1);
    opacity: 1;
  }
}
@-webkit-keyframes animation1 {
  0% {
    transform: scale(1.5);
  }
  25% {
    transform: scale(0.2);
    opacity: 0;
  }
  50% {
    transform: scale(0.2);
    opacity: 0;
  }
  75% {
    transform: scale(0.2);
    opacity: 0;
  }
  10% {
    transform: scale(1.5);
    opacity: 1;
  }
}
@-webkit-keyframes animation2 {
  0% {
    transform: scale(0.2);
  }
  25% {
    opacity: 1;
    transform: scale(1.5);
  }
  50% {
    opacity: 1;
    transform: scale(1.5);
  }
  75% {
    opacity: 1;
    transform: scale(1.5);
  }
  100% {
    transform: scale(0.2);
    opacity: 0;
  }
}

