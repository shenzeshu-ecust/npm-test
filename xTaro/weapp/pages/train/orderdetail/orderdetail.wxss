/*前置扣位loading*/
.train-loading {
  position: relative;
  height: 36px;
  background: #fff;
}

.train-loading-bar {
  height: 36px;
  position: absolute;
  background: url('http://pic.c-ctrip.com/h5/train/train-loading.png') no-repeat
    top right;
  background-size: 80px 76px;
  background-color: #c2e8f8;
  width: 100%;
  overflow: hidden;
  -webkit-animation-name: trainrunning;
  -webkit-animation-duration: 1.5s;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-timing-function: linear;
  -webkit-transition: all 300ms;

  animation-name: trainrunning;
  animation-duration: 1.5s;
  animation-iteration-count: infinite;
  animation-timing-function: linear;
  transition: all 300ms;
}

.train-loading-txt {
  position: relative;
  z-index: 10;
  text-align: center;
  line-height: 36px;
  font-size: 12px;
}

@-webkit-keyframes trainrunning {
  from {
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
  }

  to {
    -webkit-transform: translate3d(200%, 0, 0);
    transform: translate3d(200%, 0, 0);
  }
}

@keyframes trainrunning {
  from {
    -webkit-transform: translate3d(-100%, 0, 0);
    transform: translate3d(-100%, 0, 0);
  }

  to {
    -webkit-transform: translate3d(200%, 0, 0);
    transform: translate3d(200%, 0, 0);
  }
}

