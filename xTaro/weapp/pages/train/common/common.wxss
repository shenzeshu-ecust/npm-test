.icon-closed{position: absolute;width: 60rpx;height: 60rpx;top:0rpx; right:0rpx;}
.icon-closed::before, .icon-closed::after{width: 1px;height: 28rpx;background: #19A0F0;position: absolute;content:"";left: 50%;top:50%;margin: -14rpx 0 0 -1rpx;}
.icon-closed::before{transform: rotate(45deg)}
.icon-closed::after{transform: rotate(-45deg)}

