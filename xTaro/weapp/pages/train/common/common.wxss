.icon.icon-radio, .icon.icon-que {
    width: 36rpx;
    height: 36rpx;
    background: #fff;
    border: 1px solid #19a0f0;
    border-radius: 50%;
    position: absolute;
}

.current .icon-radio {
    background: #19a0f0;
}

.current .icon-radio::after {
    width: 20rpx;
    height: 10rpx;
    border-left: 1px solid #fff;
    border-bottom: 1px solid #fff;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    position: absolute;
    content: "";
}

.triangle {
    width: 0;
    height: 0;
    border-left: 18rpx solid transparent;
    border-right: 18rpx solid transparent;
    border-top: 18rpx solid #fff;
    margin-left: 25%;
}

.css-tick {
    width: 20rpx;
    height: 10rpx;
    border-left: 2rpx solid #A1D737;
    border-bottom: 2rpx solid #A1D737;
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
    content: "";
}

.red {
    color:#F25306;
}
.cpinkgray {
    color: #999;
}
.placeholder-color {
    color: #ccc;
    font-size: 28rpx;
}
.jt-info {
    width: 144rpx;height: 36rpx;border-radius: 36rpx;border:1rpx solid rgba(255,255,255,.6);line-height: 36rpx;font-size: 24rpx;color: rgba(255,255,255,.8);margin: 0 auto;display: block;position: relative
}
.jt-info::before {
    position: absolute;
    top: 0;
    left: 0;
    width: 200%;
    border-radius: 10px;
    box-sizing: border-box;
    -webkit-transform: scale(.5);
    transform: scale(.5);
    -webkit-transform-origin: left top;
    transform-origin: left top;
    z-index: 0;
    border: 1px solid #fff;
    content: "经停信息";
    font-size: 40rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #19a0f0;
}
.jt-info::after {
    width: 180rpx;
    height: 1px;
    position: absolute;
    content: "";
    background: rgba(255,255,255,.5);
    left: 50%;
    top: 20rpx;
    margin-left: -90rpx;
    z-index: -1;
}


