.new-guest-panel .list {display: flex;align-items: center;gap: 8rpx;flex: 1;position: relative;z-index: 1;}
.new-guest-panel .item {background: #fff;border-radius: 12rpx;text-align: center;color: #333;flex: 1;box-shadow: inset 0px 0px 15px 0px rgba(255, 130, 146, 0.25);height: 158rpx;}
