/* 230713 列表新客礼包浮层 */
.new-guest-slideup {position: fixed;bottom: 0;left: 0;width: 100%;padding-bottom: env(safe-area-inset-bottom);background: #fff;border-radius: 24rpx 24rpx 0 0;overflow: hidden;box-shadow: inset 0px 0px 15px 0px rgba(255, 188, 188, 0.3);z-index: 3002;}
