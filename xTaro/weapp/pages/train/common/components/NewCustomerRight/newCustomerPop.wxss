/* 230713 新客礼包固定展示 */
.new-guest-panel {margin: 16rpx; border-radius: 16rpx;padding: 16rpx 16rpx 8rpx;box-sizing: border-box;box-shadow: inset 0px 0px 15px 0px rgba(255, 188, 188, 0.3);background: url(https://images3.c-ctrip.com/train/2023-3/zengzhang/7yue/xinke/BG.png) no-repeat;background-size: 100% 100%;position: relative;}
.new-guest-panel::before {content: '';position: absolute;top: 0;right: 0;width: 104rpx;height: 104rpx;background: url(https://images3.c-ctrip.com/train/2023-3/zengzhang/7yue/xinke/yilingqu.png) no-repeat;background-size: 100% 100%;}
.new-guest-panel.no-rights::before {
    content: none;
}
.new-guest-panel .tit{display: flex;align-items: flex-end;margin-bottom: 28rpx;text-indent: 8rpx;}
.new-guest-panel .tit .price{font-size: 50rpx;font-weight: 500;color: #333;line-height: 44rpx;}
