@import "../../iconfont.wxss";
@import "../../pop-box.wxss";
@import "../../repeatCss/bookPucMobileInput.wxss";
button::after{border:0;}
.page{background: #EEF1F6;display: flex;flex-direction: column;width: 100%;height:100%;position: absolute;left: 0;top:0;}
.wrap{ flex: 1;overflow-y: auto;-webkit-overflow-scrolling: touch;}

.tgrey{color: #999}
.torage{color: #f70;}
.tblue{color: #0086F6}

.fr{float:right}

.book-white{margin: 0 16rpx 16rpx;background: #fff;border-radius: 10px;}
.book-white.mt8{margin-top: 16rpx}

.book-tips{border-radius: 5px;background: #FFF8F2;padding: 16rpx 20rpx;line-height: 36rpx;font-size: 24rpx;color: #f70}
.book-tips.linear{background: -webkit-linear-gradient(left,#FFF8F2,#fff)}
.book-tips .item{margin-bottom: 8rpx;padding-left: 22rpx;position: relative}
.book-tips .item:last-child{margin-bottom: 0}
.book-tips .item::before{width: 6rpx;height: 6rpx;position: absolute;content: "";top: 12rpx;left: 0;border-radius: 6rpx;background: #FFA04D}

.login-12306{height:96rpx;display: flex;padding: 0 24rpx;align-items: center;}
.login-12306.ordinary{height: 124rpx;}
.login-12306.haslogin{height: 104rpx;}
.login-12306 .cont {display: flex;margin-left: 20rpx;flex: 1;align-items: center;}
.login-12306 .name{flex: 1;font-size: 34rpx;font-weight: 600;}
.login-12306 .name .tit{font-size: 32rpx;}
.login-12306 .name .txt{font-size: 22rpx;color: #999999;font-weight: 400;}
.login-12306 .rbox{font-size: 26rpx;color: #999;}
.login-12306 .rbox.ordinary{font-size: 28rpx;color: #0088FF;line-height: 28rpx;}
.login-12306 .ifont-arr{font-size: 20rpx;color: #ccc;margin-left: 4rpx;}
.login-12306 .ifont-arr.ordinary{color: #0088FF;}
.login-12306 .icon12306{width: 64rpx;height: 64rpx;background: url(https://images3.c-ctrip.com/train/dulujie/icon-12306@3x.png) no-repeat;background-size: 100% 100%;}
.add-pasg {padding: 1rpx 0 0}
.add-pasg .gray{color: #999;}
.add-pasg .item{margin: 0 24rpx;height: 88rpx;display: flex;align-items: center;padding: 20rpx 0;}
.add-pasg .item .del{width: 56rpx;margin-left: 4rpx;height: 88rpx;}
.add-pasg .ifont-del{font-size:34rpx;color:#F63B2E;line-height: 88rpx;}
.add-pasg .ifont-edit{font-size:30rpx;color:#ddd;line-height: 88rpx;}
.add-pasg .item .cont{flex: 1;position: relative;}
.add-pasg .item.disabled{color: #ccc;}
.add-pasg .item .cont.gaiqian::after{display: none}
.add-pasg .item .name{font-size: 34rpx;font-weight: 600;line-height: 40rpx;}
.add-pasg .item .type{font-size: 20rpx;color: #999;font-weight: 400;border:3rpx solid #EEE;padding: 0 4rpx;line-height: 28rpx;display: inline-block;vertical-align: middle;margin: -2px 0 0 12rpx;border-radius: 2px}
.add-pasg .item .cert{font-size: 24rpx;line-height: 26rpx;margin-top: 8rpx;color: #999;}
.add-pasg .item .cert .fr{float: right;color: #5678A8;}
.add-pasg .item .info{color: #5678A8;margin-left: 16rpx}

.add-pasg .btn-box{display: flex;margin: 0 32rpx;}
.add-pasg .btn-add{flex: 1;background: #fff;color: #0088ff;font-size: 30rpx;font-weight: 600;height: 100rpx;line-height: 100rpx;padding: 0;overflow: inherit;}
.add-pasg .btn-add::before{height: 48rpx;width: 1px;position: absolute;left: -10rpx;top:50%;margin-top: -24rpx;content: "";background: #eee;}
.add-pasg .btn-add:first-child::before{display: none;}
.add-pasg .item + .btn-box{border-top: 1px solid rgba(238,238,238,.5);margin-top: 12rpx;}
.add-pasg .item + .btn-box .btn-add{height: 108rpx;line-height: 108rpx;}
.add-pasg .btn-add text{font-size: 20rpx;margin-left: 4rpx}
.add-pasg .btn-add .ifont-add{font-size:38rpx;margin:-2px 8rpx 0 0;font-weight: 400}
.add-pasg .book-tips{margin: 24rpx 24rpx 12rpx}
.add-pasg .tick{margin-right: 24rpx}

.add-box {padding: 8rpx 0}
.add-box .item{display: flex;align-items: center;min-height: 110rpx;margin: 0 24rpx;border-bottom: 1rpx solid #eee;position: relative;flex-flow: row wrap;}
.add-box .item:last-child{border-bottom: 0;}
.add-box .item .ifont-arr{font-size: 20rpx;color: #ccc;margin-left: 4rpx;}
.add-box .label{width: 192rpx;font-size: 30rpx;}
.add-box .label.strong{font-size: 34rpx;font-weight: 600}
.add-box .cont{flex: 1;font-size: 30rpx;line-height: 110rpx}
.add-box .txt-r{text-align: right}
.add-box .txt-l16{margin-left: 16rpx}
.add-box .warn{color: #FF1313;font-size: 22rpx;line-height: 48rpx;margin:-1px 24rpx -12rpx;padding-left: 170rpx;background: #fff;position: relative;z-index: 2;}
.add-box .warn::before{position: absolute;left: 170rpx;right:0;top:0;border-top:1rpx solid #FF1313;content: "";}

.input-txt{width: 100%;height: 96rpx;line-height: 96rpx;font-size: 34rpx;min-height: 1.4rem;font-family: UICTFontTextStyleBody;}
.input-pldr{color: #ccc;font-size: 34rpx;}

.pop-login-bd .item .input-txt{width:90%;height: 90rpx;}
.pop-login-bd .item .input-pld{color: #BBB;}

.cleararea {padding:22rpx 30rpx 22rpx 40rpx;margin-right: -30rpx;position: relative;}

.agreement-info{margin: 32rpx 0 8rpx 40rpx;font-size: 22rpx;color: #999;line-height: 28rpx;}
.agreement-info .link{color: #5678A8}

.botm-pay{background: #fff;z-index: 50;border-top: 1rpx solid rgba(238,238,238,.5);padding: 8px 16px;display: flex;align-items: center;box-sizing: border-box;position: relative;padding-bottom:calc(8px + env(safe-area-inset-bottom));}
.botm-pay .lbox{flex: 1;font-size: 26rpx;margin-right: 32rpx}
.botm-pay .strong,.botm-pay .price{font-size: 42rpx;font-weight: 600;color: #f70;margin-left: 8rpx;}
.botm-pay .price::before{content: "\00A5";font-size: 24rpx;}
.botm-pay .arr::after{width: 12rpx;height: 20rpx;background: url(https://pic.c-ctrip.com/train/wechat/icon-book.png?v=0627) no-repeat;background-size: 144rpx 54rpx;background-position: -132rpx -16rpx;content: "";display: inline-block;vertical-align: middle;margin: -2px 0 0 8px;transform: rotate(-90deg)}
.botm-pay .arr.open::after{transform: rotate(90deg)}
.botm-pay .btn-submit{flex: 1;height: 88rpx;line-height: 88rpx;border-radius: 6px;background: -webkit-linear-gradient(left,#FFA50A,#FF7700);color: #fff;font-size: 34rpx;font-weight: 600;}
.botm-pay .btn-submit.green{background: linear-gradient(-221.15deg, rgb(20, 240, 157) 0%, rgb(53, 188, 231) 100%);}
.botm-pay .btn-blue{flex: 1;height: 88rpx;line-height: 88rpx;border-radius: 6px;background: -webkit-linear-gradient(left,#00A7FA,#0086F6);color: #fff;font-size: 34rpx;font-weight: 600;}
.botm-pay.iphone{padding-bottom: 50rpx; }

.pay-syz button{flex:1;margin-right: 32rpx;border-radius: 4px;height: 88rpx;}
.pay-syz button:last-child{margin-right: 0}

.pay-syz .btn-orage,
.pay-syz .btn-gold,
.pay-syz .btn-blue,
.pay-syz .btn-green{color:#fff;line-height: 1;font-size: 32rpx;padding-top: 12rpx;font-weight: 600;padding-left: 0;padding-right: 0}

.pay-syz .btn-orage{background: -webkit-linear-gradient(left,#FFA50A,#f70);}
.pay-syz .btn-blue{background: -webkit-linear-gradient(left,#00A7FA,#0086F6);}
.pay-syz .btn-gold{background: linear-gradient(-90deg, rgb(185, 136, 81) 0%, rgb(226, 188, 143) 100%);}
.pay-syz .btn-green{background: linear-gradient(-221.15deg, rgb(20, 240, 157) 0%, rgb(53, 188, 231) 100%);position: relative;}
.pay-syz .btn-green::before{content: '';position: absolute;width: 156rpx;height: 88rpx;right: 0;bottom: 0;background: url(https://images3.c-ctrip.com/train/2021/xiaochengxu/V8.44/ic_anxin_big@3x.png);background-size: cover;}

.pay-syz .btn-orage text,
.pay-syz .btn-gold text,
.pay-syz .btn-green text,
.pay-syz .btn-blue text{font-size: 22rpx;color: rgba(255,255,255,.9);display: block;margin-top: 8rpx;font-weight: 400}

.pay-syz .btn-orage .icon-light,
.pay-syz .btn-gold .icon-light,
.pay-syz .btn-green .icon-light,
.pay-syz .btn-blue .icon-light{width:24rpx;height:32rpx;background: url(https://pic.c-ctrip.com/train/wechat/light-w.png) no-repeat;background-size: cover;margin: -2px 10rpx 0 0}


.pay-syz .btn-putong{line-height: 88rpx;padding-top: 0;}

.pay-detail{bottom: calc(120rpx + env(safe-area-inset-bottom));border-bottom: 1px solid #f2f2f2;}
.pay-detail.iphoneX{bottom: 154rpx;}
.pay-detail .add-box .label{width: 280rpx;}
.pay-detail .add-box .dfn{font-size: 24rpx;color: #f70}
.pay-detail .add-box .price{font-size: 32rpx;color: #f70}

.ifont-checkbox{color:#ccc;}
.ifont-checkbox{font-size: 36rpx;}
.ifont-radio{color:#ddd;}
.ifont-radio{font-size: 32rpx;}

.cur .ifont-checkbox::before,.disabled .ifont-checkbox::before,.vip .ifont-checkbox::before{content: "\e7fa";}

.cur .ifont-checkbox,.cur .ifont-radio{color: #0086F6;}
.disabled .ifont-checkbox,.disabled .ifont-radio{color:#eee;}
.vip .ifont-checkbox,.vip .ifont-radio{color: #E3B168;}

.pop-child-select{padding: 16rpx 32rpx 32rpx;}
.pop-child-select .img{width: 100%;margin:24rpx 0 60rpx}
.pop-child-select .child-tips{background: #F8FAFD;border-radius: 4px;padding: 32rpx 24rpx 32rpx 24rpx;}
.pop-child-select .child-tips .item{padding-left: 22rpx;margin-bottom: 12rpx;position: relative;font-size: 26rpx;color: #666}
.pop-child-select .child-tips .item:last-child{margin-bottom: 0}
.pop-child-select .child-tips .item::before{width: 6rpx;height: 6rpx;border-radius: 6rpx;position: absolute;content: "";background: #AAA;left: 0;top:14rpx}
.pop-child-select .child-pasg{overflow: hidden;margin-bottom: 16rpx;}
.pop-child-select .child-pasg .item{width: 332rpx;height: 120rpx;float:left;margin-bottom: 16rpx;box-sizing: border-box;border:2rpx solid #AABBD3;text-align: center;padding: 20rpx 0; position: relative;color: #5678A8;border-radius: 4px}
.pop-child-select .child-pasg .item.cur{border-color: #0086F6;color:#0086F6;}
.pop-child-select .child-pasg .item:nth-child(even){float: right}
.pop-child-select .child-pasg .name{font-size: 34rpx;line-height: 48rpx;font-weight: 600;}
.pop-child-select .child-pasg .cert{font-size: 24rpx;line-height: 34rpx}
.pop-child-select .child-pasg .ifont-filtercheck{position: absolute;bottom: -1rpx;right:0;font-size: 28rpx;line-height: 28rpx;display: none;}
.pop-child-select .child-pasg .item.cur .ifont-filtercheck{display: block;}

.pop-child-info{padding: 12rpx 32rpx 0;}
.pop-child-info .tit{margin-bottom: 16rpx;font-size: 26rpx;color: #666;line-height: 34rpx;}
.pop-child-info .txt{margin-bottom: 6rpx;padding-left: 32rpx;position: relative;font-size: 26rpx;color: #666;line-height: 34rpx;}
.pop-child-info .txt::before{width: 6rpx;height: 6rpx;border-radius: 6rpx;position: absolute;content: "";background: #AAA;left: 10rpx;top:14rpx}
.pop-child-info .img{width: 100%;margin:50rpx 0 40rpx;}
.pop-child-info .botm-pay{margin: 0 -32rpx}
