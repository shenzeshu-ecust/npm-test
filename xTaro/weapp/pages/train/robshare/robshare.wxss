/* 任意行规则弹窗 */
.ruleCard.backmask {
    transition: all 300ms ease-in-out;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: -1;
    background-color: rgba(0, 0, 0, 0);
}
.ruleCard.backmask.active {
    z-index: 960;
    background-color: rgba(0, 0, 0, 0.7);
}
.ruleCard-main {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 750rpx;
    min-height: 88rpx;
}
.ruleCard-main.pop-botm {
    position: fixed;
    left: 0;
    top: auto;
    right: 0;
    bottom: 0;
    z-index: 1000;
    transition: all ease-in-out 300ms;
    transform: translate3d(0, 200%, 0);
    background: #fff;
    border-radius: 16px 16px 0 0;
    padding-bottom: env(safe-area-inset-bottom);
}
.ruleCard-main.active{
    transform: translate3d(0, 0, 0);
}
.ruleCard-main-head {
    height: 88rpx;
    background-color: #fff;
    border-radius: 8px 8px 0 0;
    position: relative;
    text-align: center;
    font-size: #333;
    line-height: 88rpx;
    letter-spacing: 0;
    font-weight: 500;
    font-size: 34rpx;
}
.ruleCard-main-head .ifont-closed-line {
    position: absolute;
    top: 30rpx;
    left: 30rpx;
    width: 28rpx;
    height: 28rpx;
    font-size: 28rpx;
    line-height: 1;
    text-align: center;
    color: #666;
    font-weight: normal;
    cursor: pointer;
}
.ruleCard-main-body {
    background-color: #fff;
    box-sizing: border-box;
    padding: 32rpx 40rpx 74rpx;
}
.ruleCard-main-body.scroll {
  max-height: 800rpx;
  overflow: auto;
}
.ruleCard-main-body .ruleCard-main-item {
    font-size: 28rpx;
    font-weight: 400;
    color: #333;
    line-height: 44rpx;
    margin-bottom: 44rpx;
}
.ruleCard-main-body .ruleCard-main-item:last-child {
    margin-bottom: 0;
}
