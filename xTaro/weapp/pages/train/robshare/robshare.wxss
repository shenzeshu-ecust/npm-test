    0% {
      transform: rotate(0deg);
      opacity: 1;
    }
    100%{
      transform: rotate(-360deg);
      opacity: 0;
    }
  }

  @-webkit-keyframes light{
    0%{
      -webkit-transform: scale(.85);
    }
    50%{
      -webkit-transform: scale(1.05);
      opacity: 1;
    }
    75%{
      -webkit-transform: scale(1);
    }
    100%{
      -webkit-transform: scale(1);
      opacity: 0;
    }
  }
  @keyframes light{
