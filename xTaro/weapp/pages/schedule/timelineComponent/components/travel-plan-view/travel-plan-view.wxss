.title{
    font-size: 36rpx;
    font-weight: 500;
    color: #333333;
    letter-spacing: 0;
    line-height: 44rpx;
}

.container{
    display: flex;
    flex-direction: column;
}

.card_container{
    width: 100%;
    display: flex;
    flex-direction: row;
    margin-top: 32rpx;
    justify-content: space-between;
}

.travel_item{
    border-radius: 12rpx;
    width: 48%;
    height: 218rpx;
    position: relative;
}

.see_all_text{
    color: white;
    font-size: 24rpx;
    letter-spacing: 0rpx;
    line-height: 30rpx;
    font-weight: normal;
    margin-top: 17rpx;
}

.see_more_arrow{
    width: 31rpx;
    height: 31rpx;
}

.see_more_container{
    display: flex;
    flex-direction: column;
    align-items: center;
    color: blue;
}

.see_more_content{
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
}

.travel_icon_bg{
    width: 100%;
    height: 100%;
    border-radius: 12rpx;
}

.travel_mask_bg{
    width: 100%;
    height: 60rpx;
    border-radius: 0 0 12rpx 12rpx;
    position: absolute;
    left: 0;
    bottom: 0;
    background-image: linear-gradient(0deg, rgba(0, 0, 0, 0.70), rgba(0, 0, 0, 0.00));
}

.card_title{
    width: 90%;
    position: absolute;
    left: 16rpx;
    bottom: 16rpx;
    color: white;
    font-size: 28rpx;
    font-weight: 500;
    line-height: 36rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}