
.container{
  padding-left: 36rpx;
  padding-right: 36rpx;
  padding-bottom: 30rpx;
  padding-top: 12rpx;
}
.wrapper {
  margin-bottom: 24rpx;
}
.wrapper-main {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.car-map {
  margin-right: 24rpx;
}
.row{
  display: flex;
  flex-direction: row;
}
.text-color-333{
  color: #333;
}
.text-color-999{
  color: #999;
}
.font-size-36{
  font-size: 36rpx;
}
.font-size-26{
  font-size: 26rpx;
}
.font-bold{
  font-weight: bold;
}

.chartered-text__main {
  font-size: 26rpx;
  color: #333;
}
.car-num{
  padding: 2rpx 10rpx;
  border: solid 2rpx #ccc;
  background-color: #f5f5f5;
  border-radius: 4rpx;
  flex-shrink: 0;
}

.vertical-center{
  align-items: center;
}
.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.arrival-address, .depart-address{
  line-height: 1.25;
}

.chartered-container{
  display: flex;
  flex-direction: column;
  padding: 0 12rpx 0 28rpx;
  align-items: flex-start;
}
.chartered-title {
  display: flex;
  align-items: flex-end;
  white-space: nowrap;
  width: 100%;
}
.chartered-title__text {
  font-size: 36rpx;
  color: #333333;
  font-weight: 500;
  display: inline-block;
}
.small-title {
  font-size: 26rpx;
  font-weight: 500;
  margin-bottom: 4rpx;
  margin-left: 4rpx;
}
.chartered-title__text-sub {
  font-size: 26rpx;
  font-weight: 500;
}
.margin-left__small {
  margin: 0 8rpx;
}
.chartered-content {
  display: flex;
  align-items: center;
  margin-left: 3rpx;
  margin-top: 16rpx;
}
.chartered-content__Left {
  margin-right: -13rpx;
}
.chartered-arrive__main {
  margin-top: 6rpx;
}
.margin-top10 {
  margin-top: 10rpx;
}
.car-number-common {
  color: #0186F6;
  background: linear-gradient(-30deg,  rgba(199,220,251, 0.6), rgba(225,239,250, 0.6));
  padding: 0 8rpx;
  font-size: 26rpx;
}
.car-number-bc {
  margin-left: 8rpx;
}
.car-number-others {
  margin-top: 6rpx;
}
.chartered-tip {
  color: #999999;
  font-size: 22rpx;
}

.chartered-tip_content {
  color: #999;
  font-size: 22rpx;
  margin-top: -2rpx;
  padding-left: 26rpx;
}
.circle {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  /* margin-left: 11rpx; */
  margin-right: 19rpx;
}
.cts_icon_arrow_line {
  width: 20rpx;
  height: 20rpx;
}
.margin-top-img {
  margin-top: -5rpx;
}
.start-color {
  background-color: #0086F6;
}
.end-color {
  background-color: #F51A0B;
}
.appointment-tips {
  color: #999;
  font-size: 26rpx;
  margin-left: 28rpx;
}
.margin-top11 {
  margin-top: 11rpx;
}
.margin-top16 {
  margin-top: 16rpx;
}
/** 两行省略号 **/
.ellipsis-2 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.passenger {
  display: flex;
  justify-content: flex-start;
  font-size: 26rpx;
  margin: 23rpx 16rpx 0;
  padding: 0 16rpx;
  background: #f8fafd;
  height: 72rpx;
  line-height: 72rpx;
}
.passenger-check {
  margin-right: 32rpx;
}
.passenger-label {
  color: #999;
}
.passenger-value {
  color: #333;
  font-weight: 500;
  margin-left: 16rpx;
}
.take-taxi-title__text {
  font-size: 26rpx;
}