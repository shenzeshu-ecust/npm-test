.l-modlist__item {
  margin-left: 32rpx;
  padding-top: 32rpx;
  padding-right: 32rpx;
  padding-bottom: 32rpx;
  border-bottom: 1rpx solid #ECECEC;
}

.c-listinnerwrap {
  display: flex;
  flex-direction: row;
}

.c-imgbox {
  position: relative;
  margin-right: 24rpx;
  width: 188rpx;
  height: 188rpx;
}

.c-imgbox>image {
  width: 100%;
  height: 100%;
  background-image: url("https://pages.c-ctrip.com/you/component-local/default.png");
  background-repeat: no-repeat;
  background-position: center;
  background-color: #eee;
  background-size: 50%;
}

.c-imgbox .business_time {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 18px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: rgba(34, 34, 34, 0.6);
  border-radius: 0 0 10rpx 10rpx;
}

.c-imgbox .business_time--text {
  font-size: 22rpx;
  font-family: 'PingFangSC-Regular';
  color: #fff;
}

.c-textview {
  flex: 1;
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.c-textview__title--text {
  font-size: 36rpx;
  font-family: 'PingFangSC-Regular';
  color: #333;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.c-textview__ename--text {
  font-size: 22rpx;
  font-family: 'PingFangSC-Regular';
  color: #222;
  margin-top: -4rpx;
  margin-bottom: -4rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.c-textview__comment {
  margin-top: 8rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.c-textview__comment--msltip {
  margin-right: 16rpx;
}

.c-textview__comment--msltip>text {
  font-family: 'PingFangSC-Regular';
  font-size: 28rpx;
  color: #DD2626;
}

.c-textview__comment--score {
  margin-right: 16rpx;
}

.c-textview__comment--score>text {
  font-family: 'PingFangSC-Regular';
  font-size: 28rpx;
  color: #ff6912;
}

.c-textview__comment--price {
  display: flex;
  flex-direction: row;
}

.c-textview__comment--price>text {
  font-family: 'PingFangSC-Regular';
  font-size: 26rpx;
  color: #222;
}

.c-textview__tagbox {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  flex-direction: row;
  align-items: center;
  display: flex;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
  height: 18px;
  overflow: hidden;
}

.c-textview__tagbox--item {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-right: 8rpx;
  padding: 0 10rpx;
  background-color: rgba(221, 38, 38, 0.08);
}

.c-textview__tagbox--item>text {
  font-size: 22rpx;
  color: #dd2626;
  opacity: 1.32;
}

.c-textview__bottom {
  display: flex;
  flex-direction: row;
}

.c-textview__bottom--distance {
  margin-right: 16rpx;
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.c-recommend {
  margin-right: 16rpx;
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 10rpx;
  border-top: 2rpx solid rgba(255, 255, 255, 0);
  padding-top: 16rpx
}

.c-recommend-ding {
  width: 32rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 24rpx;
  text-align: center;
  background-color: #DD2626;
  color: #fff;
}

.c-recommend-text {
  color: #808080;
  font-size: 24rpx;
  margin-left: 15rpx;

}

.landmark_outbox0 {
  margin-right: 15rpx;
}

.landmark_outbox {
  margin-right: 15rpx;
  flex: 1;
}

.landmark_outbox>text,
.landmark_outbox0>text {
  font-family: 'PingFangSC-Regular';
  color: #808080;
  font-size: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.c-textview__bottom--position>text {
  font-size: 26rpx;
  color: #808080;
}



.three-line .c-textview__title--text {
  margin-top: -8rpx;
  font-size: 36rpx;
  line-height: 56rpx;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.four-line .c-textview__title--text {
  margin-top: -8rpx;
  font-size: 36rpx;
  line-height: 56rpx;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.c-textview__sectitle {
  font-size: 12px;
  color: #666666;
  line-height: 18px;
  margin-bottom: 2px;
}

.three-line .c-textview__comment {
  line-height: 42rpx;
  display: flex;
  -webkit-box-orient: horizontal;
  flex-direction: row;
  -webkit-box-align: center;
  align-items: center;
  font-size: 28rpx;
  margin-top: 14rpx;
  margin-bottom: 14rpx;
}

.four-line .c-textview__comment {
  line-height: 42rpx;
  display: flex;
  -webkit-box-orient: horizontal;
  flex-direction: row;
  -webkit-box-align: center;
  align-items: center;
  font-size: 28rpx;
  margin-top: 4rpx;
}

.c-imgbox .specialStyle {
  border-radius: 10rpx;
}

.c-imgbox .top_number {
  display: block;
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
  width: 38rpx;
  height: 37rpx;
  line-height: 36rpx;
  text-align: center;
  color: #fff;
  font-size: 23rpx;
  border-radius: 10rpx 0 40rpx 0;
  padding-right: 12rpx;
  background-image: linear-gradient(90deg, #ff4dbb 0%, #f83030 76%);
}

.c-imgbox .top_number.current {
  background: rgba(0, 0, 0, 0.5);
}


.food-rank-text {
  display: inline-block;
  padding: 0 0 0 10rpx;
  font-size: 22rpx;
  height: 34rpx;
  line-height: 34rpx;
  color: #F8DAC7;
  background: linear-gradient(227deg, #3D3D3D 0%, #272727 100%);
  text-align: center;
  border-top-left-radius: 4rpx;
  border-bottom-left-radius: 4rpx;
  position: relative;
}

.food-rank-text::before {
  content: '';
  position: absolute;
  right: -16rpx;
  top: 0;
  display: inline-block;
  border-width: 34rpx 16rpx 0 0;
  border-color: #272727 transparent transparent transparent;
  border-style: solid;
}

.food-level-text {
  display: inline-block;
  padding: 0 10rpx 0 0;
  font-size: 22rpx;
  height: 34rpx;
  line-height: 34rpx;
  color: #333;
  background: #F8DAC7;
  text-align: center;
  margin-right: 4rpx;
  border-top-right-radius: 4rpx;
  border-bottom-right-radius: 4rpx;
  margin-left: 16rpx;
  position: relative;
}

.food-level-text::before {
  content: '';
  position: absolute;
  left: -16rpx;
  top: 0;
  display: inline-block;
  border-width: 0 0 34rpx 16rpx;
  border-color: transparent transparent #F8DAC7 transparent;
  border-style: solid;
}