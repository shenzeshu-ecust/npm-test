.wrap{position:fixed;top:0;left:0;right:0;bottom:0;z-index:300;background:rgba(0,0,0,0)}.frame-wrapper{width:100vw;height:100vh;position:fixed;left:0;top:50vh;z-index:500}.frame-wrapper .frame-content-padding{padding:40rpx 32rpx}.frame-wrapper .frame-content{position:absolute;bottom:0;box-sizing:border-box;width:100%;overflow:auto;border-radius:40rpx 40rpx 0 0}.frame-wrapper .frame-content .frame-title{display:flex;align-items:center;justify-content:space-between;font-size:36rpx;color:#333333;margin-bottom:24rpx}.frame-wrapper .frame-content .frame-title .title-box{display:flex;align-items:center}.frame-wrapper .frame-content .frame-title .title-icon{width:248rpx;height:48rpx;vertical-align:middle;margin-right:12rpx}.frame-wrapper .frame-content .frame-title .title-cont{display:inline-block;line-height:44rpx;font-weight:500}.frame-wrapper .frame-content .frame-title .frame-close{width:48rpx;height:48rpx}.frame-wrapper .frame-content .frame-slot{max-height:960rpx}.frame-wrapper .frame-content .frame-bottom{position:fixed;bottom:0;left:0}.wrapAnimate{animation:wrapAnimate 0.5s ease-in-out forwards}@keyframes wrapAnimate{0%{}100%{background:rgba(0,0,0,0.35)}}.wrapAnimateOut{animation:wrapAnimateOut 0.4s ease-in-out forwards}@keyframes wrapAnimateOut{0%{background:rgba(0,0,0,0.35)}100%{background:rgba(0,0,0,0)}}.frameAnimate{animation:frameAnimate 0.5s ease forwards}@keyframes frameAnimate{0%{}100%{opacity:1;top:0vh}}.frameAnimateOut{animation:frameAnimateOut 0.4s ease forwards}@keyframes frameAnimateOut{0%{opacity:1;top:0vh}100%{opacity:0;top:100vh}}::-webkit-scrollbar{width:0;height:0;color:transparent;display:none}
