.price-calendar{width:100%;display:flex;align-items:center;justify-content:space-between}.price-calendar-item-end{align-items:flex-end}.price-calendar-item{display:flex;flex-direction:column}.price-calendar-item .item-main{font-size:28rpx;color:#333;line-height:34rpx;font-weight:500}.price-calendar-item .item-main-txt{display:flex;align-items:center;margin-top:2rpx}.price-calendar-item .item-main-txt .week-txt{font-size:20rpx;color:#999;line-height:24rpx}.price-calendar-item .item-main-txt .more-icon{display:block;width:8rpx;height:16rpx;margin-left:8rpx;margin-bottom:-2rpx}.price-calendar-item .day-txt{font-size:22rpx;color:#333;padding:2rpx 12rpx;border:1rpx solid rgba(63,73,83,0.11);border-radius:100rpx;font-weight:normal}
