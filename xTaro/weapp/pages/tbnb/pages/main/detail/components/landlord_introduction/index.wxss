.landlord-introduction{padding:0 32rpx}.landlord-introduction .module-name{margin:60rpx 0 16rpx 0;font-size:36rpx;color:#333333;font-weight:500}.landlord-introduction-head{width:calc(100% - 44rpx);height:72rpx;line-height:60rpx;padding:0 22rpx;font-size:18rpx;display:flex;justify-content:space-between;border-radius:12rpx 12rpx 0 0;background-size:100% 100%;background-repeat:no-repeat}.landlord-introduction-content{width:calc(100% - 48rpx);margin-top:-12rpx;background:#ffffff;border-radius:12rpx;padding:0 24rpx}.landlord-introduction-content-top{display:flex;justify-content:space-between;align-items:top;padding-top:32rpx}.landlord-introduction-content-top .landlord-image{position:relative;width:108rpx;height:108rpx;border:1rpx solid #eee;background:#f3f3f3;border-radius:50%}.landlord-introduction-content-top .landlord-image .head-portrait{width:108rpx;height:108rpx;border-radius:50%}.landlord-introduction-content-top .landlord-image .img-londing-error{position:absolute;top:50%;left:50%;transform:translate(-50%, -50%);width:94rpx;height:94rpx}.landlord-introduction-content-top .landlord-image .head-badge{width:54rpx;height:54rpx;position:absolute;bottom:-10rpx;right:-10rpx}.landlord-introduction-content-top .landlord-info{flex:1;margin:0 16rpx 0 24rpx}.landlord-introduction-content-top .landlord-info-name{display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;overflow:hidden;font-size:32rpx;color:#333333;font-weight:500}.landlord-introduction-content-top .landlord-info-tag{font-size:20rpx;display:flex;margin-top:9rpx}.landlord-introduction-content-top .landlord-info-tag-item:not(:last-child)::after{display:inline-block;content:'|';height:24rpx;margin:0 8rpx;color:#9b9b9b}.landlord-introduction-content-top .landlord-info-desc{display:flex;margin-top:8rpx}.landlord-introduction-content-top .landlord-info-desc .item{padding:3rpx 8rpx;border-radius:4rpx;font-size:20rpx;margin-left:8rpx}.landlord-introduction-content-top .landlord-contact{width:144rpx;height:48rpx;line-height:48rpx;background:#0086f6;border-radius:100rpx;color:#ffffff;text-align:center;font-size:24rpx;margin-top:4rpx}.landlord-introduction-content-evaluate{display:flex;padding:43rpx 0 24rpx 0}.landlord-introduction-content-evaluate .evaluate-item{width:calc(100% / 3)}.landlord-introduction-content-evaluate .evaluate-item-rate{font-size:20rpx;color:#999999}.landlord-introduction-content-evaluate .evaluate-item-percentage{font-size:36rpx;color:#333333;font-weight:600}.landlord-introduction-content-evaluate .evaluate-item-desc{font-size:22rpx;color:#666666}.landlord-introduction-content-intro{background:#f7f9fb;border-radius:12rpx;padding:24rpx}.landlord-introduction-content-intro .text{display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:2;overflow:hidden;font-size:22rpx;color:#333333}.landlord-introduction-content-bottom{font-size:24rpx;color:#333;padding:28rpx 0}.landlord-introduction .hight-light{color:#007fe9}
