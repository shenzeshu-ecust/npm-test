.nav-bar{position:fixed;top:-80rpx;left:0;right:0;z-index:100}.nav-bar .nav-bar-inner{width:calc(100% - 80rpx);height:80rpx;background:#ffffff;box-shadow:0 2rpx 6rpx 0 rgba(0,0,0,0.06);padding:0 40rpx;position:absolute;top:0;left:0;display:flex}.nav-bar .nav-bar-li{height:100%;display:flex;align-items:center;font-size:22rpx;color:#666666;font-weight:normal;position:relative}.nav-bar .nav-bar-li:not(:last-child){margin-right:56rpx}.nav-bar .active{font-weight:500;font-size:26rpx;color:#333333}.nav-bar .bottom-active{width:44rpx;height:4rpx;background:#0086f6;position:absolute;bottom:6rpx;left:3rpx;border-radius:20rpx}.navAnimate{animation:navAnimate 0.3s ease forwards}@keyframes navAnimate{0%{}100%{top:0}}.navAnimateOut{animation:navAnimateOut 0.4s ease forwards}@keyframes navAnimateOut{0%{}100%{top:-80rpx}}
