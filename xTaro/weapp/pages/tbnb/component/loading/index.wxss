.loading-wrapper{width:100%}.loading-wrapper .loading-logo{display:flex;justify-content:center;margin:0 0 30rpx 0}.loading-wrapper .logo{width:84rpx;height:84rpx}.loading-wrapper .circular-list{display:flex;justify-content:center;box-sizing:border-box;padding-right:10rpx}.loading-wrapper .circular{width:10rpx;height:10rpx;border-radius:50%;margin:0 10rpx;margin-right:0;animation:load 1.4s infinite ease-in-out}.loading-wrapper .circular-list .circular1{animation-delay:0.13s}.loading-wrapper .circular-list .circular2{animation-delay:0.26s}.loading-wrapper .circular-list .circular3{animation-delay:0.39s}@keyframes load{0%{opacity:1}100%{opacity:0}}.loading-wrapper-fixed{width:210rpx;height:210rpx;display:flex;flex-direction:column;justify-content:center;background:#fff;border-radius:10rpx;position:fixed;top:45%;left:50%;transform:translate(-50%, -50%);z-index:3001}.loading-wrapper-fixed-mask{width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,0.1);z-index:3000}
