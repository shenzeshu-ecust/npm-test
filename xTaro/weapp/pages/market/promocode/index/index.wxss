page {background: #F4F4F4;height: 100%;display: flex;flex-direction: column;}
.fl {float: left;}
.coupon-wrap {position: fixed;left: 0;top: 0;width: 100%;z-index: 999;-webkit-box-shadow: 0 6rpx 16rpx 2rpx rgba(0, 0, 0, 0.08); box-shadow: 0 6rpx 16rpx 2rpx rgba(0, 0, 0, 0.08);}
.wx-tips{height:88rpx;line-height:88rpx;font-size:28rpx;color:#ca7b09;background-color: #fffaee;border-top: 1px solid #fbe5ab;text-align:center;}
.fixed-list {background: #FFFFFF;overflow: hidden;position: relative;z-index: 3;}
.fixed-item {position: relative;float: left;text-align: center;width: 50%;height: 88rpx;color: #666;line-height: 88rpx;font-size: 30rpx;overflow: hidden;}
.fixed-item:after {content: "";display: inline-block;width: 20rpx;height: 20rpx;margin: 0 4rpx 0 20rpx;border-right: 4rpx solid #999;border-bottom: 4rpx solid #999;box-sizing: border-box;-webkit-transform: rotate(45deg);-ms-transform: rotate(45deg);transform: rotate(45deg);vertical-align: 8rpx;}
.fixed-item.current {color: #222;font-weight: 700;}
.fixed-item.current:after {border-right: 4rpx solid #666;border-bottom: 4rpx solid #666;-webkit-transform: rotate(-135deg);-ms-transform: rotate(-135deg);transform: rotate(-135deg);vertical-align: -4rpx}
.apply-list {position: relative;padding: 20rpx 0 4rpx 20rpx;background: #fff;overflow: hidden;}
.apply-item {float: left;position: relative;margin: 0 12rpx 16rpx 0;padding: 0 24rpx;text-align: center;font-size: 26rpx;height: 72rpx;line-height: 72rpx;color: #666;border: 1px solid #e0e0e0;border-radius: 10rpx;box-sizing: border-box;}
.apply-item.current {color: #f45256;border: 1px solid #ffb2b4;}
.lately-list {position: relative;background: #fff;}
.lately-item {position: relative;padding-left: 25rpx;font-size: 28rpx;height: 96rpx;line-height: 96rpx;color: #666;border-bottom: 2rpx solid #ddd;}
.lately-item.current:after {content: "";position: absolute;right: 32rpx;top: 30rpx;width: 16rpx;height: 30rpx;border-right: 4rpx solid #000;border-bottom: 4rpx solid #000;box-sizing: border-box;-webkit-transform: rotate(45deg);-ms-transform: rotate(45deg);transform: rotate(45deg)}
.scrolltop-hastips{padding-top: 176rpx;}
.scrolltop-notips{padding-top: 88rpx;}
.coupon-list {position: relative;height: 100vh;width: 100%;font-family: "Hiragino Sans GB",Arial,"Microsoft YaHei","Lucida Grande",Verdana;}
.item-li {position: relative;margin-bottom: 20rpx;border-bottom-left-radius: 18rpx;border-bottom-right-radius: 18rpx;min-height: 100rpx;}
.item_main {position: relative;overflow: auto;width:100%;background: #F4F4F4;border-top-left-radius: 18rpx;border-bottom-left-radius: 18rpx;border-top-right-radius: 18rpx;border-bottom-right-radius: 18rpx;box-shadow: 0px 4rpx 4rpx 0 #ececec;-webkit-box-shadow: 0px 4rpx 4rpx 0 #ececec;}
.item_status{position: absolute;top: 0; right: 0;height: 81rpx;width: 78rpx;z-index: 2;}
.item_status.item_first_pay { background: url(https://images3.c-ctrip.com/marketing/2018/12/coupon_new_h5/status_first_pay.png) 100% no-repeat;background-size: contain;}
.item_status.item_is_expire { background: url(https://images3.c-ctrip.com/marketing/2018/12/coupon_new_h5/status_is_expire.png) 100% no-repeat;background-size: contain;}
.item_status.item_new_receive { background: url(https://images3.c-ctrip.com/marketing/2018/12/coupon_new_h5/status_new_receive.png) 100% no-repeat;background-size: contain;}
.item_status.item_no_effect { background: url(https://images3.c-ctrip.com/marketing/2018/12/coupon_new_h5/status_no_effect.png) 100% no-repeat;background-size: contain;}
.item_left {width: 200rpx;height: 200rpx;color: #FFFFFF;text-align: center;position: relative;background: -webkit-linear-gradient(-45deg, #F85E53, #F98078);  background: -o-linear-gradient(-45deg, #F85E53, #F98078);background: -moz-linear-gradient(-45deg, #F85E53, #F98078);background: linear-gradient(-45deg, #F85E53, #F98078);}
.item_left .icon-em {font-size: 20rpx;line-height: 28rpx;height: 28rpx;}	
.noEffect .item_left{background: -webkit-linear-gradient(-45deg, #D0D0D0, #D2D2D2);background: -o-linear-gradient(-45deg, #D0D0D0, #D2D2D2); background: -moz-linear-gradient(-45deg, #D0D0D0, #D2D2D2);  background: linear-gradient(-45deg, #D0D0D0, #D2D2D2);  }
.is_disabled .item_left{background: -webkit-linear-gradient(-45deg, #D0D0D0, #D2D2D2);background: -o-linear-gradient(-45deg, #D0D0D0, #D2D2D2); background: -moz-linear-gradient(-45deg, #D0D0D0, #D2D2D2);  background: linear-gradient(-45deg, #D0D0D0, #D2D2D2);  }
.is_expired .item_left{background: -webkit-linear-gradient(-45deg, #D0D0D0, #D2D2D2);background: -o-linear-gradient(-45deg, #D0D0D0, #D2D2D2); background: -moz-linear-gradient(-45deg, #D0D0D0, #D2D2D2);  background: linear-gradient(-45deg, #D0D0D0, #D2D2D2);  }
.item_price {width:100%;display: inline-block;text-align: center;top: 50rpx; position: relative;  }
.item_price.middle{position: absolute;  top: 50%;  left: 50%; transform: translate(-50%, -50%); }
.item_price .price_info{display: inline-block;  }
.item_price .span{  display: inline-block;  font-size: 28rpx;  }
.item_price .icon-b.fsSmall{  font-size: 48rpx;line-height: 48rpx;  }
.item_price .icon-b.fsLarge{  font-size: 80rpx; line-height: 80rpx; }
.item_price .icon-b .item_decimal {font-size: 36rpx;line-height: 36rpx;font-style: normal;font-weight: 400;}
.item_price .icon-b .item_unit {font-size: 36rpx;line-height: 36rpx;font-style: normal;font-weight: 400;}
.item_price .icon-b {display: inline-block;font-size: 64rpx; line-height: 64rpx;font-weight: bold;}
.item_price .item_available {color: #fff;  font-size: 20rpx;}
.item_left .item_pricetext{text-align: center;}
.item_pricetext{ position: absolute; top: 50%;  left: 50%;  transform: translate(-50%, -50%);  width: 120rpx;  font-size: 58rpx;  line-height: 60rpx;  }
.item_right {background-color: #fff;  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen", "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue", sans-serif;  width: calc(100% - 200rpx);  text-align: left;  height: 200rpx;  position: relative;  }
.item_right .act_body {height: 100%;font-weight: 400; margin-left: 20rpx; margin-right: 20rpx;  }
.item_right .item_shortName { font-size: 30rpx;line-height: 30rpx; height: 32rpx; padding: 28rpx 0 0 0;color: #333333;overflow: hidden;  white-space: nowrap;text-overflow: ellipsis;font-weight: 400;}
.is_expired .item_right .item_shortName {color: #999999}
.noEffect .item_right .item_shortName {color: #999999}
.is_disabled .item_right .item_shortName {color: #999999}
.item_right .ellips {white-space: nowrap;overflow: hidden;text-overflow: ellipsis;  }
.item_right .act_name {font-size: 24rpx;line-height: 32rpx; padding: 6rpx 0 0 0;color: #999999;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 2;white-space: normal;  }
.item_right .act_duration {font-size: 20rpx;line-height: 26rpx;color: #999999;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
.item_right .remark_btn { position: absolute;bottom: 0;left: 0; height: 100%;width: 320rpx;  }
.item_right .remark_btn .remark_text {position: absolute; bottom: 20rpx;  left: 0;margin-left: 20rpx;line-height: 24rpx;font-size: 20rpx;  color: #aaaaaa;  }
.item_right .remark_btn .remark_text::after {content: "";display: inline-block;width: 12rpx;height: 12rpx;margin: 0 6rpx 0 6rpx; border-right: 2rpx solid #CCCCCC;border-bottom: 2rpx solid #CCCCCC;box-sizing: border-box;  -webkit-transform: rotate(45deg);-ms-transform: rotate(45deg);  transform: rotate(45deg);vertical-align: 4rpx;}
.item_right .remark_btn.cps_expand .remark_text::after {-webkit-transform: rotate(-135deg);-ms-transform: rotate(-135deg);  transform: rotate(-135deg);vertical-align: 0px;}
.item_right .cps_item_usebtn{position: absolute;right: 0;bottom: 0;width: 160rpx;height: 100%;  }
.noEffect .item_right .cps_item_usebtn .item-btn{background-color: #EEEEEE;  border: 0;  color: #999999;}
.noEffect .item_right .cps_item_usebtn .item-btn:after{border: 0;}
.is_disabled .item_right .cps_item_usebtn .item-btn{background-color: #EEEEEE;  border: 0;  color: #999999;}
.is_disabled .item_right .cps_item_usebtn .item-btn:after{border: 0;}
.item_right .cps_item_usebtn .item-btn {position: absolute;bottom: 22rpx; right: 0; padding: 6rpx 24rpx;color:#F85E53;border: 0;font-size: 20rpx;text-align: center;line-height: 28rpx;display: block;margin-right: 16rpx;}
.item_right .cps_item_usebtn .item-btn:after{position: absolute;content: "";border: 2rpx solid #FCC5C1;width: 200%;height: 200%;top: 0;left: 0;transform: scale(.5);transform-origin: 0 0;-webkit-transform: scale(.5);border-radius: 16rpx;-webkit-transform-origin: 0 0;} 
.item_right .cps_item_usebtn .item-btn.c999 {color:#999;font-size:18rpx;}
.item_right .cps_item_usebtn .item-btn.c999:after{border: 2rpx solid #999;}
.item_sub_list {width:100%;background: #F9F9F9;border-bottom-left-radius: 18rpx;border-bottom-right-radius: 18rpx;position: static;visibility: visible;overflow: hidden;margin-top: -20rpx;margin-bottom: 0px;padding-top: 20rpx;padding-bottom: 0px;}
.item_sub {padding: 24rpx 30rpx 10rpx;font-size: 24rpx;color: #666;line-height: 30rpx;  }
.item_sub .item_tip {list-style-type: disc;}
.item_sub_list > .item_sub:last-of-type{padding-bottom:44rpx;}
.transfer_from{flex:1;padding: 22rpx 0 24rpx 0;line-height: 30rpx;}
.item_tips {  padding: 0;  margin-bottom: 24rpx;  overflow: hidden;  font-size: 20rpx;  color: #666666;  }
.item_tips .item_tip {line-height: 46rpx;float: left;list-style-type: none;box-sizing: border-box;width: 50%;border-radius: 4rpx;border-bottom: 4rpx rgba(249, 249, 249, 0.9) solid;background-color: #ffffff;}
.item_tips .item_tip:nth-of-type(odd) {border-right: 4rpx rgba(249, 249, 249, 0.9) solid;}
.tips_num {color: #999999;display: inline-block;width: 46rpx;height: 100%;text-align: center;  }
.item-text{font-size:24rpx;color:#666;line-height:1.8}
.item_remark_tmpl{overflow: hidden;list-style: none;margin: 0;padding: 0;-webkit-padding-start: 40rpx;}
.item_remark_tmpl .item_remark_li{list-style-type: disc;}

/*浮层遮罩*/
.coupon-mask{position:fixed;left:0;top:0;width:100%;height:100%;background:rgba(0,0,0,0.3);z-index:2}
/*更多券*/ 
.coupon-more{height:80rpx;line-height:80rpx;text-align:center;background:#fff;color:#666;border-radius:12rpx;position:relative;font-size:28rpx;}
.coupon-little{margin-right:20rpx;margin-left: 20rpx;margin-bottom: 40rpx;text-align: center} 
.little-hastips{margin-top: 238rpx;}
.little-notips{margin-top: 150rpx;}
.little-img{width:200rpx;height:330rpx;background:url(http://images3.c-ctrip.com/marketing/2015/07/coupon_new_h5/dlp_awk.png) no-repeat center;background-size:100%} 
.little-text{margin-top:6rpx;font-size:24rpx;text-align:center;display:block}
button{margin:14rpx 0 0 30%; width:40%;height:60rpx;line-height:60rpx;color:#04a7dd;border:1px solid rgba(4, 167, 221,1);border-radius:10rpx;text-align:center;font-size:28rpx;background:rgba(0, 0, 0, 0);}
.little-btn{color: #f45256;border: 1px solid #ffb2b4;}
.loadall{color:#999;text-align:center;font-size:24rpx;} 