/*============ 助力成功浮层--发券通用样式（目前携程券用的这个样式） ============*/
.pop_wrap{
  z-index: 2;
  background: rgba(248,248,248,1);
  border-radius: 20rpx;
}
.pop-close{
  position: absolute;
  bottom: -110rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 60rpx;
  height: 60rpx;
  padding: 15rpx;
  box-sizing: border-box;
  border: 1px solid #fff;
  border-radius: 50%;
}
.pop-close::before,
.pop-close::after {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%);
  width: 28rpx;
  height: 1px;
  background-color: #fff;
  margin-left: -14rpx;
}
.pop-close::before {
  transform: rotate(45deg);
}
.pop-close::after {
  transform: rotate(-45deg);
}
.pop_wrap-content{
  overflow: hidden;
}
.pop_wrap-content.relative{
  position: relative;
  overflow: visible;
}
.pop_wrap-content .pop-close{
    bottom: -170rpx;
    
