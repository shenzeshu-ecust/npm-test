.arrow-top {
  position: relative;
  margin-left: 12rpx;
}

.arrow-top::after {
  content: "";
  position: absolute;
  width: 14rpx;
  height: 14rpx;
  border-top: 3rpx solid #fff;
  border-right: 3rpx solid #fff;
  transform: rotate(135deg);
  top: -6rpx;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  /* transition: all 0.1s linear; */
}

.arrow-down::after{
  top: 8rpx;
  border-top: none;
  border-right: none;
  /* transform: rotate(-45deg); */
  border-left: 3rpx solid #fff;
  border-bottom: 3rpx solid #fff;
}

