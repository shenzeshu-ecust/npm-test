.dice-loading{
  position: absolute;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 1334rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/diceLoading.gif') 0 0/750rpx no-repeat;
  z-index: 3;
}
.dice-result{
  position: absolute;
  top:462rpx;
  left:280rpx;
  width: 169rpx;
  height: 185rpx;
  background-position: 0 0;
  background-repeat: no-repeat;
  background-size: 169rpx;
  transform:scale(0);
  z-index: 3;
}
.dice-result.number-1{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/1.png');
}
.dice-result.number-2{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/2.png');
}
.dice-result.number-3{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/3.png');
}
.dice-result.number-4{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/4.png');
}
.dice-result.number-5{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/5.png');
}
.dice-result.number-6{
  animation:dice_scale 0.6s ease forwards;
  background-image: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/6.png');
}
@keyframes dice_scale{
  0%{transform:scale(1);}
  50%{transform:scale(1.4);}
  100%{transform:scale(1);}
}
@keyframes light_rotate_open{
  0%{bottom: -330rpx;transform:rotate(0);}
  100%{bottom: 320rpx;transform:rotate(360deg);}
}