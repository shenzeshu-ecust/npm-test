image{
  width: 100%;
  height: 100%;
}
.f-bolder{
  font-weight: bolder;
}
.ellipsis{
  white-space:nowrap;
  text-overflow:ellipsis;
  overflow-x:hidden;
}
.index-wrap{
  position: relative;
}
.index-wrap.fullscreen{
  position:fixed;
  top:0;
  right:0;
  bottom:0;
  left:0;
  width:100%;
  height:100%;
}
.main{
  position: relative;
  width: 100%;
  height: 1049rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/index_bg6.png') 0 0/750rpx no-repeat;
}
.main .btn-icon{
  position: absolute;
  bottom: 90rpx;
  width: 98rpx;
  height:106rpx;
}
.main .btn-icon.rule{
  left: 12rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/btn-rule.png') 0 0/98rpx no-repeat;
}
.main .btn-icon.notice{
  left: 128rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/bell-bg2.png') 0 0/98rpx no-repeat;
}
.main .btn-icon.notice .bell-big{
  position: absolute;
  top: 2rpx;
  left: 16rpx;
  width: 65rpx;
  height: 67rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/bell-big.png') 0 0/65rpx no-repeat;
  animation:bellBig 4s linear infinite;
  transform-origin:top;
}
@keyframes bellBig{
  0%,8%,16%,24%,100%{
    transform:rotate(0deg);
  }
  4%,20%{
    transform:rotate(-10deg);
  }
  12%{
    transform:rotate(10deg);
  }
}
.main .btn-icon.notice .bell-small{
  position: absolute;
  bottom: 26rpx;
  left: 36rpx;
  width: 26rpx;
  height: 26rpx;
  background: url('https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/bell-small.png') 0 0/26rpx no-repeat;
  animation:bellSmall 4s linear infinite;
  transform-origin:50% -50rpx;
}
@keyframes bellSmall{
  0%,8%,16%,24%,100%{
    transform:rotate(0deg);
  }
  4%,20%{
    transform:rotate(-20deg);
  }
  12%{
    transform:rotate(20deg);
  }
}
.main .btn-icon.notice .bell-tips{
  position: absolute;
  top: 0rpx;
  right: 16rpx;
  width: 28rpx;
  height: 28rpx;
  background: url(' https://images3.c-ctrip.com/marketing/2020/08/xcx_energy/index/bell-tips.png') 0 0/28rpx no-repeat;
  animation:bellScale 1.5s linear forwards;
  transform:scale(0);
}
@keyframes bellScale{
  0%,50%{
    transform:scale(0);
  }
  55%,100% {  
	  transform:scale(1);
  }
}
