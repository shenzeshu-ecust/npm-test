}
@keyframes denglong{
  0% {
      transform: rotate(3deg);
  }
  50% {
      transform: rotate(-3deg);
  }
  100% {
      transform: rotate(3deg);
  }
}
.bg_sub_title {
  position: absolute;
  width: 610rpx;
  height: 60rpx;
  text-align: center;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/bg_sub_title.png');
  background-size: 100% 100%;
  top: 200rpx;
  left: 50%;
  transform: translateX(-50%);
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #CA7A4A;
  line-height: 33px;
}

@keyframes denglong2{
  0% {
      transform: rotate(-2deg);
  }
  50% {
      transform: rotate(2deg);
  }
  100% {
      transform: rotate(-2deg);
  }
}
.line {
  width: 750rpx;
  height: 102rpx;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/line.png');
  background-size: 100%;
  position: absolute;
  top: 240rpx;
  animation: animationLine 3.5s .15s linear infinite;
  transform-origin: center center;
}
@keyframes animationLine{
  0% {
      transform: perspective(500px) translateZ(10px);
  }
  50% {
      transform: perspective(500px) translateZ(-10px);
  }
  100% {
      transform: perspective(500px) translateZ(10px);
  }
}
.award_card_desc {
  width: 170rpx;
  height: 250rpx;
  position: absolute;
  transform-origin: top center;
  text-align: center;
  z-index: 1;
}
.award_card_desc .content {
  position: relative;
  font-size: 18rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #FAEAB6;
  line-height: 36rpx;
  width: 100%;
  text-align: center;
}
.award_card_1 {
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/award_1.png');
  background-size: 100%;
  height: 255rpx;
  top: 301rpx;
  left: 39rpx;
  z-index: 99;
}
.award_card_1.animate {
  animation: animationAward1 3.5s .15s linear infinite;
}
.award_card_1 .content {
  position: absolute;
  transform: rotate(3deg);
  top: 40rpx;
  /* left: 48rpx; */
}
@keyframes animationAward1{
  0% {
      transform: perspective(500px) translateZ(7px);
  }
  50% {
      transform: perspective(500px) translateZ(-7px);
  }
  100% {
      transform: perspective(500px) translateZ(7px);
  }
}
.award_card_2 {
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/award_2.png');
  background-size: 100%;
  top: 312rpx;
  left: 212rpx;
  width: 159rpx;
  z-index: 99;
}
.award_card_2.animate {
  animation: animationAward2 3.5s .15s linear infinite;
}
.award_card_2 .content {
  position: absolute;
  top: 41rpx;
  /* left: 48rpx; */
}
@keyframes animationAward2{
  0% {
      transform: perspective(500px) translateZ(-7px);
  }
  50% {
      transform: perspective(500px) translateZ(7px);
  }
  100% {
      transform: perspective(500px) translateZ(-7px);
  }
}
.award_card_3 {
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/award_3.png');
