  height: 730rpx;
  background: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/bg_header.jpg');
  background-size: 100% 100%;
  position: relative;
}
.btn_rule {
  position: absolute;
  top: 300rpx;
  right: 0;
  width: 76rpx;
  height: 76rpx;
  background: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/btn-rule.png') 0 0 / 100% 100% no-repeat;
  z-index: 1;
}
.btn_history {
  position: absolute;
  top: 200rpx;
  right: 0;
