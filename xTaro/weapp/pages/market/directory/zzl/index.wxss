  /* left: 53rpx; */
}
@keyframes animationAward3{
  0% {
      transform: perspective(500px) translateZ(9px);
  }
  50% {
      transform: perspective(500px) translateZ(-5px);
  }
  100% {
      transform: perspective(500px) translateZ(9px);
  }
}
.award_card_4 {
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/award_4.png');
