page {
  padding-bottom: 50rpx;
  padding-top: 120rpx;
  background: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/history-top.jpg') 0 0 / contain no-repeat #B16348;
}
.history_item{
  margin-bottom: 60rpx;
}
.point_btn {
  position: fixed;
  top: 87rpx;
  right: 0;
  width: 121rpx;
  height: 45rpx;
  line-height: 45rpx;
  background: #FF9955;
  font-size: 20rpx;
  font-family: FZLanTingHeiS-R-GB;
  color: #FFFFFF;
  text-align: center;
  border-radius: 20rpx 0 0 20rpx;
  z-index: 10000;
}

.period {
  width: 246rpx;
  font-size: 24rpx;
  font-family: FZLanTingHeiS-R-GB;
  font-weight: 400;
  line-height: 47rpx;
  color: #FFFFFF;
  text-align: center;
  margin: 30rpx auto 0;
  border: 1rpx solid #FFFFFF;
  border-radius: 24rpx;
}
.card_wrap {
  width: 710rpx;
  height: auto;
  margin: 26rpx auto 0;
}
.card_wrap .card_wrap_top {
  width: 100%;
  height: 95rpx;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/bg_card_top.png');
  background-size: 100% 100%;
}
.card_wrap .card_wrap_middle {
  width: 100%;
  height: auto;
  /* max-height: 700rpx; */
  /* overflow-y: scroll; */
  /* padding-bottom: 60rpx; */
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/lottery/bg_card_middle.png');
  background-size: contain;
  background-repeat: no-repeat;
}
.card_wrap .card_wrap_middle.card-prize-result {
  height: 484rpx;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/history-prize-result.png');
}
.card_wrap .card_wrap_middle.card-prize-result .place-holder-top{
  height: 100rpx;
}
.place-holder-top{
  height: 85rpx;
}
