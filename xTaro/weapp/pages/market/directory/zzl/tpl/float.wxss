.book {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  height: 217rpx;
  display: flex;
  justify-content: space-around;
  animation-fill-mode: forwards;
  opacity: 0;
  z-index: 999;
  animation: big 1.5s  2s;
}
.book-item{
  width: 156rpx;
  height: 217rpx;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/default_bg.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
@keyframes big {
  0% {
    bottom: 0;
  }
  30% {
    bottom: 50%;
    transform: translateY(50%);
    opacity: .5;
  }
  60% {
    bottom: 50%;
    transform: translateY(50%);
    opacity: 1;
  }
  90% {
    bottom: 50%;
    transform: translateY(50%);
    opacity: 1;
  }
  100% {
    bottom: 50%;
    transform: translateY(50%);
    opacity: 1;
  }
}

.book_short {
  animation: big2 1.5s 3s;
}
@keyframes big2 {
  0% {
    transform:translateY(0rpx);
  }
  30% {
    transform: translateY(-35%);
    opacity: .5;
  }
  60% {
    transform: translateY(-35%);
    opacity: 1;
  }
  90% {
    transform: translateY(-35%);
    opacity: 1;
  }
  100% {
    transform: translateY(-35%);
    opacity: 0;
  }
}
