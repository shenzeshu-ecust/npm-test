.round {
  position: fixed;
  left: 0;
  bottom: -500rpx;
  width: 100vw;
  height: 500rpx;
  background-image: url('https://images3.c-ctrip.com/marketing/2021/02/zzl2/xuanzhuan.png');
  background-size: 100%;
  background-repeat: no-repeat;
  animation: zhuan 3s;
  transform-origin: top center;
  animation-fill-mode: forwards;
  transform: rotate(0deg);
  opacity: 0;
  z-index: 9999;
}
@keyframes zhuan {
  /* 0% {transform:rotate(0deg);}
  30% {opacity: .3;}
  60% {opacity: .6;}
  80% {opacity: .8;}
  90% {transform:rotate(-180deg) scale(2,2);opacity: 1;}
  100% {transform:rotate(-180deg) scale(2,2);opacity: 0;} */
  0% {
    transform: rotate(0deg);
    opacity: 0;
  }
  60% {
    opacity: .6;
  }
  80% {
    opacity: .8;
  }
  90% {
    transform: rotate(-180deg);
    opacity: 1;
  }
  100% {
      transform: rotate(-180deg);
      opacity: 0;
  }
}

