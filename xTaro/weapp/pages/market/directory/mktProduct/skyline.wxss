.scene_box {
  margin: 15rpx auto;
}

.mkt_sk_card_layout {
  margin: 0 !important;
  padding: 0 !important;
  display: flex;
  flex-direction: row;
  position: relative;
}

.mkt_sk_card_left {
  width: 240rpx;
  height: 240rpx;
  border-radius: 8rpx;
  background: url("https://images3.c-ctrip.com/marketing/2023/03/sk/default.png") 50% no-repeat;
  background-size: 50%;
  position: relative;
  overflow: hidden;
}

.mkt_sk_card_productimage {
  width: 240rpx;
  height: 240rpx;
  border-radius: 8rpx;
}

.mkt_sk_card_position {
  display: inline-flex;
  position: absolute;
  top: 0;
  left: 0;
}

.mkt_sk_card_position_text {
  width: auto !important;
  min-width: 48rpx !important;
  background: rgba(51, 51, 51, 0.8);
  border-radius: 14rpx 0 14rpx 0 !important;
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #ffffff;
  line-height: 40rpx;
  text-align: center;
}

.mkt_sk_recommendicon {
  display: inline-flex;
  position: absolute;
  right: 20rpx;
  top: 0;
  width: 50rpx;
  height: 60rpx;
  border-radius: 0rpx !important;
}

.mkt_sk_explanationbox {
  display: flex;
  justify-content: center;
  align-items: center;

  position: absolute;
  bottom: 0;
  left: 0;

  color: #ffffff;
  background: linear-gradient(-90deg, #ff001b 0%, #ff6600 100%);
  text-align: center;
  font-size: 26rpx;
}

.mkt_sk_explanation {
  width: 100%;
  height: 32rpx;
  line-height: 32rpx;
}

.mkt_sk_explanationicon {
  display: inline-flex;

  width: 24rpx;
  height: 24rpx;
  margin-right: 4rpx;
  background-size: 24rpx;
}

.mkt_sk_explanation_ing {
  font-size: 20rpx;
}

.mkt_sk_explanation_item {

  color: #ffffff;
  text-align: center;
  font-size: 26rpx;

  display: flex;
  justify-content: center;
  align-items: center;
}

.mkt_sk_explanation_askbox {
  position: absolute;
  bottom: 16rpx;
  left: 50%;
  transform: translateX(-50%);

  width: 128rpx;
  height: 48rpx;

  background: rgba(0, 0, 0, 0.6);
  border-radius: 44rpx;
  line-height: 60rpx;
}

.mkt_sk_explanation_askbox_addMessage {
  width: 208rpx !important;
  background: rgba(221, 221, 221, 1) !important;
  overflow: hidden;
}

.mkt_sk_explanation_askbox .ask_text {
  font-size: 24rpx;

}

.mkt_sk_explanation_askbox .ask_icon {
  width: 24rpx;
  height: 24rpx;
  margin-right: 4rpx;
  background-size: 24rpx;
  margin-top: 2rpx;
  background-image: url("https://images3.c-ctrip.com/marketing/2023/03/sk/white-voicenew.png") !important;
}

.mkt_sk_explanation_askbox .ask_textStatus {
  color: #333333 !important;
  background: rgba(255, 255, 255, 0.9) !important;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  border: 1rpx solid transparent;
  border-radius: 44rpx;

  width: 100%;
  height: 100%;
  display: inline-flex;
  justify-content: center;
  /* align-items: center; */
  line-height: 47rpx;
}

.mkt_sk_explanation_askbox .ask_message {
  color: #333333 !important;
  background: transparent !important;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  border: 1rpx solid transparent;
  border-radius: 44rpx;

  width: 100%;
  height: 100%;
  display: inline-flex;
  justify-content: center;
  /* align-items: center; */
  line-height: 47rpx;
}

.mkt_sk_explanation_close .ask_textStatus {
  border-color: rgb(221, 221, 221);
  color: rgb(51, 51, 51, 0.5) !important;
}

.mkt_sk_explanation_close .ask_message {
  border-color: transparent;
}

.mkt_sk_explanation_close .ask_text {
  opacity: 0.45 !important;
}

.mkt_sk_explanation_close .ask_icon {
  opacity: 0.45 !important;
}

.mkt_sk_explanation_saleoff_box {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5) !important;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.mkt_sk_explanation_saleoff_box .saleoff_text {
  color: #ffffff;
  font-size: 26rpx;
}

/* 卡片的右侧 */
.mkt_sk_card_right {
  width: 450rpx;
  padding: 0 0 0 16rpx;

  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.mkt_sk_card_producttitle {
  font-size: 28rpx;
  color: #333333;
  letter-spacing: -0.17px;
  line-height: 34rpx;
  text-align: left;
  font-weight: bold;

  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
  white-space: pre-wrap;
  max-height: 68rpx;

}


.mkt_sk_card_score_layout {
  display: flex;
  align-items: center;


  margin-top: 12rpx;
  height: 26rpx;
  line-height: 26rpx;
}

.mkt_sk_card_rangtag {
  height: 32rpx;
}

.mkt_sk_card_score {
  color: #0086f6;
  font-size: 26rpx;
  font-weight: 600;
}

.mkt_sk_card_score_unit {
  color: #0086f6;
  font-size: 26rpx;
  font-weight: 600;
}

.mkt_sk_card_commentsCount_icon {
  background: #DDDDDD;
  height: 26rpx;
  width: 1rpx;
  margin-left: 12rpx;
  margin-right: 12rpx;
}

.mkt_sk_card_commentsCount_count {
  color: #666666;
  font-size: 22rpx;
  font-weight: 300;
}

.mkt_sk_card_rangtag_box {
  display: inline-flex;
  align-items: center;
}

.mkt_sk_card_rangtag_beforeicon {
  background: #DDDDDD;
  height: 26rpx;
  width: 1rpx;
  margin-left: 12rpx;
  margin-right: 12rpx;
}

.mkt_sk_card_rangtag_infobox {
  display: flex;
  align-items: center;

  background: #FFEEDF;
  border-radius: 4rpx;
  height: 32rpx;

  padding-left: 8rpx;
  padding-right: 8rpx;
}

.mkt_sk_card_rangtag_icon {
  display: inline-flex;
  width: 24rpx;
  height: 24rpx;
}

.mkt_sk_card_rangtag_text {
  color: #673114;
  font-size: 22rpx;
  font-weight: 500;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  white-space: pre-wrap;
  height: 32rpx;
  line-height: 32rpx;
  margin-right: 4rpx;
  font-weight: bold;
  padding-left: 2rpx;
}

/*  */
.mkt_sk_card_zoneName_box {
  display: inline-flex;
  align-items: center;
  margin-left: 12rpx;
}

.mkt_sk_card_zoneName_beforeicon {
  height: 26rpx;
  width: 1rpx;
  margin-left: 12rpx;
  margin-right: 12rpx;
}

.mkt_sk_card_zoneName_infobox {
  display: inline-flex;
  align-items: center;

  border-radius: 4rpx;
  height: 32rpx;

  padding-left: 8rpx;
  padding-right: 8rpx;
}

.mkt_sk_card_zoneName_icon {
  display: inline-flex;
  width: 24rpx;
  height: 24rpx;
}

.mkt_sk_card_zoneName_text {
  color: rgba(102, 102, 102, 1);
  font-size: 22rpx;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  overflow: hidden;
  margin-right: 4rpx;
  padding-left: 4rpx;
}

/*  */
.mkt_sk_card_tuanTag {
  display: inline-flex;
  align-items: center;
  flex-wrap: wrap;
  height: 32rpx;
  overflow: hidden;
  background-size: 100% 100%;
}

.mkt_sk_card_tuanTagitem {
  border-radius: 4rpx;
  height: 32rpx;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  font-size: 22rpx;
  line-height: 34rpx;
  padding: 0 6rpx;
  margin-right: 8rpx;
  color: rgba(255, 119, 0, 1);
  background-image: linear-gradient(270deg, rgba(231, 109, 5, .2) 0%, rgba(255, 163, 58, .2) 100%);
}

.mkt_sk_card_taglist {
  display: flex;
  margin-top: 12rpx;
  flex-wrap: wrap;
  height: 32rpx;
  overflow: hidden;
}

.mkt_sk_card_tagitem {
  background: #f2f5f8;
  border-radius: 4rpx;
  height: 32rpx;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  color: #5678a8;
  font-size: 22rpx;
  line-height: 34rpx;
  padding: 0 6rpx;
  margin-right: 8rpx;
}

.mkt_sk_card_productactivitytag {
  background-color: transparent;
  background-image: linear-gradient(270deg, rgba(255, 56, 161, .9) 0%, rgba(255, 129, 85, .9) 100%);
  background-size: 100% 100%;

  border-radius: 4rpx;
  color: rgba(255, 255, 255, 1);
  font-size: 20rpx;
  font-weight: 700;

  padding: 1rpx 8rpx;
  text-align: center;

  display: inline-flex;
  justify-content: center;
  align-items: center;

  margin-right: 2rpx;
}

/* 价格部分 */
.mkt_sk_card_price_layout {
  display: flex;
  align-items: flex-end;
  flex: 1;
}

.mkt_sk_card_price_wrap {
  width: 0;
  flex: 1;
}

.mkt_sk_card_price_box {
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}

.mkt_sk_card_price {}

.mkt_sk_card_price .mkt_sk_card_price_unit {
  color: #ff0022;
  font-size: 22rpx;
  font-weight: 500;
  height: 26rpx;
  letter-spacing: -0.35rpx;
  line-height: 26rpx;
  display: inline-block;
  min-width: 18rpx;
  text-align: center;
}

.mkt_sk_card_price_start {
  color: #ff0022;
  font-size: 22rpx;
  font-weight: 500;
  height: 26rpx;
  letter-spacing: 0.35rpx;
  line-height: 26rpx;
  display: inline-block;
  min-width: 22rpx;
  padding-left: 2rpx;
  text-align: center;
}

.mkt_sk_card_price .mkt_sk_card_price_priceText {
  color: #ff0022;
  font-size: 40rpx;
  font-weight: 600;
  height: 48rpx;
  letter-spacing: -0.54rpx;
  line-height: 48rpx;
}

.mkt_sk_card_price .mkt_sk_card_price_priceSuffix {
  color: #ff0022;
  font-size: 22rpx;
  font-weight: 500;
  height: 26rpx;
  letter-spacing: -0.85rpx;
  line-height: 26rpx;
  margin-left: 2rpx;
  display: inline-block;
  /* min-width: 22rpx; */
  text-align: center;
  margin-left: 4rpx;
}

.mkt_sk_card_price .mkt_sk_card_price_originPrice {
  text-decoration: line-through;
  color: #999999 !important;
  font-size: 22rpx;
  font-weight: normal;
  letter-spacing: 0rpx;
  margin-left: 6rpx;
  height: 24rpx;
  line-height: 24rpx;
}

.mkt_sk_card_priceText_empty {
  color: rgb(153, 153, 153) !important;
  font-size: 28rpx !important;
}

.mkt_sk_card_reducedPrice {
  height: 24rpx;
  border-radius: 4rpx;
  width: max-content;
  font-size: 18rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
  /* justify-content: center; */
}

.mkt_sk_card_reduced_text {
  line-height: 24rpx;
  padding: 0rpx 10rpx 0 8rpx;
  color: #ffffff;
  background: linear-gradient(-90deg, #ff001b 0%, #ff6600 100%);
  display: flex;
  align-items: center;
}

.mkt_sk_card_discount_text {
  background: #ffe6e8;
  color: #ff0022;
  padding: 0 12rpx;
  font-size: 18rpx;
  height: 24rpx;
  border: 0.2rpx solid #FF001B;
  box-sizing: border-box;
  border-radius: 0 4rpx 4rpx 0;
  display: flex;
  align-items: center;
}

.mkt_sk_card_btn_box {
  position: relative;
  margin-left: 10rpx;
  background: linear-gradient(-90deg, #ff001b 0%, #ff6600 100%);
  border-radius: 30rpx;
  font-size: 26rpx;
  color: #ffffff;
  width: 132rpx;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;

  display: inline-flex;
  justify-content: center;
  align-items: center;
}

.mkt_sk_card_btn_unit {
  font-weight: bold;
}

.mkt_sk_card_btn_disabled {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(255, 255, 255, 0.5);
}