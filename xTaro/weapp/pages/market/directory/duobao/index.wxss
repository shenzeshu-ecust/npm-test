}

/* swiper */
.duobao_swiper {
  position: absolute;
  top: 317rpx;
  left: 24rpx;
  width: 450rpx;
  height: 50rpx;
  overflow-y: hidden;
  background-position: center;
  background-repeat: no-repeat;
  background-size: contain;
  background-image: url(https://images3.c-ctrip.com/marketing/task_components/lol/btn_bg.png);
}

.swiper_item {
  display: flex;
  height: 50rpx;
  justify-content: start;
  align-items: center;
  padding-left: 10rpx;
}

.swiper_img {
  width: 28rpx;
  height: 28rpx;
  flex-shrink: 1;
  border-radius: 50%;
  margin-right: 4rpx;
}

.swiper_text {
  color: #ffffff;
  font-size: 24rpx;
  font-family: PingFangSC-Medium;
  line-height: 1;
  width: 450rpx;
}

.ellipsis {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

