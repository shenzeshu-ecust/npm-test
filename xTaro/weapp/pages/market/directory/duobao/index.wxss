page {
  min-height: 100vh;
  padding-bottom: 100rpx;
  background: rgb(255, 239, 208);
  /* background: red; */
  width: 100vw;
  overflow-x: hidden;
}

.db_head {
  position: relative;
  overflow: hidden;
}

.db_head_img {
  position: relative;
  width: 100vw;
  height: 740rpx;
}

.db_head_btn,
.db_head_btn2 {
  position: absolute;
  top: 200rpx;
  right: 0rpx;
  padding: 10rpx 5rpx;
  background: rgba(255, 255, 255, 0.24);
  border: 1rpx solid rgba(255, 255, 255, 0.6);
  border-right: none;
  border-radius: 12rpx 0px 0px 12rpx;
  writing-mode: vertical-rl;
  color: #ffffff;
  font-size: 20rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  z-index: 100;
}

.db_head_btn2 {
  top: 280rpx;
}

.db_activity {
  position: relative;
  margin: 0 auto;
  width: 702rpx;
  box-sizing: border-box;
  padding: 20rpx 30rpx 20rpx;
  background: linear-gradient(-180deg, #FFE2F3 0%, #fff 90rpx, #fff 91rpx, #fff 100%);
  box-shadow: 0rpx 10rpx 24rpx 0px rgba(235, 215, 177, 0.6);
  border-radius: 24rpx;
  margin-top: -190rpx;
}

.db_activity__head {
  margin-bottom: 6rpx;
}

.db_activity__head_text {
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: bold;
  color: rgb(102, 102, 102);
  margin-right: 50rpx;
}

.db_activity__head_text.active {
  color: rgb(51, 51, 51);
}

.db_activity__head_line1,
.db_activity__head_line2 {
  width: 53rpx;
  height: 8rpx;
  background: linear-gradient(-90deg, rgb(255, 119, 0) 0%, rgb(255, 165, 10) 100%);
  margin-top: 5rpx;
  border-radius: 10rpx;
  transform: translateX(50rpx);
  transition: all 0.1s linear;
}

.db_activity__head_line2 {
  transform: translateX(260rpx);
}

.db_activity__myyo {
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  display: flex;
  align-items: baseline;
  padding: 4rpx 10rpx;
  background: linear-gradient(-184deg, rgba(255, 246, 220, 0.6) 0%, rgb(255, 246, 243) 100%);
  border: 1px solid #ffd5b3;
  border-radius: 100px 100px 100px 100px;
}

.db_activity__myyo-text1 {
  color: #ff6002;
  font-size: 22rpx;
  font-family: PingFangSC-Regular;
}

.db_activity__myyo-text2 {
  color: #ff6002;
  font-size: 28rpx;
  font-family: PingFangSC-Semibold;
  font-weight: bold;
}

.db_activity__myyo-arrow {
  width: 20rpx;
  height: 20rpx;
}

.db_activity__more {
  color: #aaaaaa;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  text-align: center;
  margin-top: 20rpx;
}

.goods_item {
  margin-top: 20rpx;
}

.db_activity__mydb_empty {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  padding: 60rpx 0 30rpx;
}

.db_activity__mydb_empty_img {
  width: 90rpx;
  height: 68rpx;
}

.db_activity__mydb_empty_tips {
  color: #666666;
  font-size: 24rpx;
  font-family: PingFangSC-Regular;
  margin: 20rpx 0;
}

.db_activity__mydb_empty_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 160rpx;
  height: 56rpx;
  background: linear-gradient(-231.65deg, rgb(255, 189, 30) 0%, rgb(255, 125, 42) 30.93%, rgb(255, 71, 64) 72.41%, rgb(255, 96, 180) 100%);
  border-radius: 28rpx;
  color: #ffffff;
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
}

.db_activity__arrow {
  width: 8rpx;
  height: 9rpx;
  border-top: 1px solid rgb(255, 105, 19);
  border-left: 1px solid rgb(255, 105, 19);
  margin-left: 4rpx;
  transform: rotate(135deg);
  align-self: center;
}

.db__modal_guide {
  position: relative;
  padding: 50rpx 30rpx;
  background: linear-gradient(-180deg, rgb(255, 236, 226) 0%, #fff 40%, #fff 100%);
  border-radius: 24rpx;
}

.db__modal_guide_head {
  position: absolute;
