}

.db__modal_success_halo_halo2 {
  position: absolute;
  top: -135rpx;
  left: 50%;
  width: 500rpx;
  height: 500rpx;
  transform: translate(-50%, -50%);
  z-index: 99;
}

.db__modal_success_title {
  width: 325rpx;
  height: 50rpx;
}

.db__modal_success_title2 {
  width: 375rpx;
  height: 38rpx;
}

.db__modal_success_gift2 {
  position: absolute;
  top: -260rpx;
  left: 50%;
  transform: translateX(-50%);
  z-index: 101;
  width: 260rpx;
  height: 260rpx;
}

.db__modal_success_text {
  color: rgb(51, 51, 51);
  font-size: 28rpx;
  font-family: PingFangSC-Regular;
  margin-top: 30rpx;
}

.db__modal_success_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 40rpx;
  width: 368rpx;
  height: 80rpx;
  background: url("https://pages.c-ctrip.com/market/components/static/duobao/img_halfdailog_btn.png") center / contain no-repeat;
  color: rgb(255, 255, 255);
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  font-weight: bold;
  margin: 40rpx auto 0;
  z-index: 10000;
  position: relative;
}

@keyframes rotate {
  0% {
    transform: translateX(-50%) rotate(0);
  }

  100% {
    transform: translateX(-50%) rotate(360deg);
  }
}

@keyframes rotate2 {
  0% {
    transform: rotate(0);
  }

  100% {
    transform: rotate(360deg);
  }
}

.db__modal_first_sendCoupon {
  position: fixed;
  top: 50%;
  left: 0px;
  width: 100vw;
  height: 800rpx;
  transform: translateY(-50%);
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.db__modal_first_sendCoupon_ext .mkt-dialog {
  transform: none;
}

.img_sy_head_dynic {
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 500rpx;
  background-color: transparent;
}

.firstSendCoupon1 {
  width: 100vw;
  height: 300rpx;
  background-color: transparent;
}

.firstSendCouponText1 {
  width: 100vw;
  height: 80rpx;
  background-color: transparent;
}

.firstSendCoupon2 {
  position: relative;
  z-index: 100;
  width: 100vw;
  height: 480rpx;
  background-color: transparent;
}

.firstSendCouponText2 {
  width: 100vw;
  height: 80rpx;
  background-color: transparent;
}

.db__modal_first_btn {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  background: transparent;
  box-shadow: none;
}

.db__modal_first_cards {
  width: 100vw;
  display: flex;
  justify-content: center;
  margin-top: 60rpx;
}

.db__modal_first_card_item {
  width: 190rpx;
  height: 240rpx;
  margin-left: 30rpx;
}

.db__modal_first_card_item:nth-child(1) {
  margin-left: 0;
}

.switch0,
.switch1,
.switch2 {
  transition: all 0.5s linear;
}

/* 交换0，1 */
.start_switch0 .switch0 {
  transform: translateX(220rpx);
}

.start_switch0 .switch1 {
  transform: translateX(-220rpx);
}

.start_switch1 {}

/* 交换1， 2 */
.start_switch2 .switch1 {
  transform: translateX(220rpx);
}

.start_switch2 .switch2 {
  transform: translateX(-220rpx);
}

.light-xuan {
  position: absolute;
  width: 100vw;
  top: 0;
  left: 0;
  height: 700rpx;
  animation: rotate2 20s linear infinite;
}

.light-bigger {
  position: absolute;
  top: 0;
  left: 0;
  width: 100vw;
  height: 700rpx;
  animation: bigger 0.3s linear forwards 2;
}

@keyframes bigger {
  0% {
    transform: scale(0);
  }

  50% {
    transform: scale(1);
  }

  100% {
    transform: scale(2);
    opacity: 0;
  }
}

.show {
  display: block;
}

.hide {
  display: none;
}

.youItem {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 500rpx;
}

.db_task {
  width: 702rpx;
  margin: 30rpx auto 0;
  border-radius: 24rpx;
  overflow: hidden;
  background-color: #fff;
  padding-top: 30rpx;
}

.db_task__head {
  display: flex;
  align-items: center;
  color: #333333;
  font-size: 34rpx;
  font-family: PingFangSC-Semibold;
  font-weight: bold;
  padding-left: 38rpx;
}

.db_task__yopiao {
  width: 52rpx;
  height: 52rpx;
  margin-right: 16rpx;
}

.db_task .task_wrap.module {
  padding: 0;
}

.db_task .task_item {
