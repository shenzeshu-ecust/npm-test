.search-box{position:relative;z-index:1;padding:7px 0;}
.search-box:before{content:'';display:block;position:absolute;left:12px;top:16px;z-index:1;width:10px;height:10px;border:1px solid #A9A9A9;border-radius:50%;}
.search-box:after{content:'';display:block;position:absolute;top:27px;left:22px;z-index:2;width:6px;height:1px;background-color:#A9A9A9;
	-webkit-transform:rotate(40deg);
	-ms-transform:rotate(40deg);
	transform:rotate(40deg);
}
.search-box input{position:relative;width:100%;padding:0 32px 0 30px;height:32px;   font-size:14px;font-family:Arial,"Lucida Grande",Verdana,"Microsoft YaHei",hei;outline:0 none;
	-webkit-box-sizing:border-box;
	-ms-box-sizing:border-box;
	box-sizing:border-box;
	-webkit-appearance:none;

   border-radius:15px;
   background-color: #f6f6f6;
   caret-color:blue;
   overflow:hidden;
   text-overflow:ellipsis;
   white-space:nowrap
}

.i-clear{position:absolute;top:13px;right:5px;width:22px;height:22px;
}
.i-clear:before,.i-clear:after{position:absolute;top:10px;left:4px;content:"";      width:12px;height:1px;background:#999;}
.i-clear:before{
	-webkit-transform:rotate(45deg);
	-ms-transform:rotate(45deg);
	transform:rotate(45deg);}
.i-clear:after{
	-webkit-transform:rotate(135deg);
	-ms-transform:rotate(135deg);
	transform:rotate(135deg);
}
