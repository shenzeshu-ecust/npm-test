@font-face {
    font-family: 'bus_wechat_icon';
    src: url('https://pic.c-ctrip.com/bus/resource/bus_wechat.ttf?version=1.1.4');

    src: url('https://pic.c-ctrip.com/bus/resource/bus_wechat.woff2?version=1.1.4')
            format('woff2'),
        url('https://pic.c-ctrip.com/bus/resource/bus_wechat.woff?version=1.1.4')
            format('woff'),
        url('https://pic.c-ctrip.com/bus/resource/bus_wechat.ttf?version=1.1.4')
            format('truetype'); /* iOS 4.1- */
}
.icon-font {
    font-family: 'bus_wechat_icon';
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    vertical-align: middle;
}
icon {
    text-align: center;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    display: inline-block;
    vertical-align: middle;
    font-size: 32rpx;
    line-height: 32rpx;
}
icon:before {
    font-family: 'bus_wechat_icon';
}

