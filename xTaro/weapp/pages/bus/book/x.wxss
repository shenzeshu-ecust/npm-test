@import '../common/template/bookExplain.wxss';
@import '../common/template/busInfo.wxss';
@import '../bus.wxss';
@import '../common/template/xPackageItem.wxss';
@import '../common/template/busNotice.wxss';

page {
    height: 100%;
    background-color: #f8f8f8;
    overflow-x: hidden;
    font-weight: 400;
}

.line-button {
    border-width: 2rpx;
    border-style: solid;
    background-color: #ffffff;
}
.line-button:after {
    border: none;
}
.line-button.button-hover {
    filter: grayscale(0.7);
}

.roundBtn {
    font-size: 24rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-right: 10rpx;
    border-radius: 54rpx;
    text-align: center;
    justify-content: center;
    padding: 10rpx 20rpx;
}
.roundBtn:last-child {
    margin-right: 0;
}
.roundBtn text {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}
.roundBtn:after {
    border: 1px solid;
    border-radius: 54rpx;
}

.icon-location:before {
    font-size: 22rpx;
}

.bus-unit {
    font-size: 24rpx;
}

.phoneInput {
    font-size: 40rpx;
    margin: 10rpx 0;
}

/* 滑动开关 */
.b-switch {
    -webkit-appearance: none;
    appearance: none;
    position: relative;
    width: 96rpx;
    height: 56rpx;
    border: 1px solid #dfdfdf;
    outline: 0;
    border-radius: 28rpx;
    box-sizing: border-box;
    background: #dfdfdf;
    display: inline-block;
}
.b-switch:before,
.b-switch:after {
    content: ' ';
    position: absolute;
    top: 0;
    left: 0;
    height: 52rpx;
    border-radius: 52rpx;
    -webkit-transition: -webkit-transform 0.3s;
    transition: transform 0.3s;
}

.pass-title,
.b-b-phonenum {
    padding-left: 30rpx;
    min-height: 85rpx;
    line-height: 85rpx;
}

.b-b-row {
    padding-left: 30rpx;
    padding-right: 30rpx;
    align-items: center;
}

.pass-title .small {
    font-size: 24rpx;
}
.btn-add {
    height: 60rpx;
    line-height: 60rpx;
    margin: 15rpx 30rpx 0 0;
    font-size: 28rpx;
}

.bus-coupontip {
    background: linear-gradient(
        90deg,
        rgb(255, 241, 241) 0%,
        rgb(255, 255, 255) 100%
    );
    border-radius: 16rpx 16rpx 16rpx 16rpx;
    height: 68rpx;
    margin: 16rpx 24rpx 0;
    font-size: 24rpx;
    padding: 0 24rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    position: relative;
    overflow: hidden;
}
.bus-coupontip:after {
    position: absolute;
    content: '';
    height: 4rpx;
    width: 100%;
    top: 0;
    left: 0;

    background: linear-gradient(
        -90deg,
        #fee3e3 0%,
        rgba(255, 131, 122, 0.3) 100%
    );
}
.bus-coupontip .red {
    color: #ff3b30;
}
.bus-coupontip .lottery-icon:before {
    font-size: inherit;
}
.bus-coupontip .lottery-icon {
    font-size: inherit;
    margin-right: 10rpx;
}

.bus-pass-list {
    padding-bottom: 0rpx;
}

.btn-modify {
    width: 140rpx;
}
.b-t-picker {
    padding-left: 30rpx;
    position: relative;
    line-height: 85rpx;
}
.b-b-insurance {
    position: relative;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
}
.b-b-row > view {
    border-style: solid;
    border-image: url(data:image/gif;base64,R0lGODlhBQAFAPABANra2v///yH5BAUHAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7)
        2 stretch;
    border-width: 0;
    border-top-width: 1rpx;
}
.b-b-row > view:first-child {
    border: none;
}

.b-t-picker .name {
    line-height: 40rpx;
    margin-top: 20rpx;
}
.b-t-picker .card {
    line-height: 40rpx;
}
.b-b-phonenum {
    border-top-width: 1px;
}
.b-b-phonenum input {
    height: 85rpx;
    padding: 0;
    display: block;
}
.b-b-icon,
.b-b-switch {
    text-align: right;
}

.b-b-icon {
    padding-right: 0;
    text-align: center;
    padding-left: 10rpx;
}
.b-b-icon image {
    width: 40rpx;
    height: 40rpx;
}

.bus-package {
    border-top-width: 1px;
}
.bus-ins-title {
    line-height: 40rpx;
    vertical-align: middle;
}
.b-b-passlist {
    padding-bottom: 10rpx;
}
.b-b-passlist .iconfont {
    padding: 0 0 0 36rpx;
    line-height: 100rpx;
}
.b-b-passlist .info {
    padding: 20rpx 20rpx 18rpx 0;
}
.b-b-passlist .card,
.b-t-picker .card {
    font-size: 24rpx;
    font-weight: 300;
    color: #666666;
    line-height: 40rpx;
}

.b-b-passlist .item {
    border-top-width: 1px;
}
.b-b-passlist .item:first-child {
    border-top-width: 0px;
}
.memberPasRow {
    padding-top: 20rpx;
    padding-left: 20rpx;
    padding-right: 20rpx;
}
.memberPasRow:last-child {
    padding-bottom: 20rpx;
}
.memberPasItem {
    flex: 1;
    height: 72rpx;
    color: #e5e5e5;
    font-size: 32rpx;
    /* border: 1px solid #e5e5e5; */
    border-radius: 8rpx;
    text-align: center;
    line-height: 72rpx;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    margin-right: 20rpx;
    background-color: #ffffff;
    max-width: 22%;
    margin-left: 0;
    position: relative;
}
.memberPasItem:after {
    display: none;
}

.memberPasItem text {
    color: #333333;
}
.memberPasItem:before {
    position: absolute;
    z-index: 1;
    content: '';
    left: 0;
    top: 0;
    height: 100%;
    width: 100%;
    border-style: solid;
    border-width: 1px;
    border-radius: 8rpx;
    box-sizing: border-box;
}
.memberPasItem.button-hover {
    filter: grayscale(0.3);
    background-color: #f4f4f4;
}

.memberPasItem .close:after,
.memberPasItem .extend:after {
    position: relative;
    border-radius: initial;
    content: '';
    display: inline-block;
    width: 9rpx;
    height: 9rpx;
    border-top-width: 3rpx;
    border-top-style: solid;
    border-right-width: 3rpx;
    border-right-style: solid;
    vertical-align: middle;
    margin: -9rpx 0 0 15rpx;
}
.memberPasItem .extend:after {
    -webkit-transform: rotate(135deg);
    transform: rotate(135deg);
}

.memberPasItem .close:after {
    -webkit-transform: rotate(-45deg);
    transform: rotate(-45deg);
}

.dash_b_border {
    border-bottom: 1px dashed #e5e5e5;
}

.memberPasRow .tag {
    position: absolute;
    z-index: 2;
    right: 0px;
    top: 0px;
    padding: 0 5rpx;
    font-size: 20rpx;
    line-height: initial;
    border-top-right-radius: 8rpx;
    border-bottom-left-radius: 8rpx;
    border-width: 1rpx;
    border-style: solid;
}

.b-b-passlist .label {
    background-color: #f0f0f0;
    color: #333333;
    font-size: 20rpx;
    padding: 5rpx 10rpx;
    margin-left: 14rpx;
    border-radius: 2px;
}
.bar-pay {
    background-color: #ffffff;
    /* line-height: 100rpx; */
    color: #fff;
    padding: 20rpx 30rpx;
    /* height: 100rpx; */
    bottom: 0;
    left: 0;
    right: 0;
    z-index: 99;
    box-shadow: 0 0 10px #ccc;
}
.slogan-container {
    margin-bottom: 0;
}
.bar-pay .btn-pay {
    height: 70rpx;
    line-height: 70rpx;
}

.bar-pay .torange {
    font-size: 32rpx;
}
.bar-pay .detail {
    color: #b2b2b2;
    font-size: 28rpx;
}

.b-b-beixuan {
    position: relative;
    background-color: #fff;
    line-height: 85rpx;
    padding-left: 30rpx;
}
.b-b-beixuan .note {
    color: #999;
}

.switch {
    position: absolute;
    top: 18rpx;
    right: 30rpx;
}
.bus-warmnotes {
    margin: 8rpx 15rpx 80rpx;
    padding: 10rpx;
    line-height: 36rpx;
    color: #999;
    font-size: 24rpx;
}
.price-detail {
    line-height: 85rpx;
    color: #666666;
    position: fixed;
    z-index: 98;
    bottom: 100rpx;
    background-color: #fff;
    width: 100%;
    font-weight: 300;
    animation: transIn 250ms linear forwards;
}

.bookTransIN {
    animation: transIn 250ms linear forwards;
}
@keyframes transIn {
    0% {
        transform: translate(0, 100%);
    }
    100% {
        transform: translate(0, 0);
    }
}

.price-detail .left {
    width: 45%;
}

.price-detail .cancelButton {
    background-color: white;
    margin-right: 10rpx;
    font-size: inherit;
}
.price-detail .cancelButton[disabled]:not([type]) {
    color: #999999;
    background-color: white;
}

.price-detail .cancelButton:after {
    display: none;
}

.price-detail > view:last-child {
    padding-bottom: 20rpx;
}
.price-detail view {
    border-top: 1px solid #f3f3f3;
}
.price-detail .margin {
    margin-left: 30rpx;
    padding-right: 30rpx;
}
.price-detail .discount {
    margin-left: 40rpx;
    position: relative;
    overflow: visible;
}

.price-detail .discount:after {
    content: '';
    position: absolute;
    width: 0rpx;
    border-left: 1rpx solid #f3f3f3;
    height: 60%;
    top: -10%;
    left: -30rpx;
}

.price-detail .discount:before {
    content: '';
    position: absolute;
    height: 0;
    border-top: 1rpx solid #f3f3f3;
    width: 20rpx;
    top: 50%;
    left: -30rpx;
}
.price-detail .nobr {
    border-top: none;
}
.icon-info {
    padding-left: 5rpx;
}
.invoice {
    font-size: 32rpx;
}

.flex-row {
    display: flex;
    flex-direction: row;
    align-items: center;
    position: relative;
}

.invoice .flex {
    border-top: 1px solid #f3f3f3;
    line-height: 85rpx;
    padding-left: 30rpx;
}
.invoice .title {
    width: 170rpx;
}
.invoice input {
    display: block;
    height: 85rpx;
}
/* css3 icon */
.pass-remove {
    position: relative;
}
.icon-remove {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
    background-color: #ffffff;
    border-radius: 40rpx;
    border: 2rpx solid #ff0000;
    position: absolute;
    top: 50%;
    left: 50%;
    margin: -20rpx 0 0 -20rpx;
}
.icon-remove::before {
    content: '';
    height: 2rpx;
    width: 20rpx;
    position: absolute;
    top: 19rpx;
    left: 11rpx;
    background-color: #ff0000;
}
.bus-desc {
    position: fixed;
    width: 100%;
    box-sizing: border-box;
    top: 0;
    left: 0;
    bottom: 0;
    overflow: auto;
    padding: 35rpx 35rpx 95rpx;
    color: #fff;
    z-index: 1011;
    line-height: 40rpx;
    -webkit-overflow-scrolling: touch;
}

.linethrough {
    text-decoration: line-through;
}
.line25 {
    line-height: 92rpx;
}
.disabled,
.cpinkgray {
    color: #999;
}
.mask {
    position: fixed;
    z-index: 10;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.6);
}

.mask-top {
    z-index: 1100;
}

.coupon-sec {
    color: #999;
    text-align: right;
    font-size: 26rpx;
}
.new-custom-msg {
    margin-top: 8rpx;
    padding: 10rpx 0 10rpx 20rpx;
    line-height: 40rpx;
    color: #999;
    font-size: 24rpx;
}
.right-arrow {
    padding-right: 35rpx;
}
.right-arrow:after {
    position: absolute;
    width: 0.3rem;
    height: 0.3rem;
    border-top: 0.1rem solid #ccc;
    border-right: 0.1rem solid #ccc;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    content: '';
    right: 0;
    top: 50%;
    margin-top: -0.15rem;
}

.arr-down {
    position: relative;
    width: 0.9rem;
}
.arr-down:after {
    width: 0.3rem;
    height: 0.3rem;
    border-right: 0.1rem solid #999;
    border-bottom: 0.1rem solid #999;
    -webkit-transform: rotate(45deg);
    transform: rotate(45deg);
    position: absolute;
    content: '';
    margin-top: -0.2rem;
    left: 0.3rem;
}

.rotate {
    transform: rotate(180deg);
    animation: rotate 200ms linear;
}

.warn_msg {
    padding-top: 0.4rem;
    color: red;
}

.tip-bot {
    position: fixed;
    left: 0;
    top: auto;
    right: 0;
    bottom: 0;
    z-index: 3002;
    transition: all ease-in-out 300ms;
    -webkit-transform: translate3d(0, 100%, 0);
    transform: translate3d(0, 100%, 0);
}
.tip-bot.active {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
}

.cui-hd {
    background: rgba(40, 56, 71, 0.95);
    color: #fff;
    font-size: 34rpx;
    display: table;
    width: 100%;
    padding: 16rpx 0;
    height: 56rpx;
    overflow: hidden;
    min-width: 640rpx;
}
.cui-hd .cell {
    display: table-cell;
    line-height: 60rpx;
    text-align: center;
}
.cui-hd .cancel-btn,
.cui-hd .ok-btn {
    width: 20%;
}
.cui-hd .cancel-btn {
    padding-left: 30rpx;
    text-align: left;
}
.cui-hd .ok-btn {
    padding-right: 30rpx;
    text-align: right;
}
.cui-bg {
    background: #f4f4f4;
    overflow: hidden;
    height: 880rpx;
}

.tip-addpas {
    overflow-y: auto;
    -webkit-overflow-scrolling: touch;
}

.b_tip {
    padding: 10rpx 30rpx;
    color: #999999;
    font-size: 24rpx;
}

.modal-hightlight {
    font-size: 40rpx;
    color: #ff5d13;
}

.gray-min {
    margin-left: 20rpx;
    font-size: 28rpx;
    font-weight: 300;
}

.bus-book-explain {
    margin-left: 20rpx;
    font-size: 28rpx;
    font-weight: 300;
    line-height: 32rpx;
    color: #999999;
    width: 40%;
    vertical-align: middle;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    word-break: break-all;
    overflow: hidden;
    max-height: 64rpx;
}

.icon-checked-round:before,
.icon-check-round:before {
    font-size: 44rpx;
    vertical-align: top;
}

.icon-check-triangle {
    font-size: 32rpx;
    height: 32rpx;
    line-height: 32rpx;
    position: absolute;
    right: 1rpx;
    bottom: 1rpx;
}

.x-list {
    transition: all 200ms ease-out;
}
.show-component {
    background-color: #ffffff;
    text-align: center;
    font-size: 26rpx;
    color: #333333;
    margin: 0 24rpx 50rpx;
    line-height: 80rpx;
    height: 80rpx;
    z-index: 10;
}

.show-component:after {
    color: #999999;
    position: relative;
    border-radius: initial;
    content: '';
    display: inline-block;
    width: 12rpx;
    height: 12rpx;
    border-top-width: 3rpx;
    border-top-style: solid;
    border-right-width: 3rpx;
    border-right-style: solid;
    vertical-align: middle;
    margin: -9rpx 0 0 15rpx;
    transform: rotate(135deg);
}


.bus-notice-wrap {
    width: auto;
    border-radius: 16rpx;
    margin: 12rpx 24rpx 0;
    background: #ffffff;
    z-index: 10;
}

@keyframes slideUp {
    from {
        bottom: -200rpx;
    }
    to {
        bottom: 0;
    }
}
