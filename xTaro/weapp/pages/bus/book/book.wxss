@keyframes scaleDraw {
    /*定义关键帧、scaleDrew关键帧名称*/
    0% {
        transform: scale(1);
    }
    40% {
        transform: scale(1);
    }
    55% {
        transform: scale(1.3);
    }
    70% {
        transform: scale(1);
    }
    85% {
        transform: scale(1.3);
    }
    100% {
        transform: scale(1);
    }
}

@keyframes pageTransOut {
    0% {
        transform: translate(0, 0);
    }
    100% {
        transform: translate(0, 100%);
    }
}

@keyframes pageTransIn {
    0% {
        transform: translate(0, 100%);
    }
    100% {
        transform: translate(0, 0);
    }
}

@keyframes slideUp {
    from {
        bottom: -200rpx;
    }
    to {
        bottom: 0;
    }
}

