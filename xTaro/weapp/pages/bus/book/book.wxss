.b-switch {
    -webkit-appearance: none;
    appearance: none;
    position: relative;
    width: 96rpx;
    height: 56rpx;
    border: 1px solid #dfdfdf;
    outline: 0;
    border-radius: 28rpx;
    box-sizing: border-box;
    background: #dfdfdf;
    display: inline-block;
}

.b-switch:before,
.b-switch:after {
    content: ' ';
    position: absolute;
    top: 0;
    left: 0;
    height: 52rpx;
    border-radius: 52rpx;
    -webkit-transition: -webkit-transform 0.3s;
    transition: transform 0.3s;
}

