.button-noborder {
    margin-left: 0;
    margin-right: 0;
    margin: 0;
    padding: 0;
    padding-left: 0;
    background-color: initial;
    border-radius: 0;
    line-height: inherit;
    font-size: inherit;
    height: initial;
}

.tips_top {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    text-align: center;
    font-size: 15px;
    line-height: 1.5;
    padding: 5px 10px;
    color: #fff;
    background-color: #111;
    opacity: 0.7;
    z-index: 101;
}

.button-noborder::after {
    border: 0 solid transparent;
}

.button-hover {
    filter: brightness(0.8) grayscale(0.3);
}

