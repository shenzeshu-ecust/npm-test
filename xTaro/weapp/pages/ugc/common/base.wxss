/**app.wxss**/

page {
  position: relative;
  display: flex;
  flex-direction: column;
  background-color:#efefef;
  height: 100%;
  font-size: 14px;
  color: #666;
}

view{
  overflow: visible; /* 不复写，page设置字体大小后view 会继承这个字体最小高度，text大于这个高度后会触发overflow-纵向滚卷 */
  padding: 0;
  margin: 0;
  /* line-height: 1.2; */
}

/** 页面遮罩 **/
.mask {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 1000;
  background: rgba(0,0,0,.5);
}

/** 单行省略号 **/
.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

/** 列表页分页loading **/
.loading-more {
  padding: 20rpx 0;
  color: #666;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 13px;
}

.loading-more-icon {
  margin-right: 20rpx;
}

/** 评分icon **/
.score-box {
  position: relative;
}
.score-text{
  display: flex;
}
.score-bg{
  display: flex;
}
.score-fg {
  position: absolute;
  top: 0;
  left: 0;
  display: flex;
}
.gs-icon-score{
  color: #DDD;
  margin-right: 4rpx;
}
.gs-icon-score-half{
  color: #DDD;
  margin-right: 4rpx;
}
.gs-icon-last{
  margin-right: 0 !important;
}
.score-active {
  color: #FF6633;
}
.default_image_bg {
  background: #f0f1f5 url(https://pages.c-ctrip.com/you/common/default.png) no-repeat center;
}

/** 大图预览模板样式 **/
.images-swiper-box {
  z-index: 10000;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #000;
  display: flex;
}

.images-swiper {
  flex: 1;
  width: 100%;
  height: 100%;
}

.images-swiper-item {
  flex: 1;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.images-swiper-image {
  width: 100%;
}

.images-swiper-index-bar {
  position: absolute;
  width: 100%;
  text-align: center;
  left: 0;
  bottom: 80rpx;
  font-size: 16px;
  font-weight: bold;
  color: #FFF;
}

/** Font Icon **/
@font-face {
  font-family: "gs-iconfont";
  src: url(data:application/octet-stream;base64,) format('truetype');
}
.gs-iconfont {
  font-family: "gs-iconfont";
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}
