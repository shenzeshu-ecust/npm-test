/* animation */
@keyframes enlarge {
    0%{
        transform: scale(0.4);
    }
    25%{
        transform: scale(0.6);
    }
    50%{
        transform: scale(1);
    }
    75%{
        transform: scale(1.2);
    }
    100%{
        transform: scale(1);
    }
}

.animation {
    animation: enlarge 0.8s linear 0s 1 normal;
}
.loop {
    animation: loop 0.8s ease 0.8s infinite normal;
}
