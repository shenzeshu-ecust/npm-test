/* 房型Start */
.transz0 { transform: translateZ(0); }
.dt-room { background-color: #fff; position: relative; }
.dt-room .no-room { color: #808080; padding: 30rpx; font-size: 28rpx; }

.num-base {
    white-space: unset;
    word-break: break-all;
    display: inline-block;
}

.dt-room .space { padding: 16rpx 0 16rpx 24rpx; }
.dt-room .space2 { padding: 0 20rpx 0 0; }
.dt-room .more-room-btn {
    margin-top: 24rpx;
    padding-bottom: 24rpx;
}

.dt-room .room-item { overflow: hidden; padding: 24rpx; border-top: 1rpx solid #ebebeb; }
.friend_share_room .room-item, .dt-room .room-item.guess-like-room, .dt-room .m-room:first-child .room-item {
    border: unset;
}
/* room-img */
.dt-room .room-img {
    float: left;
    position: relative;
    width: 160rpx;
    margin-right: 16rpx;
    background: url(https://pic.c-ctrip.com/h5/hotel/hotel-loading-default.png) center center no-repeat;
    background-size: 58rpx 58rpx;
}
.dt-room .room-img image { width: 160rpx; height: 160rpx; display: block;border-radius: var(--img-border); }
.room-top {
    position: sticky;
    -webkit-position: sticky;
    top: 0;
    z-index: 10;
    background-color: #fff;
    transition: top 200ms linear;
    will-change:top;
    box-shadow: 0 4px 16px 1px rgba(0,0,0,0.04);
}
.no-shadow {
    box-shadow: none
}
.dt-room .room-img .room-like {
    position: absolute;
    top: 0;
    border-radius: 8rpx 0;
    padding: 7rpx 8rpx;
    background: var(--like-bg);
}
.dt-room .room-like {
    font-size:20rpx;
    font-weight:500;
    color: #fff;
    padding: 8rpx 12rpx;
    background:linear-gradient(to right,#FF8100 0%,#FF7100 100%);
    border-radius: 4rpx;
}
.dt-room .room-img .num{
    height:24rpx;
    line-height: 24rpx;
    text-align:center;
    position: absolute;
    top:126rpx; right:10rpx;
    width: 24rpx;
    background-color: #fff;
    border-radius: 14rpx;
    color: #333;
    font-size: 16rpx;
    padding: 0;
    font-weight: 500;
}
/* card-txt */
.dt-room .multi {
    color: #666;
}
.dt-room .multi .h3 { font-size: var(--h3-fs); color:var(--h3-color); font-weight: 500; margin-bottom: 12rpx; line-height: 32rpx;}
.dt-room .room-item .txt-item {
    line-height: 32rpx;
    margin-bottom: 12rpx;
    font-size: 24rpx;
}
.dt-room .room-item .baseinfo-item {
    margin-right: 10rpx;
    white-space: nowrap;
}

.dt-room .gift-icon, .dt-room .pac-icon {
    font-size: 20rpx;
    margin-right: 8rpx;
    display: inline-block;
}

.dt-room .gift-icon {
    background: #fff1e6;
    color: #f70;
    padding: 2rpx;
    line-height: 1;
    border-radius: 4rpx;
}
.dt-room .pac-icon {
    background: #FF7700;
    color: #fff;
    width: 26rpx;
    height: 26rpx;
    border-radius: 50%;
    line-height: 26rpx;
    flex-shrink: 0;
    text-align: center;
}

.dt-room .multi .s { font-size: 24rpx; }
.dt-room .multi .s.l-name{ line-height: 32rpx; margin-bottom: 15rpx; }
/* 标签 */
.dt-room .tags-wrap {overflow: hidden;max-height: 34rpx}
.dt-room .tags-wrap.nowrap {max-height: none}
.dt-room .tags-wrap .ftag-item {
    position: relative;
    display: inline-block;
    height: 30rpx;
    line-height: 28rpx;
    vertical-align: 6rpx;
    font-size: 20rpx;
    margin-right: 4rpx;
    padding: 0 4rpx;
    border-radius: 4rpx;
    color: #5678a8;
    border: 2rpx solid #bbc9dc;
    box-sizing: border-box;
}
.dt-room .star.multi { text-align: right; padding: 0 20rpx 0 10rpx; box-sizing: border-box; line-height: 1; align-items: flex-end; }
.dt-room .cell .multi.pr0 {padding-right: 0;}
.dt-room .cell-v .star.multi {padding: 0;}
.dt-room .multi .price { font-size: 40rpx; line-height: 40rpx; font-weight: 700; }
.dt-room .multi .price-tax { font-size: 22rpx; color: #f40; }
.dt-room .multi .price-star { font-size: 24rpx; color: #666; padding: 12rpx 0 0 6rpx; }
.dt-room .multi .price-star.sub-price-star { padding: 0 0 0 6rpx; }
.dt-room .multi .surplus-room { text-align: center; color: #ff3950; font-size: 20rpx; }
.dt-room .multi .tags-wrap { margin-bottom: 16rpx; }
.multi, .btn { justify-content: center; -webkit-box-orient: vertical; flex-direction: column; display: flex; }

.dt-room .base-title {
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.dt-room .base-room-price {align-self: flex-end;}

/* 子房型 */
.dt-room .sub { padding: 0 24rpx; }
.dt-room .sub .room-item { padding: 24rpx 0; }
.dt-room .sub .room-item:first-child { border: unset; }
.dt-room .sub .like {font-size: 18rpx;height: 28rpx;line-height: 28rpx;padding: 0 8rpx;display: inline-block;vertical-align: 6rpx;margin-right: 10rpx;}
.dt-room .sub .item { margin-left: 30rpx; word-break: break-all; }
.dt-room .sub .c { position: relative; }
/* .dt-room .sub .c.title-bar { display: flex; justify-content: space-between; align-items: center;} */
.dt-room .sub .subroom-title {
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
    font-weight: 500;
    margin-bottom: 12rpx;
}
.dt-room .sub .guest-desc { margin-left: 15rpx; }
.dt-room .sub .space { padding: 20rpx 24rpx 20rpx 0; }
.dt-room .sub .space2 { padding-bottom: 20rpx; }
.dt-room .sub .s { color: #666; font-size: 24rpx;}
.dt-room .dt-kf { color: #666; font-size: 24rpx; }

.dt-room .head-tag {font-size: 18rpx;
    height: 24rpx;line-height:24rpx;padding: 0 8rpx;display: inline-block;vertical-align: 6rpx;
    
    border-radius: 4rpx;border:2rpx solid;}
.dt-room .k{ margin-left: 10rpx;}
.dt-room .sub .tags-wrap { margin-bottom: 16rpx; }


.dt-room .orange-text {font-size: 20rpx; color: #f70;}
.dt-room .flash-sale {
    display: inline-block;
    color: #fff;
    height: unset;
    font-size: 20rpx;
    padding: 4rpx 8rpx;
    line-height: 1;
    border-radius: 4rpx;
    background-image: linear-gradient(to right, #FF7F5C 6%, #FB4F21 100%);
}
.room-h1-tag {
    background-image: linear-gradient(90deg, #FF9D0A 0%, #f70 100%);
    border-radius: 4rpx;
    font-size: 18rpx;
    color: #fff;
    padding: 5rpx 6rpx;
    vertical-align: 6rpx;
    display: inline-block;
    margin-right: 10rpx;
    line-height: 1;
}
.sub .room-like {
    border: unset;
    background-image: var(--like-bg);
}

/* 支付按钮 */
button:after {display: none}
.booking-btn {
    width: 80rpx;
    height: 84rpx;
    background-image: var(--booking-btn-bg);
    border-radius: 8rpx;
    display: flex;
    flex-direction: column;
    padding: 2rpx !important;
    box-sizing: border-box;
    overflow: hidden;
    text-align: center;
}
.booking-btn.panicbuy-btn {
    background-image: var(--panic-buy-btn-bg);
}
.booking-btn .btn-t {
    line-height: 53rpx;
    font-size: 36rpx;
    font-weight: 500;
    color: #fff;
    flex: 1;
}
.booking-btn .btn-t.fs22 { font-size: 22rpx; }
.booking-btn .btn-t.fs28 { font-size: 28rpx; }
.booking-btn .btn-t.r-cl-btn {
    background-color: #ff5500;
}
.booking-btn .btn-t.r-cl-btn-txt {
    font-size: 28rpx;
