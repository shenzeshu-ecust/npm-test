    font-weight: 400;
}
/*订改抢*/
.panicbuy-text {
    font-size:20rpx;
    color: #F5190A;
    text-align: center;
    line-height: 1;
    margin-bottom: 10rpx;
}

/* 满房申请房型，预订按钮上方文案样式 */
.full-room {
    max-width: 95rpx;
    line-height: 24rpx;
}

.desc-pop {
    position: absolute;
    color: #fff;
    padding: 2rpx 8rpx;
    font-size: 20rpx;
    height: 28rpx;
    background: #000;
    opacity: 0.6;
    border-radius: 16rpx 16rpx 0 16rpx;
    line-height: 28rpx;
    top: -8rpx;
    right: 20rpx;
}

.friend-share-room {
    background: linear-gradient(90deg, rgb(255, 247, 239) 0%, rgb(255, 237, 222) 50.21%, rgb(255, 247, 239) 100%);
    padding: 0 0 16rpx;
}
.friend-share-room .friend-share-title {
    display: block;
    width: 100%;
    height: 64rpx;
}
/* 钟点房 */
.hour-room .room-title, .store-box .hd {
    font-size: 32rpx;
    color: #333;
    line-height: 32rpx;
    font-weight: 400;
    padding: 30rpx 0 0 30rpx;
}
.hour-room .item:not(:last-child) {
    border-width: 0 0 1rpx 0;
    border-style: solid;
    border-color: #ddd;
    border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
}
.hour-room .hour-info {
    font-size: 24rpx;
    color: #333;
    font-weight: 500;
}
.hour-room .room-item:nth-child(2) {
    border-top: unset;
}
.hour-room .hour-duration {
    font-size: 24rpx;
    color: #333;
    line-height: 24rpx;
    font-weight: 400;
}
/* 房型End */

/* 入离日期 */
.room-mod {
    position: relative;
}
.dt-date {
    background-color: #fff;
}
.dt-date-wifi {
    border-radius: 20rpx 20rpx 0 0;
    width: 100%;
}
.dt-date-wifi .cl-nights {
    font-weight: 500;
}
.dt-date .date-box {
    height: 80rpx;
    border-radius: 8rpx;
    margin: 0 24rpx;
    color: #333;
    font-size: 28rpx;
    display: flex;
    align-items: center;
}
.dt-date .date-box-wifi {
    height: 80rpx;
    margin: 0 32rpx;
