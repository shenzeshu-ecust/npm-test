}
.dt-date .date-box-wifi .cl-time-line-wrapper {
    margin: 0 20rpx;
}
.dt-date .date-box-wifi .cl-date-line {
    height: 32rpx;
    width: 2rpx;
    overflow: hidden;
    background-color: #979797;
}
.dt-date .date-box-wifi .time-line-wrapper {
    margin: 0 16rpx;
}
.dt-date .date-box-wifi .date-line {
    height: 2rpx;
    width: 28rpx;
    overflow: hidden;
    background-color: #979797;
}
.dt-date .date-box .times { font-size: 32rpx; padding-right: 10rpx; font-weight: 500; }
.dt-date .date-box .cust { font-weight: normal; font-size: 24rpx; color: #666; }
.dt-date .date-box .cell-end { padding-left: 24rpx; }
.dt-date .date-box .time-line-wrapper {
    margin: 0 20rpx;
}
.dt-date .date-box .date-line {
