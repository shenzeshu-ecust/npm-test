/* 筛选 */
.dt-filter {
    display: flex;
    font-size: 24rpx;
    padding: 0 24rpx;
    background: #fff;
}
.dt-filter .qs-box {
    flex: 1;
    display: flex;
    flex-wrap: nowrap;
    overflow-x: auto;
    -webkit-overflow-scrolling: touch;
}
.dt-filter .filter-btn {
    height: auto;
    width: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
    box-shadow: -3rpx 5rpx 10rpx rgba(0, 0, 0, .1);
    position: relative;
}
.dt-filter .qs-box .qs-item {
    margin: 16rpx 10rpx 16rpx 0;
    padding: 12rpx 20rpx;
    display: inline-block;
    text-align: center;
    border-radius: 8rpx;
    white-space: nowrap;
    font-weight: 300;
    background-color: #f5f5fa;
}
.dt-filter .filter-btn .arrow-down {
    display: inline;
    font-size: 30rpx;

}
.dt-filter .filter-btn .filtered-sign {
    position: absolute;
    color: red;
    top: 9px;
    right:11px;
    font-size: 27rpx;
    font-weight: 700;
}
/* 筛选浮层 */
.filter-layer {
    height: 65%;
    padding: 24rpx;
    box-sizing: border-box;
}
.filter-layer .content {
    height: calc(100% - 74rpx);
}
.filter-layer .filter-tit {
    font-size: 32rpx;
    font-weight: 700;
}
.filter-layer .filter-box {
    display: flex;
    margin: 30rpx 0;
    flex-wrap: wrap;
}
.filter-layer .filter-box .filter-item {
    flex: 0 0 32%;
    margin: 0 2% 2% 0;
    font-size: 26rpx;
    text-align: center;
    border-radius: 8rpx;
    background-color: #f5f5fa;
    height: 64rpx;
    box-sizing: border-box;
    line-height: 64rpx;
}
.filter-layer .filter-box .filter-item:nth-child(3n+3) {
    margin-right: 0;
}
.filter-layer .filter-box .filter-s {
    background: #e7f3ff url(https://pages.c-ctrip.com/hotels/wechat/img/filter-arrow.png) right bottom no-repeat;
    background-size: 28rpx;
}
.filter-layer .filter-box .filter-s, .qs-box .filter-s {
    color: #0086F6;
}
.filter-layer .btns {
    display: flex;
    padding: 16rpx 24rpx;
    border-top: 4rpx solid #f1f1f1;
    border-image: linear-gradient(rgba(0,0,0,0.04), rgba(0,0,0,0.00)) 8;
    background-color: #fff;
}
.filter-layer .btns .btn {
    font-size: 30rpx;
    padding: 14rpx;
    border-radius: 8rpx;
    text-align: center;
    flex: 1;
}
.filter-layer .btns .reset {
    background-color: #fff;
    color: #333;
    border: 2rpx solid #ccc;
}
.filter-layer .btns .confirm {
    background-color: #0086f6;
    color: #fff;
    margin-left: 24rpx;
}

