/* 高星落地页客房送物 */
.hs-guest-service {
    --container-bg: linear-gradient(to bottom, rgb(52, 52, 52) 0%, rgb(94, 94, 94) 100%);
    --guest-img: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/high-bg.png');
    --guest-body-top: -12rpx;
    --guest-body-bg: linear-gradient(to bottom, rgb(52, 52, 52) 0%, rgb(94, 94, 94) 80%, rgb(94, 94, 94, 0) 100%);
    --guest-body-padding: 8rpx 21rpx 12rpx 35rpx;
    --guest-service: 1rpx solid#898989;
    --guest-begin: #fff;
    --guest-detail: #f4f4f4;
    --guest-btn-bg: #ffdfa1;
    --guest-btn-color: #462a03;
    --guest-facility-name: #f4f4f4;
}

/* 低星落地页客房送物 */
.ls-guest-service {
    --container-bg: #fff;
    --guest-img: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/low-bg.png');
    --guest-body-top: -16rpx;
    --guest-body-bg: #fff;
    --guest-body-padding: 8rpx 21rpx 8rpx 35rpx;
    --guest-service: 1rpx solid #d8d8d8;
    --guest-begin: #333;
    --guest-detail: #333;
    --guest-btn-bg: #0086f6;
    --guest-btn-color: #fff;
    --guest-facility-name: #000;
}

/* 高星优惠券角标 */
.hs-coupon-bar {
    --no-coupon-bg: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/no_coupon1.png') top left/auto 72rpx no-repeat;
    --coupon-bar-height: 72rpx;
    --old-coupon-bg: url(https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/old_coupon1.png) top left/auto 72rpx no-repeat;
    --new-coupon-bg: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/new_coupon1.png');
}

/* 前台码优惠券角标 */
.fd-coupon-bar {
    --no-coupon-bg: url(https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/no_coupon_jh.png) top left/auto 80rpx no-repeat;
    --coupon-bar-height: 80rpx;
    --old-coupon-bg: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/old_coupon2.png') top left/auto 72rpx no-repeat;
    --new-coupon-bg: url('https://pages.c-ctrip.com/hotels/wechat/market/smz/aggregate/new_coupon2.png');
}

