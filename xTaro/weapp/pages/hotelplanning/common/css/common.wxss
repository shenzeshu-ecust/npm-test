/* 逐渐显示动画 */
@keyframes showIn {
	0% {
		opacity: 0;
	}
	100% {
		opacity: 1;
	}
}
.show-animation {
	animation: showIn 350ms linear both;
}
/* 逐渐消失动画 */
@keyframes disappear {
	0% {
		opacity: 1;
	}
	100% {
		opacity: 0;
	}
}
.disappear-animation {
	animation: disappear 350ms linear both;
}
