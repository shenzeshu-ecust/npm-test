/*pop-up*/
.global-layer {
	position: fixed;
	z-index: 1001;
	bottom: 0;
	left: 0;
	right: 0;
	border-radius: 16rpx 16rpx 0 0;
	background-color: #fff;
	animation-duration: .2s;
    animation-fill-mode: both;
    animation-timing-function: ease-out;
    animation-name: fadeInBottom;
    transform-origin: center bottom;
}
@keyframes fadeInBottom {
	0% {
	  transform: scale(1,0);
	}
	30% {
	  transform: scale(1,0.5);
	}
	100% {
	  transform: scale(1,1);
	}
}
.global-layer .thead {
	color: #333;
	position: relative;
}
.global-layer .thead .tit {
	font-size: 34rpx;
	height: 88rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	font-weight: 700;
}
.global-layer .thead .tit .sub {
	font-size: 24rpx;
	color: #666;
	font-weight: 300;
}
.global-layer .thead .wechat-font-close,
.global-layer .thead .wechat-font-close2 {
	font-size: 32rpx;
	width: 88rpx;
	height: 88rpx;
	display: flex;
	justify-content: center;
	align-items: center;
	position: absolute;
	right: 0;
	top: 0;
}
.global-layer .tbody {
	color: #333;
	font-size: 28rpx;
}
.global-layer .des-list {
	min-height: 600rpx;
	padding: 32rpx;
}
.des-list .dd,
.des-list .tl {
	display: flex;
	padding-bottom: 20rpx;
}
.des-list .del {
	font-size: 28rpx;
}
.des-list .tl .first-tit {
	order: 1;
}
.des-list .tl .last-tit {
	order: 3;
}
.des-list .tl:after {
	content: "";
	height: 20rpx;
	margin: 0 24rpx;
	border-bottom: 1rpx #ccc dashed;
	flex: 1;
	order: 2;
}
.global-layer .tfoot {
	display: flex;
	padding: 0 48rpx;
}
.global-layer .tfoot .gbtn {
	font-size: 32rpx;
	padding: 25rpx;
	border-radius: 100rpx;
	text-align: center;
	margin: 32rpx 0 60rpx;
	flex: 1;
}
.global-layer .tfoot .gbtn:nth-of-type(2) {
	margin-left: 24rpx;
}
.global-layer .tfoot .cancel {
	background-color: #fff;
	color: #333;
	border: 2rpx solid #babdc1;
}
.global-layer .tfoot .confirm {
	background-color: #ff6913;
	color: #fff;
}

.global-pd-confirm-btn {
	width: 686rpx;
