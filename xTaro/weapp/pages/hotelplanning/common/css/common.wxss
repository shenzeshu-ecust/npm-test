/*标签统一改版*/
.tags-wrap .wxtag {
	color: #fff;
	font-size: 18rpx;
	background-color: var(--wxtag-bg-color);
	padding: 0 8rpx;
	border-radius: 4rpx;
	margin-right: 8rpx;
	height: 28rpx;
	line-height: 28rpx;
	display: inline-block;
}
.tags-wrap .ctag,
.tags-wrap .c2tag {
	font-size: 18rpx;
	padding: 0 7rpx;
	height: 28rpx;
	line-height: 28rpx;
	position: relative;
	display: inline-block;
}
.tags-line .catgs {
	display: inline-block;
	margin: 1rpx 0rpx;
}
.tags-line .ctag:after {
	content: "";
	position: absolute;
	width: 2rpx;
	height: 16rpx;
	top: 7rpx;
	right: 0;
}
.tags-wrap .ctag {
	color: var(--ctag-color);
	background-color: var(--ctag-bg-color);
	border: var(--ctag-border);
	margin-right: var(--ctag-mr);
}
.tags-wrap .ctag + .c2tag:nth-of-type(-n + 1) {
	margin-left: 8rpx;
}
.tags-wrap .c2tag {
	color: #2681ff;
	background-color: rgba(195, 220, 255, 0.4);
}
.tags-wrap .ctag:after {
	background-color: rgba(255, 77, 29, 0.2);
}
.tags-wrap .c2tag:after {
	background-color: rgba(46, 134, 255, 0.4);
}
.tags-line .ctag:first-of-type {
	border-radius: 4rpx 0 0 4rpx;
}
.tags-line .ctag:last-of-type {
	border-radius: 0 4rpx 4rpx 0;
}
.tags-line .ctag:last-of-type:after {
	display: none;
}
.tags-line .ctag:first-of-type:last-of-type {
	border-radius: 4rpx;
}
.tags-line .tag-after-none:after {
	display: none;
}

