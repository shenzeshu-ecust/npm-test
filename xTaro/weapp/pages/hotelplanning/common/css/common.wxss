/* mask */
.cmask {
	position: fixed;
	z-index: 1000;
	background-color: rgba(0, 0, 0, 0.7);
	top: 0;
	bottom: 0;
	right: 0;
	left: 0;
}
.cmask-popbox {
	position: fixed;
	z-index: 1001;
	top: 50%;
	left: 50%;
	transform: translate3d(-50%, -50%, 0);
}
.hidden {
	display: none !important;
}

/* z-index */
.z-index0 {
	z-index: 0;
}

.z-index1 {
	z-index: 1;
}

