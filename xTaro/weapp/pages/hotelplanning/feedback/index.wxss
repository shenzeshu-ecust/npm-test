.feedback-wrapper {
    position: relative;
    background-color: #f8f8f8;
}

.feedback-content {
    height: 625rpx;
    background: #fff;
    border-radius: 16rpx;
    margin: 24rpx;
    padding: 32rpx;
}

.feedback-title {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
}

.feedback-subtitle {
    color: #111;
    font-size: 22rpx;
    margin-top: 16rpx;
}

.feedback-input {
    width: 100%;
    border-top: 2rpx solid #e6e6e6;
    margin-top: 32rpx;
    padding: 32rpx 0;
    font-size: 28rpx;
    color: #111;
}

.room-content {
    display: flex;
    align-items: center;
    margin: 24rpx;
    background-color: #fff;
    padding: 24rpx 42rpx;
    border-radius: 16rpx;
}

.room-tip {
    color: #333;
    font-size: 30rpx;
    font-weight: 500;
    margin-right: 32rpx;
}

.room-input {
    font-size: 28rpx;
    color: #111;
}

.submit-content {
    width: 100%;
    box-sizing: border-box;
    position: fixed;
    left: 0;
    bottom: 0;
    padding: 24rpx 24rpx 92rpx 24rpx;
    background-color: #fff;
}
.submit-button {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 34rpx;
    font-weight: 500;
    border-radius: 12rpx;
    color: #fff;
}

.submit-button-grey {
    background-color: #888;
}

.submit-button-active {
    background-color: #0066f6;
}