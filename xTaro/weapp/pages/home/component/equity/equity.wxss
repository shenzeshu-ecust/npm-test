.equity-wrap {
  position: fixed;
  /* top: 60px; */
  left: 24rpx;
  right: 24rpx;
  /* height: 322rpx; */
  overflow: hidden;
  z-index: 52;
  border-radius: 24rpx;
  box-shadow:0 6rpx 20rpx 2rpx rgba(0,0,0,0.11);
}

.equity-wrap.hidden {
  display: none;
}

.equity-box{
  height: 100%;
  width: 100%;
  border-radius: 24rpx;
  overflow: hidden;
}

.equity-info {
  display: flex;
  align-items: center;
  margin: 32rpx 0 0 36rpx;
}

.equity-img {
  height: 100rpx;
  width: 100rpx;
  background-color: #FFF;
  border-radius: 50%;
  box-sizing: border-box;
  overflow: hidden;
}

.equity-img-detail {
  height: 100%;
  width: 100%;
}

.equity-user {
  margin-left: 16rpx;
}

.equity-name {
  display: flex;
  align-items: center;
}

.equity-name-txt {
  max-width: 255rpx;
  font-size: 34rpx;
  line-height: 42rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.equity-tag {
  margin-left: 12rpx;
  width: 118rpx;
  height: 28rpx;
}

.equity-progress {
  position: relative;
  width: 378rpx;
  height: 4rpx;
  border-radius: 2rpx;
  background-color: #FFF;
  margin-top: 4rpx;
  overflow: hidden;
}

.equity-progress-completed {
  height: 100%;
  border-radius: 2rpx;
}

.equity-grade {
  font-size: 20rpx;
  line-height: 22rpx;
  margin-top: 8rpx;
}

.equity-discount {
  display: flex;
  margin-left: 44rpx;
  margin-top: 18rpx;
}

.equity-discount-hotel {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  width: 234rpx;
}

.equity-discount-coupon {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  width: 194rpx;
}

.equity-discount-integral {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
}

.equity-discount-txt {
  font-size: 20rpx;
  line-height: 22rpx;
}

.equity-discount-detail {
  display: flex;
  align-items: baseline;
  margin-top: 10rpx;
}

.equity-discount-num {
  font-size: 56rpx;
  line-height: 56rpx;
  font-weight: bold;
}

.equity-discount-unit {
  font-size: 20rpx;
  line-height: 22rpx;
  margin-left: 2rpx;
}

.equity-discount-sort {
  font-size: 20rpx;
  line-height: 28rpx;
  padding: 0 6rpx;
  margin-left: 6rpx;
  border-radius: 4rpx;
}

.equity-project {
  display: flex;
  align-items: center;
  margin: 20rpx 0 32rpx 40rpx;
}

.equity-project-item {
  display: flex;
  align-items: center;
}

.equity-project-item + .equity-project-item {
  margin-left: 20rpx;
}

.equity-project-img {
  height: 30rpx;
  width: 30rpx;
  border-radius: 100%;
  overflow: hidden;
}

.equity-project-txt {
  font-size: 26rpx;
  line-height: 34rpx;
  margin-left: 4rpx;
}

.equity-project-more {
  font-size: 26rpx;
  line-height: 34rpx;
  margin-left: 42rpx;
}

.equity-message {
  display: none;
  font-size: 40rpx;
  line-height: 48rpx;
  font-weight: bold;
  margin: 20rpx 0 36rpx 40rpx
}


/* 普通会员 */
.ordinary .equity-box{
  background: url(//pic.c-ctrip.com/platform/h5/mini_programe/equity/ordinary/bg.png) 0 0 no-repeat;
  background-size: cover;
}

.ordinary .equity-tag {
  background: url(//pic.c-ctrip.com/platform/h5/mini_programe/equity/ordinary/tag.png) 0 0 no-repeat;
  background-size: contain;
}

.ordinary .equity-img {
  border: 2rpx solid #7AC9FF;
}

.ordinary .equity-name-txt,
.ordinary .equity-grade, 
.ordinary .equity-message{
  color: #0086F6;
}

.ordinary .equity-progress-completed {
  background-color: #0086F6;
}

.ordinary .equity-discount,
.ordinary .equity-project {
  display: none;
}

.ordinary .equity-message {
  display: block;
}


