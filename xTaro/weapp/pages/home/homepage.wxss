page {
  font-family: -apple-system, "Heiti SC", "黑体-简", Helvetica, 'Hiragino Sans GB W3', 'Microsoft YaHei', arial, sans-serif;
  background-color: #F8F8F8;
  --height: 40px;
  --right: 190rpx;
  overflow: hidden;
}

::-webkit-scrollbar{
  display:none;
  width: 0;
  height: 0;
  color: transparent;
}

.body-container {
  position: absolute;
  height: 100%;
}

/* 临时宫格方案样式 start */
.temp-swiper-wrap {
  padding-top: 16rpx;
  padding-bottom: 24rpx;
  background-color: #FFF;
}
.temp-bu-wrap {
  width: 702rpx;
  margin: 0 auto;
  overflow: hidden;
  border-top-left-radius: 16rpx;
  border-top-right-radius: 16rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-wrap: wrap;
}
.temp-bu-more {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 702rpx;
  height: 88rpx;
  background: #F8FAFD;
  margin: 0 auto;
  overflow: hidden;
  border-bottom-left-radius: 16rpx;
  border-bottom-right-radius: 16rpx;
}
.temp-bu-more-img {
  flex: none;
  height: 40rpx;
  width: 190rpx;
}
.temp-bu-more-txt {
  flex: none;
  font-size: 24rpx;
  line-height: 30rpx;
  color: #333;
  margin: 0 2rpx 0 16rpx;
}
.temp-bu-more-arrow {
  flex: none;
  height: 24rpx;
  width: 24rpx;
}
/* 临时宫格方案样式 end */

/* 自定义头部 */
.home-navigation-bar {
  overflow: hidden;
  background-color: #FFF;
}

.home-navigation-bar .android {
  --height: 44px;
  --right: 222rpx
}

.home-navigation-bar__inner {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 51;
  height: var(--height);
  display: flex;
  align-items: center;
  padding-right: var(--right);
  width: calc(100% - var(--right));
  background-color: #FFF;
}

.home-navigation-bar__inner .home-navigation-bar__center {
  font-size: 17px;
  text-align: center;
  position: relative;
  height: 32px;
  flex: 1;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center
}

.home-navigation-bar__placeholder {
  height: var(--height);
  background: #F8F8F8;
  position: relative;
  z-index: 50
}

.signin-box {
  flex: none;
  width: 64rpx;
  height: 64rpx;
  margin-left: 32rpx;
  margin-right: 16rpx;
}

.signin-img {
  height: 100%;
  width: 100%;
}

/* 中间通栏广告 */
.slider-wrap {
  display: flex;
  height: 176rpx;
  width: 100%;
  background-image: linear-gradient(to bottom, #FFF, #F8F8F8);
}

.slider-show {
  position: relative;
  width: 100%;
  height: 176rpx;
  margin: 0 24rpx;
  background: url(http://pic.c-ctrip.com/platform/h5/mini_programe/new_index_top.png) top center no-repeat;
  background-size: 702rpx 176rpx;
  border-radius: 16rpx;
  overflow: hidden;
  z-index: 1;
}

.swiper-wrap {
  padding-top: 16rpx;
  background-color: #FFF;
}

.bu-list-content {
  display: flex;
  flex-direction: row;
  margin: 0 auto;
  overflow: hidden;
  position: relative;
  width: 702rpx;
  border-radius: 16rpx;
  transition: height .3s;
}

/* 首屏宫格 */
.bu-list_item {
  position: relative;
  float: left;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  text-align: center;
  height: 116rpx;
  width: 138rpx;
  margin-bottom: 2rpx;
  /* margin-right: 2rpx; */
}

.bu-list_item:nth-of-type(5n+2),
.bu-list_item:nth-of-type(5n+4) {
  width: 140rpx;
}
/* 
.bu-list_item:nth-of-type(5n) {
  margin-right: 0;
} */

.bu-list_item:nth-of-type(-n+5) {
  margin-bottom: 0;
}

.bu-list_item:nth-of-type(1) {
  background-image: linear-gradient(to bottom, #FA5956, #FB8650);
}

.bu-list_item:nth-of-type(2) {
  background-image: linear-gradient(to bottom, #3C83FA, #50B2FA);
}

.bu-list_item:nth-of-type(3) {
  background-image: linear-gradient(to bottom, #5E80FF, #66A4FF);
}

.bu-list_item:nth-of-type(4) {
  background-image: linear-gradient(to bottom, #21BAAD, #18CFBF);
}

.bu-list_item:nth-of-type(5) {
  background-image: linear-gradient(to bottom, #FF8939, #FFA846);
}

.bu-list_item:nth-child(6),
.bu-list_item:nth-child(11) {
  background-color: #FEF2F5;
}

.bu-list_item:nth-child(7),
.bu-list_item:nth-child(12) {
  background-color: #F0F8FE;
}

.bu-list_item:nth-child(8),
.bu-list_item:nth-child(13) {
  background-color: #F2F4FF;
}

.bu-list_item:nth-child(9),
.bu-list_item:nth-child(14) {
  background-color: #ECFBF8;
}

.bu-list_item:nth-child(10),
.bu-list_item:nth-child(15) {
  background-color: #FFF9F3;
}

.bu-list_item:nth-of-type(-n+5) .bu-name {
  color: #FFF;
  font-weight: bold;
}

/* 二屏宫格 */
.bu-list_cusitem {
  position: relative;
  float: left;
  overflow: hidden;
  display: flex;
  flex-direction: column;
  text-align: center;
  height: 116rpx;
  width: 138rpx;
  margin-bottom: 2rpx;
  margin-right: 2rpx;
}

.bu-list_cusitem:nth-of-type(5n+2),
.bu-list_cusitem:nth-of-type(5n+4) {
  width: 140rpx;
}

.bu-list_cusitem:nth-of-type(5n) {
  margin-right: 0;
}

/* 宫格公共 */
.bu-wrap {
  flex: none;
  display: flex;
  align-items: center;
  flex-wrap: wrap;
  width: 702rpx;
  height: fit-content !important;
  margin: 0 auto;
  overflow: hidden;
  border-radius: 16rpx;
}

.firstpage {
  justify-content: space-between;
}

.bu-icon {
  display: flex;
  width: 56rpx;
  height: 56rpx;
  margin: 12rpx auto 8rpx;
}

.bu-name {
  text-align: center;
  color: #333;
  line-height: 30rpx;
  font-size: 24rpx;
}

.bu-tip {
  position: absolute;
  font-size: 18rpx;
  font-weight: 700;
  height: 20rpx;
  padding: 2rpx 6rpx 2rpx 10rpx;
  border-radius: 0 0 0 16rpx;
  color: #fff;
  background: #F97269;
  right: 0rpx;
  top: 0rpx;
  white-space: nowrap;
  display: flex;
  align-items: center;
}

/* 宫格point */
.dots {
  display: flex;
  justify-content: center;
  height: 40rpx;
  background: #FFF;
  padding-top: 12rpx;
  box-sizing: border-box;
}

.dot {
  height: 8rpx;
  width: 8rpx;
  border-radius: 4rpx;
  background-color: #CCC;
}

.dot+.dot {
  margin-left: 8rpx;
}

.dot.active {
  width: 28rpx;
  background-color: #0086F6;
}

/* 二屏 */
.second-wrap {
  width: 100%;
  background-image: #F8F8F8;
}

.hot-sale {
