    align-items: center;
    justify-content: center;
}
.dt-header .card {
    background-color: #FFF;
    border-radius: 24rpx;
    margin: -24rpx 0 16rpx 0;
    position: relative;
    font-size: 24rpx;
}
.dt-header .card.card-transparent {
    background-color:transparent;
}
.dt-header .card .item {
    display: flex;
}
.dt-header .card .item.col {
    flex-direction: column;
}
.dt-header .head-info-row {
    display: flex;
    flex-wrap: nowrap;
}
.dt-header .card .hotel-title-box {
    margin: 34rpx 0rpx 16rpx 24rpx;
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    position: relative;
}
.dt-header .card .hotel-title-box .copy-btn {
    position: absolute;
    border-radius: 8rpx;
    background-color: rgba(0, 0, 0, .6);
    color: #fff;
    padding: 12rpx 34rpx;
    top: -76rpx;
    left: 12%;
    font-size: 26rpx;
}
.dt-header .card .hotel-title-box .copy-btn::after {
    content: "";
    position: absolute;
    left: 50rpx;
    top: 100%;
    width: 0; height: 0;
