/* 导航 */
.custom-navigation-back .navback-arrow.white {
    color: #fff;
    text-shadow: 0 0 20rpx rgba(0,0,0,0.9);
}
.custom-navigation-back .navback-more {
    position: relative;
    height: 60rpx;
    box-sizing: border-box;
    background-color: rgba(255,255,255,0.6);
    border-radius: 30rpx;
    line-height: 20rpx;
    display: flex;
    align-items: center;
    text-align: center;
}
.custom-navigation-back .navback-more::after {
    content: '';
    position: absolute;
    border-radius: 64rpx;
    width: 202%;
    height: 206%;
    box-sizing: border-box;
    border: 2rpx solid rgba(151, 151, 151, .3);
    transform-origin: 0;
    transform: scale(0.5);
    z-index: -1;
}
.navback-arrow-share {transform: rotate(90deg); width: 76rpx;}
.navback-more .navback-more-text {
    font-size: 20rpx;
    width: 78rpx;
    border-width: 0 0 0 1rpx;
    border-style: solid;
    border-color: #ddd;
    border-image: url(data:image/gif;base64,R0lGODlhBQAFAIABAN3d3f///yH5BAEAAAEALAAAAAAFAAUAAAIHhB9pGatnCgA7) 2 stretch;
    font-weight: 600;
}

/* head-img */
.dt-header .head-img {
    position: relative;
    min-height: 375rpx;
}
.head-img .img-slider { height: 100%; }
.head-img .bg { display: block; width: 100%; height: auto; margin-top: -1px; }
.head-img .phone-num {
    position: absolute;
    left: 16rpx;
    bottom: 44rpx;
    background-color: rgba(0, 0, 0, .4);
    color: #fff;
    font-size: 20rpx;
    display: flex;
    align-items: center;
    border-radius: 28rpx;
    padding: 10rpx 16rpx;
}

/* head-card */
.wechat-font-collected {color: #f00}
.dt-header .dt-icon-box {
    position: absolute;
    top: -28rpx;
    right: 40rpx;
    display: flex;
    color: #111111;
    font-size: 32rpx;
