}
.dt-header .card .hotel-title-box .hotel-title.copy-bg {background-color: rgba(0, 0, 0, .08);}
.hotel-title {
    font-size: 40rpx;
    line-height: 42rpx;
    font-weight: 600;
    margin-right: 10rpx;
    font-family: PingFangSC-Medium;
}
.hotel-title.ft36 { font-size: 36rpx; }
.dt-header .card .item .fac {
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 16rpx 24rpx 24rpx;
}
.dt-header .card .item .fac .l {
    height: 30rpx;
    padding-right: 30rpx;
    display: flex;
    flex-wrap: wrap;
    overflow: hidden;
}
.dt-header .card .item .fac .r {
    flex: 0 0 auto;
}
.dt-header .card .item .hotel-title-box .li {
    font-size: 20rpx;
    height: 30rpx;
    line-height: 30rpx;
    flex: 0 0 auto;
}
.dt-header .card .rank-icon {
    line-height: 24rpx;
    font-size: 20rpx;
