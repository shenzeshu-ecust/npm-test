}
.dt-header .card .item .li:not(:last-child) {
    margin-right: 10rpx;
}
.dt-header .dstar {
    width: 70rpx;
    height: 24rpx;
    margin-bottom: 3rpx;
    vertical-align: middle;
}
.dt-header .medal {
    width: 28rpx;
    height: 28rpx;
    vertical-align: middle;
}
.dt-header .star-diamond {
    height: 10px;
}

/* 头部榜单勋章 */
.dt-header .rank-medal {
    margin-right: 24rpx;
    transform: translateY(-64rpx);
    margin-left: 32rpx;
}
.dt-header .rank-medal image {
    width: auto;
    height: 136rpx;
}
.dt-header .card .hotel-type {
    font-size: 24rpx;
    color: #555;
    height: 32rpx;
    line-height: 32rpx;
    margin: 6rpx 8rpx 0rpx 0rpx;
}
.dt-header .tag-img {
    height: 24rpx;
    line-height: 24rpx;
    width: auto;
    margin-right: 8rpx;
    margin-top: 6rpx;
}
.dt-header .prime-img {
    height: 24rpx;
    line-height: 24rpx;
    width: auto;
    background: #feefcf;
    border-radius: 4rpx;
    margin-right: 8rpx;
    margin-top: 6rpx;
}
.dt-header .card .rank-label {
    background:#ffeedf;
    border-radius: 4rpx;
    color: #673114;
    font-size: 22rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    height: 32rpx;
    line-height: 32rpx;
    padding-left: 6rpx;
    box-sizing: border-box;
    margin-top: 6rpx;
}
.dt-header .card .annual-label {
    color: #352216 !important;
    background: #fee4bf !important;
}
.dt-header .card .item .fac .fac-tag {
    height: 32rpx;
    line-height: 32rpx;
    font-size: 24rpx;
    display: flex;
    align-items: center;
    letter-spacing: 0;
}
.dt-header .center-line {
    background: #aaa;
    height: 20rpx;
    line-height: 20rpx;
    width: 1rpx;
    margin-left: 10rpx;
    display: inline-block;
}
.dt-header .card .item .li:last-child .center-line {
    display: none;
}
.dt-header .detail-fac {
    color: #111;
    font-size: 24rpx;
    height: 32rpx;
    line-height: 32rpx;
}
.dt-header .dt-color {
    color: #111;
}
.dt-header .fl {
    display: flex;
    align-items: center;
}

/* 收藏与分享样式*/
.collect-share-box {
    display: flex;
    flex-direction: column;
    position: fixed;
    bottom: 215rpx;
    left: 20rpx;
    color: #111111;
    font-size: 40rpx;
    z-index: 100;
}
.bt {
    bottom: 320rpx;
}
.collect-share-box .collect-share-item {
    height: 80rpx;
    width: 80rpx;
    border-radius: 50%;
    background: #FFFFFF;
    box-shadow: 0 6px 16px 2px rgba(0,0,0,0.08);
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 24rpx;
    border: 1rpx solid #e5e5e5;
}
.comment-address {
    display: flex;
    align-items: stretch;
    padding: 0 24rpx 28rpx;
    box-sizing: border-box;
}

/* 点评 */
.comment-brief-info {
    display: flex;
    padding: 20rpx 17rpx;
    font-size: 24rpx;
    background: #F3F8FF;
    border-radius: 8rpx;
    width: 270rpx;
    flex-shrink: 0;
    flex-wrap: wrap;
    align-content: center;
    box-sizing: border-box;
}
.comment-brief-info .fen {
    font-size: 30rpx;
    border-radius: 8rpx 8rpx 20rpx 8rpx;
    display: inline-block;
    font-weight: 600;
    line-height: 1;
    padding: 6rpx 13rpx 6rpx 8rpx;
    box-sizing: border-box;
    color: #fff;
    background: url("https://pages.c-ctrip.com/hotels/wechat/img/icons/detail-comment-wave.png") right bottom/40rpx 40rpx no-repeat, var(--color);
}
.comment-brief-info .dt-font {
    font-size: 24rpx;
}
.comment-brief-info .num {
    font-size: 24rpx;
    font-weight: normal;
}
.comment-brief-info .fen-desc {
    margin: 0 7rpx;
    font-weight: 500;
}
.comment-brief-info .cust {
    color: #111111;
    font-weight: 500;
}
.comment-brief-info .desc {
    color: #555;
    text-align: left;
}
.comment-brief-info .desc + .desc {
    margin-left: 11rpx;
}
.comment-brief-info .no-comment {
    word-break: break-all;
    text-align: justify;
    font-weight: bold;
}

/* 地图 */
.dt-header .card .map {
    justify-content: space-between;
    border-radius: 8rpx;
    background: #F2FCF9 url(https://pages.c-ctrip.com/hotels/wechat/img/detail-map-mark.png) no-repeat right / contain;
    padding: 24rpx 20rpx;
    margin-left: 12rpx;
    display: flex;
    align-items: center;
    flex: 1;
    box-sizing: border-box;
    width: calc(100% - 330rpx);
}
.dt-header .card .map .txt {
    width: calc(100% - 88rpx);
}
.dt-header .card .map .clamp2 {
    overflow: hidden;
    text-overflow: ellipsis;
    /* 只要超过宽度就换行，不论中文还是英文 */
    word-break: break-all;
    /* 最多展示两行 */
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
}
.dt-header .card .map .map-loc {
    font-weight: normal;
    padding-bottom: 4rpx;
    color: #555555;
}
.dt-header .card .map .map-loc .vl {
    line-height: 36rpx;
    color: #888888;
    margin: 0 12rpx 0 8rpx;
}
.dt-header .card .map .round {
    font-family: PingFangSC-Medium;
    font-weight: bold;
    margin-bottom: 8rpx;
    color: #111111;
}
.dt-header .dt-map-box {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    margin-left: 24rpx;
}
.dt-header .dt-map-box .wechat-font-locate-fill {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
    background: linear-gradient(-180deg, rgb(85, 85, 85) 0%, rgb(68, 68, 68) 100%);
    color: #fff;
    text-align: center;
    line-height: 40rpx;
    font-size: 20rpx;
    box-shadow: 0 4rpx 6rpx 0 rgba(154, 206, 251, 0.2);

}
.dt-header .card .map .mark {
    color: #555555;
    margin-top: 8rpx;
    width: 58rpx;
    text-align: center;
}

/* 星球号 */
.from-planet-banner {
    height: 88rpx;
    margin: 0 24rpx 16rpx 24rpx;
    border-radius: 8rpx;
    overflow: hidden;
}
.from-planet-banner .planet-img {
    height: 100%;
    width: 100%;
}
/*爆款预售氛围包装*/
.presale-banner { width: calc(100% - 48rpx); height: 88rpx; margin: 16rpx 24rpx; border-radius: 8rpx; }
.presale-tag { display: inline-block; background-color: #f5190A; color: #fff; font-size: 20rpx; padding: 4rpx; border-radius: 4rpx; vertical-align: 8rpx; margin-right: 4rpx;}
.from-planet{display: inline-block; border: 1rpx solid #bbc9dc; color: #5679a8; font-size: 20rpx; padding: 4rpx; border-radius: 4rpx; vertical-align: 8rpx; margin-right: 4rpx;}

/* 重要通知 */
.detail-emerge {
    position: relative;
    z-index: 1000;
}
.detail-emerge::after {
    position: absolute;
    background: #FFFFFF;
    height: 24rpx;
    width: 100%;
    content: '';
}
.em-detail-notice {
    background: linear-gradient(90deg, rgb(255, 238, 237) 50%, rgb(255, 255, 255));
    border-radius: 24rpx 24rpx 0 0;
    border: 1rpx solid #ffffff;
}
.em-detail-inner {
    padding-left: 24rpx;
    line-height: 80rpx;
    position: relative;
    display: flex;
    align-items:center;
    font-size: 24rpx;
    color: #F5190A;
}
.em-detail-lmask {
    position: absolute;
    right: 48rpx;
    height: 100%;
    width: 32rpx;
    background: linear-gradient(90deg, rgb(255, 255, 255, 0) 0%, rgb(255, 255, 255, 1) 100%);
}

/* 底部点评设施等common part */
.dt-fs-box { background-color: #fff; font-size: 26rpx; color:#111111; margin-top: 8rpx; padding: 0 30rpx 32rpx; }
.dt-fs-box .h2 { font-size: 36rpx; padding: 32rpx 0 24rpx 0;font-weight: bold; }
.dt-fs-box .label { font-size: 28rpx; padding-bottom: 10rpx; clear: both; font-weight:bold; }
.jump-box {
    text-align: center;
    font-size: 24rpx;
    color: #2698F7;
    margin-top: 32rpx;
}
.jump-box .cbtn {
    color: #111;
    border: 1rpx solid #c5c5c5;
    padding: 10rpx 16rpx;
    border-radius: 8rpx;
    display: inline-block;
    position: relative;
    border-width: 0rpx !important;
    z-index: 0;
}
/* 修复部分iphone手机无边框 */
.cbtn::after {
    content: "";
    border-style: inherit;
    border-color: inherit;
    border-radius: 16rpx;
    box-sizing: border-box !important;
    position: absolute;
    border-width: 2rpx !important;
    left: 0;
    top: 0;
    width: 200% !important;
    height: 200% !important;
    transform-origin: 0 0;
    transform: scale(0.5) !important;
    z-index: -1;
}
/* 固定日历bar */
.calendar-top-bar {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 100;
    box-shadow: 0 4px 20px 4px rgba(0, 0, 0, 0.06);
}

/* 优惠bar */
.promotion-bar { border-radius: 16rpx 16rpx 0 0; padding: 24rpx 24rpx 12rpx 24rpx; background-color: #fff; display: flex; }
.promotion-bar .pro-img { width: 56rpx; height: 26rpx; margin-top: 4rpx; }
.promotion-bar .pro-tag-box { flex: 1; display: flex; flex-wrap: wrap; margin: 0 12rpx; }
.promotion-bar .pro-tag-box .pro-tag { margin: 0 8rpx 12rpx 0; }
.pro-icon-box { color: #f70; font-size: 24rpx; display: flex; line-height: 1; padding-top: 5rpx; font-weight: 500; }
/* 优惠common */
.promotion-cicon { color: #888888; margin-left: 6rpx; }
.promotion-ctag {
    border: 1rpx solid #FFDCBF;
    border-radius: 6rpx;
    color: #FF6913;
    font-size: 20rpx;
    line-height: 1;
    padding: 6rpx 8rpx;
    align-self: flex-start;
    font-weight: 500;
    transform: rotateZ(360deg);
}
/* 优惠浮层 */
.promotion-layer .pro-item { display: flex; margin-bottom: 26rpx; }
.promotion-layer .pro-item .pro-text { flex: 1; font-size: 26rpx; margin: 0 20rpx; }
.room-mod {
    position: relative;
    border-radius: 24rpx 24rpx 0 0;
}
/* 筛选 */
.dt-filter {
    display: flex;
    font-size: 24rpx;
    padding: 0 24rpx;
    background: #fff;
}
.dt-filter .qs-box {
    flex: 1;
    white-space: nowrap;
    overflow-x: scroll;
    scrollbar-width: none;
    -ms-overflow-style: none;
    -webkit-overflow-scrolling: touch;
}
.dt-filter .qs-box::-webkit-scrollbar {
    display: none;
    width: 0 !important;
    height: 0 !important;
    -webkit-appearance: none;
    background: transparent;
    color: transparent;
}
.dt-filter .filter-btn {
    height: auto;
    width: 100rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
    box-shadow: -17rpx 0rpx 15rpx -13rpx rgba(0, 0, 0, .1);
    position: relative;
}
.dt-filter .qs-box .qs-item {
    display: inline-block;
    text-align: center;
    border-radius: 8rpx;
    white-space: nowrap;
    margin-right: 10rpx;
    padding: 14rpx 16rpx;
}
.dt-filter .filter-btn .arrow-down {
    display: inline;
    font-size: 16rpx;
    margin-left: 8rpx;
}
.dt-filter .filter-btn .filtered-sign {
    position: absolute;
    color: red;
    top: 0;
    right: 2rpx;
    font-size: 44rpx;
    font-weight: 700;
}
/* 筛选浮层 */
.filter-layer .filter-tit {
    font-size: 32rpx;
    font-weight: 700;
}
.filter-layer .filter-box {
    display: flex;
    margin: 30rpx 0;
    flex-wrap: wrap;
}
.filter-layer .filter-box .filter-item {
    flex: 0 0 32%;
    margin: 0 2% 2% 0;
    font-size: 26rpx;
    text-align: center;
    border-radius: 8rpx;
    background-color: #f5f5fa;
    box-sizing: border-box;
    padding: 19rpx 4rpx 14rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}
.filter-layer .filter-box .filter-item:nth-child(3n+3) {
    margin-right: 0;
}
.filter-layer .filter-box .filter-s {
    background: #e7f3ff url(https://pages.c-ctrip.com/hotels/wechat/img/checkbox-icon_new@2x.png) right bottom no-repeat;
    background-size: 22rpx;
}
.filter-layer .filter-box .filter-s, .qs-box .filter-s {
    color: #006FF6;
}

/* 房型Start */
.transz0 { transform: translateZ(0); }
.dt-room {  position: relative; padding: 0rpx 16rpx; }
.dt-room .no-room { color: #808080; padding: 30rpx; font-size: 28rpx; display: flex; align-items: center; justify-content: center; background: #fff; border-radius: 16rpx; }
.dt-room .no-room.no-bottom-border { border-radius: 16rpx 16rpx 0 0; }
.no-room-img {
    width: 166rpx;
    height: 186rpx;
    margin-right: 16rpx;
    flex: 0 0 auto;
}
.no-room-reason {
    font-size: 26rpx;
}
.no-room-tit {
    margin-bottom: 7rpx;
    font-weight: bold;
    color: #111111;
