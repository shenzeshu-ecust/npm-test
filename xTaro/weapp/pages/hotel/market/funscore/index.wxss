/*权益浮层*/
.equity-layer-title {
	font-size: 34rpx;
	font-weight: bold;
	color: #333333;
}
.equity-des {
	-webkit-overflow-scrolling: touch;
}
.equity-des .items {
	padding-top: 20rpx;
	border-bottom: 8rpx solid #F4F4F6;
	font-size: 24rpx;
 	color: #111111
}
.equity-des .items:last-of-type {
	border:none 0;
}
.equity-des .h2 {
	font-size: 34rpx;
	color: #111111;
	font-weight: 500;
	padding-bottom: 42rpx;
	position: relative;
}
.equity-des .h2:before {
	content: "";
	width: 8rpx;
	height: 34rpx;
	display: inline-block;
	vertical-align: -4rpx;
	margin-right: 6rpx;
	background-color: #D5AF62;
 	border-radius: 8rpx;
}
.equity-des .step .item {
	padding-bottom: 42rpx;
	position: relative;
}
.equity-des .step .item:after{
	width:2rpx;
	content: "";
	position: absolute;
	top:40rpx;
	bottom: 0;
	left: 23rpx;
	background-color:#FAE1AD;
}
.equity-des .step .item:last-of-type:after {
	display: none;
}
.equity-des .step .item .image {
	padding-top:26rpx;
	text-align: center;
}
.equity-des .i1 {
	width: 488rpx;
	height: 202rpx;
}
.equity-des .i2 {
	width: 490rpx;
	height: 246rpx;
}
.equity-des .i3 {
	width: 464rpx;
	height: 337rpx;
}
.equity-des .i4 {
	width: 464rpx;
	height: 287rpx;
}
.equity-des .n {
	width: 46rpx;
	height: 46rpx;
	line-height: 46rpx;
	border-radius: 50%;
	font-size: 34rpx;
	color: #8C5B15;
	margin-right:16rpx;
	text-align: center;
 	background-color: #FAE1AD;
}
.equity-des .member-text {
	padding-left: 18rpx;
	font-size: 24rpx;
	color: #111111;
}
.equity-des .member-text .c1 {
	color: #D5AF62;
	padding-bottom: 8rpx;
}
.equity-des .member-text .c2 {
	padding-bottom: 30rpx;
