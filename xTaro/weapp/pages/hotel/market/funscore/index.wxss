/*tbody*/
.page-container .tbody .items {
	padding: 24rpx 0;
	margin:0 24rpx 24rpx;
	background-color: #fff;
	border-radius: 8rpx;
	overflow: hidden;
}
.page-container .tbody .basis{
	background-color: #FEF9DF;
	border: 2rpx solid #FEF1DF;
	border-radius: 8rpx;
	align-self:flex-start;
	display: flex;
	align-items: center;
	justify-content: center;
}
.page-container .tbody .h2 {
	font-size: 34rpx;
	font-weight: 500;
	color: #111111;
	padding:0 24rpx;
	text-align: center;
	position: relative;
}
.page-container .tbody .h4 {
	font-size: 24rpx;
	font-weight: 300;
	color: #888888;
	padding:20rpx 24rpx 0;
	text-align: center;
}
.page-container .tbody .ck-details {
	position: absolute;
	right: 0;
	top: -12rpx;
	padding: 20rpx;
	color: #555555;
	font-size: 20rpx;
	font-weight: 300;
}
.page-container .tbody .h3 {
	font-size: 28rpx;
	font-weight: 500;
	color: #111111;
}
.dotask .item {
	display: flex;
	padding: 0 24rpx;
}
.dotask .item .tip{
	font-size: 28rpx;
	color: #FFA000;
	font-weight: 500;
}
.dotask .item .tip2{
	font-size: 24rpx;
	color: #888888;
	padding: 8rpx 0 10rpx;
}
.dotask .item .basis {
	width: 88rpx;
	height: 88rpx;
}
.dotask .item .shrink {
	flex: 1;
	display: flex;
	border-bottom: 1rpx solid #E5E5E5;
	margin:0 0 8rpx 16rpx;
	padding-bottom: 22rpx;
}
.dotask .item:last-of-type .shrink {
	border-bottom: 0;
	margin-bottom: 0;
	padding-bottom: 0;
}
.dotask .item .shrink .slider {
	margin: 14rpx 0 8rpx;
	height: 8rpx;
	background-color: rgba(0,0,0,.1);
	border-radius: 4rpx;
	overflow: hidden;
}
.dotask .item .shrink .slider .step {
	background-color: #FFA000;
	height: 100%;
	transition:all 400ms ease-in-out;
	border-radius: 4rpx;
}
.dotask .item .shrink .step-state{
	font-size: 24rpx;
	color: #888888;
	float: right;
	font-weight: 400;
	padding-top: 8rpx;
}
.dotask .item .shrink .middle {
	flex: 1;
	margin: 10rpx 24rpx 0 0
}
.dotask .item .task-btn {
	font-size: 24rpx;
	color: #fff;
	padding: 10rpx 18rpx;
	margin-top: 18rpx;
	border-radius: 30rpx;
	align-self: flex-start;
	line-height: 32rpx;
}
.dotask .item .gettask{
	background-image: linear-gradient(to right, #65B5FF 1%, #006FF6 99%);
	box-shadow: 0 8rpx 16rpx 0 rgba(35,150,255,0.3);
}
.dotask .item .gotask{
	background-image: linear-gradient(to right, #FFC649 0%, #FFA000 99%);
	box-shadow: 0 8rpx 16rpx 0 rgba(255,160,0,0.3);
}
.dotask .item .getscore{
	border: 1px solid #FFA000;
	color: #ffa000;
	box-shadow: 0 8rpx 16rpx 0 rgba(255,160,0,0.05);
}
.dotask .item .donescore{
	background-color: #ddd;
	box-shadow: 0 8rpx 16rpx 0 rgba(221,221,221,0.3);
}
.dotask .item .qa {
	background-color: #FEF9DF;
	box-shadow: 0 8rpx 16rpx 0 #FEF9DF;
	border-radius: 8rpx;
	margin-top: 24rpx;
	padding: 40rpx 32rpx;
}
.dotask .item .qa .h4 {
	font-size: 28rpx;
	color: #FFA000;
	font-weight: 500;
	padding-bottom: 12rpx;
}
.dotask .item .qa .list {
	border:2rpx solid #FFA000;
	border-radius: 60rpx;
	padding: 16rpx 64rpx 16rpx 24rpx;
	box-sizing: border-box;
	margin-top: 16rpx;
	line-height: 1.4;
	font-size: 24rpx;
	font-weight: 500;
	color: #FFA000;
	word-break:break-all;
}
.dotask .item .qa .list.done {
	color:#fff;
	background-color: #ddd;
	border:none;
	box-shadow: 0 8rpx 16rpx 0 rgba(221,221,221,0.3);
}
.dotask .item .qa .list.answer-results,.dotask .item .qa .list.right {
	color:#fff;
	border:none;
	background-image: linear-gradient(to right, #FFC649 0%, #FFA000 99%);
	box-shadow: 0 8rpx 16rpx 0 rgba(255,160,0,0.3);
	position: relative;
}
.dotask .item .qa .list.answer-results:after {
	font-family: "wechat-font";
	font-size: 30rpx;
	content: "\e901";
	position: absolute;
	right:26rpx;
	top: 50%;
	transform: translateY(-50%);
}
.dotask .item .qa .list.error {
	color:#fff;
	border:none;
	background-image: linear-gradient(to right, #FF8260 1%, #FF4D1D 99%);
	box-shadow: 0 8rpx 16rpx 0 rgba(255,77,29,0.3);
	position: relative;
}
.dotask .item .qa .list.error:after {
	font-family: "wechat-font";
	content: "\e914";
	position: absolute;
	right:26rpx;
	top: 50%;
	transform: translateY(-50%);
}
.c-layout {
	display: flex;
	flex-wrap: wrap;
	justify-content: center;
	font-size: 22rpx;
	color: #111111;
}
.c-layout .item {
	padding: 0 35rpx;
	margin-bottom: 32rpx;
	text-align: center;
}
.c-layout.no-mbottom .item{
	margin-bottom: 0;
}
.c-layout .basic {
	width: 72rpx;
	height: 72rpx;
	background-color: #FEF9DF;
	border: 2rpx solid #FEF1DF;
	border-radius: 50%;
	margin: 12rpx auto 16rpx;
	display: flex;
	align-items: center;
	justify-content: center;
}

/*积分兑换权益*/
.tobooking {
	width: 100%;
	position: fixed;
	bottom: 0;
	left: 0;
	padding:20rpx 72rpx;
	box-sizing: border-box;
	background-color: #fff;
	box-shadow: 0 -2rpx 10rpx rgba(0,0,0,.05);
}
.tobooking.iphonex {
	padding-bottom: 88rpx;
}
.tobooking .btn {
	padding: 20rpx;
	text-align: center;
	font-size: 34rpx;
	color: #fff;
	font-weight: 500;
	background-image: linear-gradient(to right, #FFB114 0%, #FF7700 100%);
	border-radius: 44rpx;
}
/*积分明细*/
.score-list {
	max-height: 800rpx;
}
.score-list .item {
	padding: 36rpx 0;
	margin:0 8rpx;
	border-bottom: 1rpx dashed #ccc;
}
.score-list .item:last-of-type {
	border-bottom: 0
}
.score-list .item .get-time {
	font-size: 28rpx;
	color: #888888;
	padding-top: 14rpx;
}
.score-list .item .value-added{
	font-size: 32rpx;
	color: #FFA000;
	font-weight: 500;
}
.score-list .item .value-minus{
	font-size: 32rpx;
	color: #888888;
	font-weight: 500;
}
.score-list .item .tit{
	flex: 1;
	font-size: 32rpx;
	color: #111111;
	font-weight: 500;
}
.score-list .item.text{
	font-size: 24rpx;
	color: #ddd;
	font-weight: 500;
	text-align: center;
}

/* UI改版 */
.page-container .thead{
	height: 250rpx;
	background-image: linear-gradient(to right, #D1B189 0%, #BF8E54 100%);
	color: #fff;
}
.page-container .thead .head {
	width: 100%;
	height: 100%;
	display: flex;
}
.score-entry{
	margin: 30rpx auto 80rpx;
	font-weight: 300;
	text-align: center
}
.score-entry .score {
	font-size: 64rpx;
	font-weight: 500;
}
.score-entry .jftxt {
	font-size: 24rpx;
}
.score-entry .check {
	margin-top : 16rpx;
	font-size: 20rpx;
	border:2rpx solid rgba(255,255,255,.6);
	padding: 10rpx 20rpx;
	border-radius: 30rpx;
}
.page-container .thead .item {
	width: 33%;
	height: 100%;
	text-align: center;
	margin: 60rpx auto;
}

