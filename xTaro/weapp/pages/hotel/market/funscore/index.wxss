/*common box*/
.g-box .item {
	width: 148rpx;
	height: 144rpx;
	padding: 56rpx 2rpx 0 2rpx;
	box-sizing: border-box;
	background-color: #fff;
	border:1rpx solid transparent;
	box-shadow: 0 0 8rpx 0 rgba(204,136,0,0.20);
	border-radius: 8rpx;
	text-align: center;
	position: relative;
	transition: background-color 300ms linear,border 300ms linear;
}
.g-box .item .jf-icon {
	width: 76rpx;
	height: 80rpx;
	border-radius: 50%;
	position: absolute;
	top:-32rpx;
	left: 50%;
	transform: translateX(-50%) translateY(0);
	transition: all 300ms linear;
}
.g-box .item .jf-icon .sale {
	font-size: 20rpx;
	color: #fff;
	padding: 4rpx 8rpx;
	background-color: #FF7700;
	border-radius: 12rpx 12rpx 12rpx 0;
	position: absolute;
	right: -28rpx;
	top: 0;
}
.g-box .item.current {
	background-color: #FFF3DB;
 	border: 1rpx solid #D5AF62;
}
.g-box .item.current .jf-icon {
	transform: translateX(-50%) translateY(-12rpx);
}
.g-box .item .h {
	font-size: 24rpx;
	color: #806033;
	font-weight: 500;
}
.g-box .item .s {
	font-size: 20rpx;
	color: #888888;
	font-weight: 300;
}
