    text-align: center;
    flex-shrink: 0;
    border-bottom: 1rpx solid #b8c6d9;
    box-sizing: border-box;
    background: #f5f7fa;
    display: flex;
    align-items: center;
    justify-content: center;
}
.cb-con {
    display: flex;
}
.cb-sub-tit {
    text-align: center;
    width: 96rpx;
    flex: 0 0 auto;
    background: #f5f7fa;
    border-bottom: 1rpx solid #b8c6d9;
    box-sizing: border-box;
    font-family: PingFangSC-Medium;
}
.cb-sub-con {
    flex: 1;
