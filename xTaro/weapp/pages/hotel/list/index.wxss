/*下拉显示品牌名*/
.slogan-attachment {
  width:100%;
  position: absolute;
  left: 0;
  top: 270rpx;
  z-index: -1;
  color: #888888;
  font-size: 28rpx;
  text-align: center;
  height: 146rpx;
  padding: 15rpx 15rpx 10rpx;
  box-sizing: border-box;
  display: block;
  background:url(https://pages.c-ctrip.com/hotels/wechat/img/slogan-icon.png) center 60rpx no-repeat;
  background-size: 70rpx 70rpx;
  transform: translate3d(0,0,0);
}

.heart {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    bottom: 0;
    left: calc(50% - 15px);
    transform: scale(0.2);
    opacity: 0;
  }
.heart:before,
.heart:after {
    position: absolute;
    content: "";
    left: 50rpx;
    top: 50rpx;
    width: 50rpx;
    height: 80rpx;
    background: #fc2e5a;
    border-radius: 50rpx 50rpx 0 0;
    transform: rotate(-45deg);
    transform-origin: 0 100%;
}
.heart:after {
  left: 0;
  transform: rotate(45deg);
  transform-origin: 100% 100%;
}
.heart1 {
  animation: heart1 5s infinite linear;
}
.heart2 {
  animation: heart2 6s 1.5s infinite linear;
}
.heart3 {
  animation: heart3 5s 3s infinite linear;
}
.heart4 {
  animation: heart4 4s 2s infinite linear;
}
@keyframes heart1 {
  0% {
    opacity: 0;
    transform: scale(0.2);
  }
  100% {
    opacity: 1;
    transform: scale(1) translate(-60rpx, -240rpx);
  }
}
@keyframes heart2 {
  0% {
    opacity: 0;
    transform: scale(0.2);
  }
  100% {
    opacity: 1;
    transform: scale(1) translate(80rpx, -280rpx);
  }
}
@keyframes heart3 {
  0% {
    opacity: 0;
    transform: scale(0.2);
  }
  100% {
    opacity: 1;
    transform: scale(1) translate(40rpx, -320rpx);
  }
}
@keyframes heart4 {
  0% {
    opacity: 0;
    transform: scale(0.2);
  }
  100% {
    opacity: 1;
    transform: scale(1) translate(-80rpx, -240rpx);
  }
}

