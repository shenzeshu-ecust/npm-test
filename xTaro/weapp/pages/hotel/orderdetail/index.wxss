@import "../common/common.wxss";
@import "operateLoglayer/index.wxss";
@import "noticelayer/index.wxss";
@import "../commontemplates/roompackage/package.wxss";
.module {
    border-radius: 24rpx;
    background: #fff;
    margin-top: 16rpx;
    padding: 32rpx 24rpx;
}
.pb40 { padding-bottom: 40rpx !important; }
.pb29 { padding-bottom: 29rpx; }
.ft26 { font-size: 26rpx; }
.mt7 { margin-top: 7rpx; }
.va { vertical-align: middle; }
.iphonex-margin {
    margin-bottom: 66rpx;
}
/*自定义导航栏 start*/
.nav-bar {
    color: #fff;
}
.nav-bar .weui-navigation-bar__inner {
    background: linear-gradient(90deg, rgb(0, 136, 246) 0%, rgb(0, 102, 246) 100%);
}
/*自定义导航栏 start*/
/*头部卡片 start*/
.head-info {
    background: linear-gradient(90deg, rgb(0, 136, 246) 0%, rgb(0, 102, 246) 100%);
    padding: 20rpx 24rpx;
    box-sizing: border-box;
    padding-bottom: 132rpx;
}
.double-ellipsis {
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.oc-close {
    position: absolute;
    right: 0;
    top: 0;
    font-size: 17rpx;
    width: 48rpx;
    height: 48rpx;
    display: inline-block;
    background: #eee;
    border-radius: 0px 16rpx 0px 100rpx;
    padding: 12rpx 12rpx 20rpx 20rpx;
    box-sizing: border-box;
}
.oc-arrow {
    width: calc(100% - 598rpx);
    text-align: right;
}
.order-status {
    color: #fff;
    line-height: 1;
}
.order-status-title {
    font-size: 50rpx;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    display: flex;
    align-items: center;
}
.arrow-ico {
    font-size: 30rpx;
    display: inline-block;
    height: 100%;
    line-height: 56rpx;
}
.order-status-desc {
    margin-top: 20rpx;
    font-size: 26rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    line-height: 36rpx;
}
.order-card-swiper {
    height: 128rpx;
    background: #fff;
    box-sizing: border-box;
    border-radius: 16rpx;
    font-size: 26rpx;
    margin-top: 20rpx;
    overflow: hidden;
}
.order-card-swiper .wx-swiper-dots .wx-swiper-dot {
    width: 8rpx !important;
    height: 8rpx !important;
}
.order-card-swiper .wx-swiper-dot.wx-swiper-dot-active {
    width: 24rpx !important;
    border-radius: 5rpx !important;
}
.order-card-swiper.high-height {
    height: 200rpx;
}
.order-card {
    height: initial;
}
.order-card .content {
    height: 128rpx;
    box-sizing: border-box;
    display: flex;
    padding: 24rpx;
    align-items: center;
}
.oc-title {
    color: #111111;
    font-size: 28rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    height: 36rpx;
    line-height: 36rpx;
}
.order-card .price {
    color: #0066f6;
}
.order-card .green {
    color: #08a66f;
}
.order-card .orange {
    color: #f50;
}
.notice-detail .oc-subtitle {
    margin: 0 24rpx;
    position: relative;
}
.notice-detail .oc-subtitle:before {
    content: "";
    width: 8rpx;
    height: 8rpx;
    border-radius: 50%;
    background: #f70;
    position: absolute;
    top: 50%;
    transform: translateY(-50%);
    left: -20rpx;
}
.notice-detail .title-num {
    display: flex;
    align-items: center;
}
.oc-subtitle {
    color: #555555;
    font-size: 24rpx;
    height: 32rpx;
    line-height: 32rpx;
}
.order-card .content .left {
    flex: 1;
    min-width: 480rpx;
    max-width: 598rpx;
}
.order-card .content .btn{
    flex-shrink: 0;
    padding: 13rpx 36rpx;
    color: #006FF6;
    border: 1rpx solid #2698f7;
    border-radius: 8rpx;
    transform: rotateZ(360deg);
}
.order-card .btns {
    background: #f5f9ff;
    border-radius: 0 0 16rpx 16rpx;
    overflow: hidden;
    display: flex;
    color: #0066f6;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
    height: 72rpx;
    padding: 0;
    line-height: 72rpx;
}
.order-card .btns .single-btn {
    flex: 1;
    text-align: center;
    border-right: 1rpx solid #dde4ed;
}
.order-card .btns .sub-label {
    color: #ffffff;
    font-size: 16rpx;
    padding: 1rpx 8rpx;
    background: #ff7700;
    margin-left: 10rpx;
    border-radius: 12rpx 12rpx 12rpx 0;
}
.oc-notice-num {
    display: inline-block;
    color: #f70;
    border: 1rpx solid #f70;
    margin-left: 4rpx;
    font-size: 20rpx;
    border-radius: 8rpx;
    vertical-align: text-bottom;
    transform: rotateZ(360deg);
    line-height: 1.3;
    padding: 0 4rpx;
}
.oc-detail-btn {
    position: absolute;
    right: 24rpx;
    bottom: 24rpx;
    color: #0066f6;
}
.order-card .content .btn.pay-btn {
    font-size: 26rpx;
    text-align: center;
    background: #006FF6;
    color: #fff;
    padding: 9rpx 30rpx;
    line-height: 1;
    font-weight: 500;
}
.pay-btn .count-down{
    font-size: 20rpx;
    margin-top: 4rpx;
}
.refresh {
    display: inline-block;
    background: rgba(0,0,0,0.2);
    border-radius: 22rpx;
    font-size: 26rpx;
    font-weight: normal;
    font-family: PingFangSC-Regular;
    margin-left: 18rpx;
    padding: 5rpx 14rpx;
    flex-shrink: 0;
}
/*头部卡片 end*/
/*通用浮层 start*/
.card-info {
    background: #fff;
    border-radius: 16rpx;
    color: #111111;
}
/*通用浮层 end*/
/*价格信息卡片 start*/
.price-cancel-area {
    margin-top: -108rpx;
    padding: 31rpx 24rpx 24rpx;
}
.price-info {
    padding-bottom: 24rpx;
}
.pay-info {
    display: flex;
    align-items: center;
}
.pay-item {
    flex: 1;
    display: flex;
    align-items: center;
}
.pay-item .title {
    color: #111111;
    font-size: 38rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    margin-right: 9rpx;
    flex: 0 0 auto;
}
.pay-item .wechat-font-qa {
    font-size: 28rpx;
    color: #555;
}
.pay-item .price {
    display: inline-block;
    margin-left: 9rpx;
    color: #ff5500;
    font-size: 44rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
}
.pay-item .price .desc {
    font-size: 24rpx;
    font-weight: normal;
    font-family: PingFangSC-Regular;
}
.price-detail-btn {
    color: #555555;
    font-size: 26rpx;
}
/*价格信息卡片 end*/
.policy {
    display: flex;
    align-items: flex-start;
    justify-content: space-between;
    font-size: 26rpx;
    padding-top: 24rpx;
    border-top: 1rpx solid #eee;
}
.policy.presale-order {
    padding-top: 0;
    border: unset;
}
.policy .desc {
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    color: #111;
}
.policy .btn {
    flex-shrink: 0;
    padding-left: 10rpx;
    color: #555;
}
/* 安心取消 */
.ec-icon {
    width: 28rpx;
    height: 28rpx;
}
.ec-name {
    color: #08a66f;
    font-size: 24rpx;
    font-weight: 500;
    margin: 0 8rpx;
    font-family: PingFangSC-Medium;
}
.policy .ec-detail {
    line-height: 1;
}
.ec-detail-desc .ec-icon  {
    width: 29rpx;
    height: 29rpx;
}
.ec-detail-desc .ec-name {
    font-size: 30rpx;
}
.ec-detail-desc .ec-title {
    display: flex;
    align-items: center;
    margin-bottom: 24rpx;
}
.ec-detail-desc .desc {
    background: #f2fcf9;
    border-radius: 8rpx;
    padding: 24rpx;
    font-size: 26rpx;
    color: #111;
    margin-bottom: 32rpx;
}
.ec-detail-desc .old-title {
    color: #111111;
    font-family: PingFangSC-Medium;
    font-size: 30rpx;
    font-weight: 500;
    margin-bottom: 25rpx;
}
/* 取消政策浮层 start */
.tips-list .ladder-cancel{
    border: unset;
}
.tips-list .jt-thead {
    border-radius: 8rpx 8rpx 0 0;
}
.tips-list .jt-thead .jt-txt:first-child {
    border-radius: 8rpx 0 0 0;
}
.tips-list .jt-thead .jt-txt:last-child {
    border-radius: 0 8rpx 0 0;
}
.tips-list .jt-thead .jt-txt {
    background-color: #eef1f6 !important;
}
.tips-list .ladder-cancel {
    margin-left: 0;
}
.tips-list .ladder-cancel .jt-txt {
    border-right: 3rpx solid #fff;
    background-color: #f8fafd;
}
.ladder-cancel .jt-tbody .jt-free{ color: #009c67; }
.ladder-cancel .jt-tbody {
    border-top: unset;
    border-bottom: 3rpx solid #fff;
}
.ladder-cancel .jt-tbody:last-child {
    border-radius: 0 0 8rpx 8rpx;
}
.ladder-cancel .jt-tbody:last-child .jt-txt:first-child {
    border-radius: 0 0 0 8rpx;
}
.ladder-cancel .jt-tbody:last-child .jt-txt:last-child {
    border-radius: 0 0 8rpx 0;
}
.ladder-cancel .hight-light {
    position: relative;
}
.ladder-cancel .hight-light:before {
    width: 3rpx;
    height: 100%;
    position: absolute;
    left: 0;
    content:'';
    background: linear-gradient(-180.2deg, rgb(255, 255, 255) 0%, rgb(0, 134, 246) 29.63%, rgb(0, 134, 246) 55.29%, rgb(255, 255, 255) 100%);
}
.ladder-cancel .hight-light:after {
    height: 0;
    width: 0;
    position: absolute;
    left: 3rpx;
    top: 30%;
    content: '';
    border: 6rpx solid;
    border-color: transparent transparent transparent #006FF6;
    border-right: 0;
}
.tips-list {
    color: #111111;
    font-size: 28rpx;
    height: 100%;
    min-height: 700rpx;
    font-weight: 500;
}
.tips-list .tips-item {
    text-align: justify;
    line-height: 36rpx;
    font-size: 24rpx;
    margin-top: 33rpx;
}
/* 取消政策浮层 end */
/*骨架屏*/
.order-fishbone {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 999;
}
.order-fishbone .img {
    width: 100%;
    height: 100%;
}
.order-fishbone .fishbone-loading {
    height: 58%;
    margin-top: 65%;
    position: absolute;
    bottom: 170rpx;
}
/*登录弹窗*/
.login-layer {
    background: #fff;
    width: 640rpx;
    border-radius: 8rpx;
    padding: 80rpx 10rpx;
    text-align: center;
}
.login-layer .text-title {
    font-size: 32rpx;
    color: #111111;
    font-weight: 500;
    line-height: 32rpx;
    padding: 20rpx 40rpx;
}
.login-layer .text-crude {
    font-size: 24rpx;
    color: #555555;
    font-weight: 500;
    line-height: 32rpx;
    padding: 20rpx 40rpx;
    margin-bottom: 40rpx;
}
.login-layer .text-line {
    width: 500rpx;
    height: 2rpx;
    margin: 0 auto;
    margin-bottom: 30rpx;
    background-color: #111111;
}
.login-layer .agree-btn {
    width: 500rpx;
    height: 82rpx;
    text-align: center;
    color: #fff;
    font-size: 28rpx;
    line-height: 82rpx;
    font-weight: 500;
    margin: 0 auto;
    background: linear-gradient(90deg, rgb(0, 136, 246) 0%, rgb(0, 102, 246) 100%);
}
/*酒店信息*/
.hotel-info {
    display: flex;
    position: relative;
}
.hotel-logo {
    width: 150rpx;
    border-radius: 8rpx;
    flex: 0 0 auto;
    margin-right: 18rpx;
    font-size: 26rpx;
    color: #111111;
    position: absolute;
    top: 0;
    bottom: 0;
    overflow: hidden;
    background-size: cover;
    background-image: url("https://pages.c-ctrip.com/hotels/wechat/img/default.png");
}
.hotel-content {
    padding-left: 168rpx;
}
.hotel-logo .days {
    font-size: 23rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: #fff;
    padding: 0 10rpx 0 6rpx;
    background: rgba(0,0,0,0.6);
    margin: 5rpx;
    border-radius: 7rpx;
    text-align: center;
    display: inline-block;
    height: 32rpx;
    line-height: 32rpx;
}
.hc-name {
    font-size: 34rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
}
.hc-en-name {
    font-size: 26rpx;
}
.hc-address {
    margin-top: 12rpx;
    font-size: 26rpx;
}
.hc-address .wechat-font-copy {
    color: #000;
    font-size: 20rpx;
}
.hotel-btns {
    margin-top: 30rpx;
    display: flex;
}
.hotel-btns .btn {
    flex: 1;
    display: inline-block;
    text-align: center;
    background: #f5f7fa;
    border-radius: 8rpx;
    padding: 19rpx 0;
    font-size: 26rpx;
    margin-right: 4%;
}

.hotel-btns .btn:last-child {
    margin-right: 0 !important;
}
.hotel-btns .car-box {
    position: relative;
}
.hotel-btns .promotion-tag {
    position: absolute;
    background: #ff7700;
    left: 0rpx;
    top: -8rpx;
    width: 48rpx;
    height: 24rpx;
    border-radius: 12rpx 12rpx 12rpx 0rpx;
    display: flex;
    flex-direction: column;
}
.hotel-btns .promotion-text {
    font-size: 16rpx;
    color: #fff;
    line-height: 24rpx;
    justify-content: center;
}
.hotel-btns .car-tag {
    margin-right: 9rpx;
}
.hotel-btns .icon {
    color: #3665a7;
}
/*入住信息*/
.module-title {
    color: #111111;
    font-size: 36rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    margin-bottom: 24rpx;
}
.checkin-content {
    box-shadow: 0 0 4px 3px rgb(138 140 142 / 9%);
    padding: 32rpx 24rpx;
    border-radius: 16rpx;
}
.oversea-time-tip {
    position: relative;
    z-index: 1;
    background: #fff;
    color: #555;
    font-size: 26rpx;
    padding-top: 15rpx;
}
.date-info, .date-info-hourrroom {
    border-bottom: 2rpx dotted #99aeca;
    padding-bottom: 29rpx;
    position: relative;
}
.date-info:after {
    content: '';
    position: absolute;
    bottom: 32rpx;
    top: 0;
    left: 13rpx;
    width: 10rpx;
    background: #D9E8FE;
}
.date-info .item {
    line-height: 1;
    z-index: 1;
    position: relative;
}
.date-info .checkin {
    margin-bottom: 25rpx;
}
.date-info .item .icon {
    vertical-align: bottom;
    background: #0f4999;
    color: #fff;
    border-radius: 50%;
    font-size: 24rpx;
    font-family: FZY3JW--GB1-0;
    display: inline-block;
    width: 38rpx;
    height: 38rpx;
    line-height: 38rpx;
    text-align: center;
    margin-right: 16rpx;
}
.date-info .item .time {
    color: #111111;
    font-size: 30rpx;
    font-family: PingFangSC-Medium;
}
.room-info {
    margin-top: 29rpx;
}
.room-title {
    font-size: 30rpx;
    margin-bottom: 24rpx;
}
.date-info-hourrroom .tit, .room-title .tit {
    color: #111111;
    font-size: 30rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
}
.date-info-hourrroom .desc, .room-basic {
    color: #111111;
    font-size: 26rpx;
    font-family: PingFangSC-Regular;
}
.room-basic .facility-item:before {
    content: '|';
    color: #aaa;
    margin: 0 8rpx;
}
.room-basic .facility-item:first-child:before {
    content: '' !important;
    margin: 0;
}
/*餐食信息*/
.checkin-breakfast {
    margin-top: 32rpx;
}
.cb-sub-title {
    line-height: 1;
    margin-bottom: 16rpx;
}
.cb-sub-title .line {
    background: linear-gradient(-180deg, rgb(0, 118, 245) 0%, rgba(0, 167, 250, 0.5) 100%);
    width: 6rpx;
    height: 22rpx;
    border-radius: 4rpx;
    display: inline-block;
}
.cb-sub-title .subtitle {
    display: inline-block;
    margin-left: 18rpx;
    color: #111111;
    font-size: 26rpx;
    font-family: PingFangSC-Medium;
}
.cb-title {
    font-size: 30rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    margin-bottom: 20rpx;
    color: #111;
}
.breakfast-detail {
    position: relative;
    background: #f5f7fa;
    border-radius: 16rpx;
    padding: 20rpx 24rpx;
}
.breakfast-detail .item {
    display: flex;
    font-size: 26rpx;
    margin-bottom: 8rpx;
    align-items: center;
}
.breakfast-detail .bd-title {
    width: 124rpx;
    flex: 0 0 auto;
}
.breakfast-fold {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    height: 103rpx;
    color: #555555;
    line-height: 146rpx;
    background: linear-gradient(-180deg, rgba(245,247,250, 0) 0%, rgba(245,247,250, 1) 49.27%, rgb(245,247,250) 100%);
}
.checkin-info-module .toggle-btn {
    font-size: 26rpx;
    text-align: center;
}
.checkin-info-module .wechat-font-g-arrow-up, .breakfast-fold .wechat-font-g-arrow-down {
    margin-left: 8rpx;
}
/*套餐内餐食*/
.meal-suite, .gift-suite {
    margin-top: 32rpx;
}
.meal-suite-content {
    position: relative;
    background: #f5f7fa;
    border-radius: 16rpx;
    font-size: 26rpx;
    padding: 0 24rpx;
    color: #111;
}
.mealsuite-item {
    padding: 29rpx 0;
    border-top: 1rpx solid #ddd;
    box-sizing: border-box;
    display: flex;
    align-items: center;
}
.meal-suite-content .mealsuite-item:first-child {
    border: unset;
}
.mealsuite-name {
    flex: 1;
}
.ms-appoint {
    min-width: 160rpx;
    text-align: right;
    margin-right: 8rpx;
}
.ms-appoint.highlight, .tc-left .highlight{
    color: #ff7700;
}
.meal-suite .breakfast-fold, .gift-suite .breakfast-fold {
    height: 100rpx;
    line-height: 123rpx;
    background: linear-gradient(-180deg, rgba(245,247,250, 0) 0%, rgba(245,247,250, 1) 26.27%, rgb(245,247,250) 100%);
}
/*套餐浮层*/
.sl-content {
    box-sizing: border-box;
}
.suite-item {
    background: #fff;
    padding: 28rpx 24rpx 32rpx;
    border-radius: 16rpx;
    margin: 16rpx 0;
}
.sl-content .pac-menu {
    margin: 24rpx 0 20rpx;
}
.suite-header {
    display: flex;
    font-size: 26rpx;
    align-items: center;
}
.suite-header-desc {
    flex: 1;
    padding-bottom: 20rpx;
    border-bottom: 1rpx solid #eee;
}
.suite-content {
    margin-top: 24rpx;
}
.suite-header-desc .name {
    color: #111111;
    font-size: 30rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    display: block;
    margin-bottom: 8rpx;
}
.suite-header-desc .highlight {
    color: #f24c3d;
    margin-top: 6rpx;
    display: block;
}
.suite-appoint-btn {
    padding: 15rpx 28rpx;
    color: #fff;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    background: #0066f6;
    border-radius: 8rpx;
}
.suite-item .pac-menu-border {
    border: unset;
    background: #f8fafd;
}
.gift-suite-title {
    color: #111111;
    font-size: 30rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    margin: 32rpx 0rpx 20rpx ;
}
/*门票*/
.ticket-info {
    margin-top: 30rpx;
    display: flex;
    margin-bottom: 16rpx;
    background: #f5f7fa;
    border-radius: 12rpx;
    padding: 24rpx 0rpx 24rpx 24rpx;
}
.ticket-img {
    background: url(https://pages.c-ctrip.com/hotels/wechat/img/status-ques.png) center center no-repeat;
    background-size: cover;
    width: 88rpx;
    flex: 0 0 auto;
    border-radius: 8rpx;
    overflow: hidden;
    margin-right: 20rpx;
}
.ticket-img .t-icon {
    display: inline-block;
    color: #ffffff;
    font-size: 20rpx;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    background: #ff7700;
    border-radius: 4rpx;
    line-height: 24rpx;
    width: 24rpx;
