/* 酒店榜单 */
.popular-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 35rpx;
}
.popular-item:not(:last-child) {
  margin-bottom: 38rpx;
}
.popular-item .text {
  font-size: 28rpx;
  color: #000;
  font-weight: 500;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.dt-color { color: #ddd; }
.mr8 { margin-right: 8rpx; }

