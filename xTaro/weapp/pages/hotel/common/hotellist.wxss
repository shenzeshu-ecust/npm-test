.mr-8 {
  margin-right: 8rpx;
}
.ml-8 {
  margin-left: 8rpx;
}
.plr4 {
  padding: 0 4rpx;
}
.color {
  color: #006FF6;
}
/* 星星钻钻；非集成使用 */
.diamond-level,
.star-level {
  height: 10px;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: middle;
  background-image: url(https://pages.c-ctrip.com/hotels/wechat/img/star-diamond.png);
  background-repeat: repeat-x;
  background-size: 10px 20px;
}
.circle-level {
  height: 13px;
  margin-right: 10rpx;
  display: inline-block;
  vertical-align: middle;
  background-image: url(https://pages.c-ctrip.com/wireless-app/imgs/T2Images/0623/ic_new_circle2@3x.png);
  background-repeat: repeat-x;
  background-size: 20px 13px;
  background-position: 10px 0;
}
.star-level {
  background-position: 0 0;
}
.diamond-level {
  background-position: 0 -12px;
}
.level-1 {
  width: 10px !important;
}
.level-2 {
  width: 20px !important;
}
.level-3 {
  width: 30px !important;
}
.level-4 {
  width: 40px !important;
}
.level-5 {
  width: 50px !important;
}
/* 星钻 */
.star-icon {
  height: 24rpx;
  vertical-align: middle;
  margin: 0 6rpx;
  width: auto;
}
/* 挂牌 */
.list-medal {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}
.list-medal-agency {
  /*v7.5: 挂牌标签不区分直采和代理，都用直采的标签*/
  /*width: 62rpx;*/
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}

/* 金钻/铂钻 */
.icon-d-star {
  width: 44rpx;
  height: 24rpx;
  margin-right: 8rpx;
  vertical-align: middle;
}

/* 精选 */
.wow-icon {
  width: 44rpx;
  height: 24rpx;
  margin-right: 8rpx;
  vertical-align: middle;
}

.hotel-item {
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  display: flex;
  background-color: #fff;
  position: relative;
  transform: translateZ(0);
  padding: 0 24rpx;
}

.hotel-item.no-border .hotel-info{
  border: unset;
}

.hotel-img-container {
  width: 204rpx;
  height: calc(100% - 24rpx);
  overflow: hidden;
  border-radius: 8rpx 8rpx 24rpx 8rpx;
  position: absolute;
  top: 12rpx;
  background: #f2f2f2
    url(https://pages.c-ctrip.com/hotels/wechat/img/default.png) no-repeat;
  background-size: cover;
}

.hotel-img-container .top-desc {
  background: rgba(0, 0, 0, 0.7);
  height: 32rpx;
  position: absolute;
  width: 100%;
  bottom: 0;
  color: #fff;
  text-align: center;
  font-size: 20rpx;
  line-height: 32rpx;
}
.hotel-img {
  width: 100%;
  height: 100%;
}
.full-booking-mark {
  position: absolute;
  right: 0;
  bottom: 24rpx;
  align-self: center;
  width: 92rpx;
  height: 102rpx;
  z-index: 1;
  background: url(https://pages.c-ctrip.com/hotels/wechat/img/newfullbooking.png)
    no-repeat;
  background-size: cover;
}

.hotel-info {
  padding: 24rpx 0;
  margin-left: 220rpx;
  min-width: 0;
  flex: 1;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #e5e5e5;
}

.hotel-info .head-line {
  display: flex;
  justify-content: space-between;
}
.hotel-name {
  color: #111;
  font-weight: 500;
  font-size: 32rpx;
  font-family: 'PingFangSC-Medium';
  word-break: break-all;
  line-height: 40rpx;
  margin-bottom: 12rpx;
}
.hotel-name .hotel-feature {
  display: inline-block;
  padding: 0 4rpx;
  border-radius: 4rpx;
  background: linear-gradient(270deg, #8f9fbf 0%, #6b83b3 100%);
  color: #fff;
  font-size: 18rpx;
  height: 24rpx;
  line-height: 24rpx;
  vertical-align: middle;
  white-space: nowrap;
}
.hotel-pyramid-ad {
  flex-shrink: 0;
  color: #ccc;
  opacity: 0.8;
}
.hotel-pyramid-ad text {
  margin-top: 3rpx;
  font-size: 18rpx;
  border: 2rpx solid #ccc;
  border-radius: 2rpx;
  padding: 0 7rpx;
  box-sizing: border-box;
  border-radius: 4rpx;
  height: 28rpx;
  line-height: 28rpx;
}
.hotel-tag {
  padding: 2rpx;
  color: #00aea8;
  border: 2rpx solid #b2e6e4;
  font-size: 20rpx;
  border-radius: 4rpx;
  vertical-align: middle;
  white-space: nowrap;
  display: inline-block;
  background-color: #fff;
}
.list-c {
  display: flex;
  justify-content: space-between;
  line-height: 32rpx;
  align-items: bottom;
  margin-bottom: 8rpx;
}
.list-d {
  padding-top: 5rpx;
  line-height: 1;
}
.rate-info {
  display: flex;
  flex: 1;
  align-items: center;
  font-size: 24rpx;
}
.rate-num {
  color: #006FF6;
  font-size: 24rpx;
  font-weight: 500;
  font-family: 'PingFangSC-Medium';
}
.rate-fize {
  display: block;
  width: 54rpx;
  height: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 32rpx;
  background-color: #006FF6;
  border-radius: 4rpx 4rpx 12rpx 4rpx;
}
.hotel-label {
  color: #006FF6;
  font-size: 24rpx;
  padding: 0 8rpx;
}
.hotel-comment {
  color: #111;
  font-size: 24rpx;
  line-height: 32rpx;
  margin-left: 8rpx;
  font-family: 'PingFangSC-Regular';
  letter-spacing: 0px;
}
.hotel-hourtime {
  font-size: 24rpx;
}
.list-cgray {
  color: #555;
  font-size: 24rpx;
  font-family: 'PingFangSC-Regular';
}
.lt-font-qa {
  color: #888888;
  vertical-align: middle;
  padding-right: 8rpx;
  font-size: 28rpx;
}

.browse {
  background-color: #f9faff;
}

/*砍价*/
.cut-tag {
  color: #ff4d1d;
  font-size: 20rpx;
  font-weight: 400;
}

/* 酒店标签 */
.hotel-img-container .pic-icon-tag {
  position: absolute;
  top: 0;
}
.hotel-img-container .tags-wrap-1,
.hotel-img-container .tags-wrap-2 {
  width: 100%;
  overflow: hidden;
  max-height: 34rpx;
}
.hotel-img-container .tags-wrap-1 {
  margin-bottom: 4rpx;
  max-width: 164rpx;
}
.hotel-img-container .tags-wrap-2 {
  bottom: 0;
  position: absolute;
}
.cell-category-icon {
  border-radius: 50%;
  margin-right: 6rpx;
  vertical-align: bottom;
  height: auto;
  width: auto;
}
.cell-category-icon.icon-image-1 {
  margin: 4rpx 0 4rpx 4rpx;
  height: 30rpx;
  vertical-align: top;
}
.cell-category-icon.icon-image-2 {
  width: 100%;
  margin: 0;
}
.cell-category-icon.icon-image-3 {
  display: block;
  margin-left: 4rpx;
}
.cell-category-icon.icon-image-1,
.cell-category-icon.icon-image-2 {
  border-radius: unset;
}
.hotel-name .new-tag {
  margin-bottom: 0;
  margin-left: 4rpx;
}
.tag-info {
  background: #ffeedf;
  border-radius: 4rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 32rpx;
  line-height: 32rpx;
}
.annual-tag {
  height: 28rpx;
  background: #fee4bf;
}
.tag-info .tag-icon {
  height: 24rpx;
  width: auto;
  margin-right: 4rpx;
  margin-left: 4rpx;
}
.annual-tag .tag-icon {
  margin-left: 0rpx !important;
  height: 20rpx;
  margin-right: 6rpx;
}
.icon-box {
  display: flex;
  align-items: center;
}
.annual-tag .icon-box {
  background: linear-gradient(
    -90deg,
    rgb(100, 91, 87) 100%,
    rgb(61, 39, 31) 0%
  );
  border-radius: 4rpx 0 12rpx 4rpx;
  height: 28rpx;
  margin-right: 6rpx;
}
.tag-item {
  color: #673114;
  font-size: 20rpx;
  height: 24rpx;
  line-height: 24rpx;
  margin-right: 10rpx;
}
.cell-category-tag {
  font-size: 20rpx;
  margin-right: 8rpx;
  height: 30rpx;
  line-height: 28rpx;
  position: relative;
  vertical-align: middle;
  display: inline-block;
  border-radius: 4rpx;
  color: #888888;
  box-sizing: border-box;
  margin-bottom: 5rpx;
  max-width: 100%;
  transform: rotateZ(360deg);
}

.cell-category-tag:not(.new-tag) {
  border: 1rpx solid #888888;
}
.tags-line .cell-category-tag {
  margin-bottom: 0;
}

.tags-line .cell-category-tag.new-tag {
  margin-bottom: 5rpx;
}
.new-tag .cell-category-icon {
  width: 20rpx;
  height: 20rpx;
}
.cell-category-tag.t1 {
  color: #b39336;
  border-color: #d8caa1;
}
.cell-category-tag.t2 {
  color: #ff7700;
  border-color: #ffd6b2;
}
.cell-category-tag.t3 {
  color: #ff7700;
  border-color: #ffd6b2;
}
/* .cell-category-tag.t3 {color: #F5190A; border-color: #FCBAB5;} */
.cell-category-tag.t4 {
  color: #00b87a;
  border-color: #b2e9d7;
}
.cell-category-tag.t5 {
  color: #006FF6;
  border-color: #b2dafc;
}

/* 高铁游 */
.chr-tag {
  font-size: 22rpx;
  color: #888888;
  display: inline-block;
}
/* sha认证 */
.icon-sha {
  height: 40rpx;
  width: 40rpx;
}
.icon-sha-plus {
  height: 40rpx;
  width: 72rpx;
}

/* 优享会 */
.icon-yxh {
  background: #5e6c89;
  font-size: 20rpx;
  font-weight: 500;
  color: #ffedc3;
  border-radius: 0rpx 0rpx 16rpx 0rpx;
  padding: 2rpx;
  width: 116rpx;
  height: 30rpx;
  left: 0;
  top: 0;
  text-align: center;
  margin-bottom: 8rpx;
}
.yxh-icon-new {
  background: linear-gradient(83deg, #f0cd9b 3%, #f2bc7c 97%);
  color: #53596d;
}
.yxh-tag {
  background: rgba(232, 222, 194, 0.4);
  border-radius: 4rpx;
  font-size: 18rpx;
  line-height: 28rpx;
  height: 28rpx;
  color: #b37e36;
  padding: 0 8rpx;
  display: inline-block;
  margin-right: 8rpx;
}

.price-calc-box {
  display: flex;
  justify-content: flex-end;
}
.flex-box {
  display: flex;
}
.left-shrink .tags-wrap {
  margin-bottom: 16rpx;
}
.layout-reset .left-shrink .new-tag {
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  max-width: 490rpx;
  margin-right: 24rpx;
  line-height: 1;
  line-height: 24rpx;
}
.hotel-info .tags-wrap {
  margin-bottom: 12rpx;
}
.hotel-info .tags-line .cell-category-icon {
  margin-right: 4rpx;
  margin-bottom: 4rpx;
  vertical-align: middle;
}
.list-cgray,
.tags-wrap,
.cut-tag {
  line-height: 1;
}
.lnum {
  line-height: 0.9;
}

.lnum .yen {
  line-height: 24rpx;
}
.lnumb {
  line-height: 0.9;
  margin-bottom: 16rpx;
}
.lnumb .c2 {
  color: #555555;
  font-size: 20rpx;
}
.mb16 {
  margin-bottom: 16rpx;
}
.mt16 {
  margin-top: 16rpx;
}
.addr {
  line-height: 32rpx;
  margin-bottom: 12rpx;
}
.invalid-reason {
    text-align: right;
    font-size: 24rpx;
    color: #aaa;
}
