/* 欢迎度排序 */
.sort-bar {
  position: absolute;
  z-index: 1100;
  background-color: #fff;
  width: 100%;
  padding: 0 24rpx;
  box-sizing: border-box;
}
.sort-bar .item {
  min-height: 96rpx;
  border-bottom: 1rpx solid #efefef;
  font-size: 26rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  line-height: 34rpx;
  box-sizing: border-box;
}
.sort-bar .item .con-text {
  line-height: 34rpx;
  min-height: 34rpx;
}
.sort-current {
  color: #006FF6;
  position: relative;
}
.sort-current:after {
  animation: swing 0.5s linear both;
  position: absolute;
  content: '';
  right: 24rpx;
  top: 50%;
  margin-top: -14rpx;
  width: 32rpx;
  height: 16rpx;
  border-left: 2rpx solid #006FF6;
  border-bottom: 2rpx solid #006FF6;
  transform: rotate(-45deg);
}

