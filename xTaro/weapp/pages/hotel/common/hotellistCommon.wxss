.animated {
  animation-duration: .3s;
  animation-fill-mode: both;
  animation-timing-function: ease-out;
}
@keyframes fadeInDown {
  0% {
    transform:scale(1,0);
  }
  30% {
    transform: scale(1,0.5);
  }
  100% {
    transform:scale(1,1);
  }
}
.fadeInDown {
  animation-name: fadeInDown;
  transform-origin: 50% 0;
}
