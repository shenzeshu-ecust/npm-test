.highlight {color:#006FF6;}
.sres-item {display:flex;align-items:center;padding:29rpx 20rpx;border-bottom:1rpx solid #e5e5e5;}
.sres-item .res-icon {font-size: 44rpx; margin-right: 16rpx;}
.sres-item .word {flex:1;font-size:26rpx;color:#111111; overflow:hidden;}
.sres-item .word-text {font-size: 32rpx; font-weight: 600;}
.sres-item .res-text-ellipsis {overflow:hidden; white-space: nowrap; text-overflow: ellipsis;}
.sres-item .res-price-box {color: #006FF6; font-size: 24rpx; margin-left: 50rpx; text-align: right;}
.sres-item .res-price-box .res-price-num {font-size: 32rpx;}
.sres-item .res-price-box .res-price-qi {color: #111111;}
.sres-item .res-price-box .res-price-tax {font-size: 22rpx;}
.sres-item .catg {margin-left:50rpx;font-size:24rpx;text-align:center;color:#888888;}
/* 搜索结果icon */
.font-reshotel-color {background-image: linear-gradient(135deg, #FFAB69, #FF7508); -webkit-background-clip: text; -webkit-text-fill-color: transparent;}
.font-resothers-color {background-image: linear-gradient(135deg, #00A7FA, #0076F5); -webkit-background-clip: text; -webkit-text-fill-color: transparent;}
