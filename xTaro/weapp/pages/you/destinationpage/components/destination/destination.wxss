}
.entry-icon-img{
	margin-bottom: 2rpx;
}
.entry-name{
	font-size: 28rpx;
	color: #222222;
	line-height: 28rpx;
}

/* trainticket 火车票入口样式 */
.trainticket-left{
	display: flex;
	flex-direction: row;
}
.trainticket-img{
	width: 72rpx;
	height: 72rpx;
	/*background: url('https://pages.c-ctrip.com/you/livestream/you-destination-hightrain.png');
	background-size: 100% 100%;*/
