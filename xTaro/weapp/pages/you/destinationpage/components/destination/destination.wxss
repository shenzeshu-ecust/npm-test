	display:flex;
	justify-content:space-between;
	flex-direction:column;
	align-items:center;
}
.square-swiper-item .entry-navigator-replace{
	height:140rpx;
	width: auto;
	display:flex;
	justify-content:space-between;
	flex-direction:column;
