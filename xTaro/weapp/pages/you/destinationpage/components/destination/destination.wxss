	flex-direction: row;
}
.square-swiper-item{
	width: 20%;
	text-align: center;
}
.square-swiper-item image{
	width: 100rpx;
	height: 100rpx;

}
.square-swiper-item view{
	font-size: 28rpx;
	color: #222222;
	line-height: 28rpx;
}
.square-swiper-item navigator{
	height:140rpx;
