/* pages/destination/destination.wxss */
@import '../../common/desTemplate/waterfall/waterfall.wxss';
.iphonex-bottom{

}
/*commonstyle样式*/
.oneLineOverflow {
    overflow: hidden;
    text-overflow: ellipsis;
    display:-webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.oneLineOverflowNew{
	display: -webkit-box;
	overflow: hidden;
	white-space: normal;
	text-overflow: ellipsis;
	word-wrap: break-word;
	-webkit-line-clamp:1;
	-webkit-box-orient: vertical;
}
.destination-section{
	background: #fff;
	/* margin: 0 0 16rpx 0; */
	width: 750rpx;
}
.destination-title{
	display: flex;
	flex-direction: row;
	justify-content: space-between;
}
/*必玩Top榜，预定酒店，特色主题*/
.destination-title-padding1{
	padding: 32rpx;
}
/*高铁动车*/
.destination-title-padding2{
	padding: 28rpx 32rpx;
}
/*快速了解当地*/
.destination-title-padding3{
	padding: 42rpx 32rpx;
}
.destination-title .title-name{
	font-size: 36rpx;
	color: #222222;
	line-height: 36rpx;
	font-family: PingFangSC-Medium;
}
.destination-title .title-sub{
	font-size: 26rpx;
	color: #666666;
	line-height: 36rpx;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
}
.destination-title .title-sub text{
	width: 30rpx;
	height: 30rpx;
	background: url('https://pages.c-ctrip.com/you/livestream/you-destination-rightarrow.png');
	background-size: 100% 100%;
	display: block;
}
.destination-title .title-sub text.title-sub-more{
	position: relative;
	top: -2rpx;
}


/*必玩TOP榜和特色主题公用scroll-view样式*/
.destination-scroll-wrap{
	padding:0 0 32rpx 0rpx ;
}
.destination-scroll-view{
	white-space: nowrap;
	display: flex;
}
.destination-scroll-item{
	width: 296rpx;
	margin-right: 20rpx;
	display:inline-block;
	position: relative;
}
.destination-scroll-item:last-child{
	margin-right: 32rpx;
}
.destination-scroll-item:first-child{
	margin-left: 32rpx;
}
.destination-scroll-item image{
	width: 296rpx;
	height: 197rpx;
	border-radius: 10rpx;
}



/* banner banner头模块样式 */
.destination-banner{
	width: 100%;
	height: 380rpx;
	padding-bottom: 20rpx;
	position: relative;
}
.destination-banner:after{
	content: "";
	width: 100%;
	height: 100%;
	display: block;
	position: absolute;
	top: 0;
	left: 0;
	background: rgba(0,0,0,.3);
}
.banner-con{
	box-sizing: border-box;
	width: 750rpx;
	height: 380rpx;
	padding: 40rpx 24rpx;
	position: absolute;
	top: 0;
	left: 0;
	display: flex;
	flex-direction:column;
	align-items: flex-start;
	justify-content:flex-end;
	color: #fff;
	z-index: 9;
}
.banner-weather{
	position: absolute;
	top: 27rpx;
	right: 24rpx;
	font-size: 24rpx;
	line-height: 26rpx;
	display: flex;
	flex-direction:row;
	align-items: center;
	justify-content:center;
}
.banner-weather text{
	width: 44rpx;
	height: 38rpx;
	background: rgba(255,85,18,.4);
	display: inline-block;
	margin-right: 8rpx;
}
.banner-cityname-china{
	display: flex;
	flex-direction:row;
	align-items: center;
	justify-content:left;
	margin-bottom: 8rpx;
}
.banner-cityname-china-text{
    font-size: 64rpx;
	line-height: 68rpx;
	text-shadow: 0 2rpx 4rpx rgba(0,0,0,0.20);
	max-width :90%
}
.banner-cityname-china .banner-tag{
	width: 32rpx;
	height: 32rpx;
	background: url('https://pages.c-ctrip.com/you/livestream/you-destination-change.png');
	background-size: 100% 100%;
	display: inline-block;
	margin-left: 16rpx;
}
.banner-cityname-english{
	font-size: 28rpx;
	line-height: 52rpx;
	text-shadow: 0 2rpx 4rpx rgba(0,0,0,0.20);
	text-transform: uppercase;
}
.banner-mask {
	width: 750rpx;
	height: 184rpx;
	background-image: linear-gradient(180deg,#22222200,#22222260);
	position: absolute;
	bottom: 0;
	left: 0;
	z-index: 2;
}
.banner-search {
	height: 64rpx;
	width: 702rpx;
	border-radius: 32rpx;
	background-color: #fff;
	position: absolute;
	left: 24rpx;
	top: 24rpx;
	z-index: 99;
	display: flex;
	flex-direction: row;
	justify-content: flex-start;
	align-items: center;
}
.search-icon {
	width: 24rpx;
	height: 24rpx;
	margin-left: 24rpx;
}
.search-text {
	color: #999;
	line-height: 36rpx;
	font-size: 28rpx;
	margin-left: 16rpx;
}

/* square 九宫格模块样式 */
.destination-square{
	width: 100%;
	position: relative;
	/* margin-top: -32rpx; */
	/* border-radius: 40rpx 40rpx 0 0; */
	/* padding: 35rpx 0rpx 0rpx 0rpx; */
	z-index: 99;
	background-color: #fff;
}
.destination-square swiper{
	height: 190rpx;
}
.destination-square swiper swiper-item{
