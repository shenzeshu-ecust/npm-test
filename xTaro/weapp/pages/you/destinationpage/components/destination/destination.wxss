/* waterfall 笔记瀑布流模块样式 */
.theme-scroll .destination-scroll-item:before{
	content: '';
	display: block;
	width: 296rpx;
	height: 197rpx;
	border-radius: 10rpx;
	position: absolute;
	top: 0;
	left: 0;
	background: rgba(0,0,0,.3);
}
.theme-wrap{
	width: 296rpx;
	height: 197rpx;
	border-radius: 10rpx;
	position: absolute;
	top: 0;
	left: 0;
	display: flex;
	flex-direction: row;
	align-items: center;
	justify-content: center;
	color: #fff;
	z-index: 1;
}
.theme-wrap view{
	position: relative;
	font-size: 32rpx;
	line-height: 32rpx;
	text-shadow: 0 2rpx 8rpx rgba(0,0,0,0.20);
	font-family: PingFangSC-Medium;
}
.theme-wrap view:after{
	content: '';
	display: block;
	width: 48rpx;
	height: 4rpx;
	border-radius: 2rpx;
	position: absolute;
	bottom: -12rpx;
	left: 50%;
	margin-left: -24rpx;
	background: #fff;
}

