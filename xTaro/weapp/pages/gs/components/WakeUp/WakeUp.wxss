.wakeup-go-back{
  position: fixed;
  bottom: 168rpx;
  z-index: 200;
  transition:all 0.5s;
}
.wakeup-go-back-pull{ 
  right: 0!important;
}
.wakeup-go-back-pull .wakeup-go-back-icon{
  background: rgba(0, 0, 0, 0.8);
}
.wakeup-go-back-pull .wakeup-go-back-icon .wakeup-go-back-icon-img{
  transform: rotate(180deg);
}
.wakeup-go-back-pull .wakeup-go-back-btns{ 
  background: rgba(0, 0, 0, 0.8);
}
.wakeup-go-back-icon{
  width: 82rpx;
  height: 70rpx;
  border-radius: 36rpx 0 0 36rpx;
  background: rgba(0, 0, 0, 0.4);
  position: absolute;
  left: -81rpx;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index:-1; 
  transition:all 1s;
}
.wakeup-go-back-icon-img{
  width: 34rpx;
  height: 34rpx;
  transition:all 0.6s;
}
.wakeup-go-back-btns{
  height: 138rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-bottom-left-radius: 12rpx;
  overflow: hidden;
  background: rgba(0, 0, 0, 0.4); 
  padding: 0 22rpx;
  transition: all 1s;
}
.wakeup-go-back-btn{
  width: 100rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: transparent;
}
.wakeup-go-back-btn:last-child{
  margin-left: 12rpx;
}
.wakeup-go-back-btn-img{
  width: 42rpx;
  height: 42rpx;
  margin-bottom: 8rpx;
}
.wakeup-go-back-btn-text{
  font-size: 20rpx;
  color: #FFFFFF;
  line-height: 28rpx;
  width:100rpx;
}